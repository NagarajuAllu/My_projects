<?xml version="1.0" encoding="UTF-8" ?>
<participant name="workflow.biTeamParticipant">
  <distributionType>Shared</distributionType>
  <duration type="dur">
    <length>P0Y0M1DT0H0M0S</length>
  </duration>
  <interface>iface_workflow.biTeamIF</interface>
  <label>BI Team Participant</label>
  <metaVersion>25</metaVersion>
  <operationConditionsList>
    <operationConditions type="opco">
      <bulkComplete>true</bulkComplete>
      <operation>iface_workflow.biTeamIF/oper_validBIRequest</operation>
      <tasksList>
        <tasks>validateRequest</tasks>
      </tasksList>
      <methodList>
        <method name="cwOnParticipantOpAct" type="action">
          <system>true</system>
          <script><![CDATA[
            workflow.computeNextUser(this, "biAssociationProcess", "prepareToCompleteBIMgmt");
            workflow.completeParticipantActivity(this.processOrder.id, this.workItemDocument, "Valid BI Request");

            if(Global.getWorkListFinder().selected.length == 1) {
                Global.showUserMessage("AI0006");
            }
          ]]></script>
        </method>
        <method name="cwOnPartOpConf" type="action">
          <system>true</system>
          <script><![CDATA[return "UU0167";]]></script>
        </method>
      </methodList>
    </operationConditions>
    <operationConditions type="opco">
      <bulkComplete>true</bulkComplete>
      <operation>iface_workflow.biTeamIF/oper_rejectBIRequest</operation>
      <tasksList>
        <tasks>validateRequest</tasks>
      </tasksList>
      <methodList>
        <method name="cwOnParticipantOpAct" type="action">
          <system>true</system>
          <script><![CDATA[
            workflow.completeParticipantActivity(this.processOrder.id, this.workItemDocument, "Rejected BI Request");

            if(Global.getWorkListFinder().selected.length == 1) {
                Global.showUserMessage("AI0006");
            }
          ]]></script>
        </method>
        <method name="cwOnPartOpConf" type="action">
          <system>true</system>
          <script><![CDATA[return "UU0167";]]></script>
        </method>
      </methodList>
    </operationConditions>
    <operationConditions type="opco">
      <operation>iface_workflow.biTeamIF/oper_biMgmtCompleted</operation>
      <tasksList>
        <tasks>prepareToCompleteBIMgmt</tasks>
      </tasksList>
      <methodList>
        <method name="cwOnParticipantOpAct" type="action">
          <system>true</system>
          <script><![CDATA[
            workflow.completeParticipantActivity(this.processOrder.id, this.workItemDocument, "BI Management Completed");

            if(Global.getWorkListFinder().selected.length == 1) {
                Global.showUserMessage("AI0006");
            }
          ]]></script>
        </method>
        <method name="cwOnPartOpConf" type="action">
          <system>true</system>
          <script><![CDATA[return "UU0167";]]></script>
        </method>
      </methodList>
    </operationConditions>
    <operationConditions type="opco">
      <bulkComplete>true</bulkComplete>
      <operation>iface_workflow.biTeamIF/oper_closeBITicket</operation>
      <tasksList>
        <tasks>prepareToCloseBITicket</tasks>
      </tasksList>
      <methodList>
        <method name="cwOnParticipantOpAct" type="action">
          <system>true</system>
          <script><![CDATA[
            workflow.completeParticipantActivity(this.processOrder.id, this.workItemDocument, "BI Completed");

            if(Global.getWorkListFinder().selected.length == 1) {
                Global.showUserMessage(Global.translateText("AI0007", null, ["BI"]));
            }
          ]]></script>
        </method>
        <method name="cwOnPartOpConf" type="action">
          <system>true</system>
          <script><![CDATA[return "UU0167";]]></script>
        </method>
      </methodList>
    </operationConditions>
  </operationConditionsList>
  <privilege>STC_BIPriv</privilege>
  <removeTasks>true</removeTasks>
  <UserInterface name="UserInterface" type="ui">
    <extends>ui_com.conceptwave.system.WLTaskUserInterface</extends>
    <vformList>
      <vform name="Forms" type="frmui">
        <extends>ui_com.conceptwave.system.WLTaskUserInterface/frmui_Forms</extends>
      </vform>
    </vformList>
  </UserInterface>
</participant>