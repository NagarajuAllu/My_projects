  <label>Integration Team Participant</label>
  <metaVersion>25</metaVersion>
  <operationConditionsList>
    <operationConditions type="opco">
      <bulkComplete>true</bulkComplete>
      <operation>iface_workflow.integrationTeamIF/oper_reject</operation>
      <tasksList>
        <tasks>analyzeRejection</tasks>
      </tasksList>
      <methodList>
        <method name="cwOnParticipantOpAct" type="action">
          <system>true</system>
          <script><![CDATA[
            workflow.computeNextUser(this, "installOrderProcess", "rejectAnalys");
            workflow.completeParticipantActivity(this.processOrder.id, this.workItemDocument, "Reject");

            if(Global.getWorkListFinder().selected.length == 1) {
                Global.showUserMessage("AI0006");
            }
          ]]></script>
        </method>
        <method name="cwOnPartOpConf" type="action">
          <system>true</system>
          <script><![CDATA[return "UU0167";]]></script>
        </method>
      </methodList>
    </operationConditions>
