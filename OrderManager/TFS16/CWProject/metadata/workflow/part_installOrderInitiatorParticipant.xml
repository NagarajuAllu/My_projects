    <operationConditions type="opco">
      <operation>iface_workflow.orderInitiatorTeamIF/oper_answerSelected</operation>
      <tasksList>
        <tasks>proposeNextQuestion</tasks>
      </tasksList>
      <methodList>
        <method name="cwOnPartOpConf" type="action">
          <system>true</system>
          <script><![CDATA[
            if(order.rejectionProcData.selectedAnswerId == null) {
                Global.throwException("UU0116");
            }

            var msg = null;

            var nextQuestionId = tfs.searchNextQuestionForRejectionAnswer(order.rejectionProcData.selectedAnswerId);
            if(nextQuestionId == null) {
                msg = "AW0006"
            }

            return msg;
          ]]></script>
        </method>
      </methodList>
    </operationConditions>
    <operationConditions type="opco">
      <operation>iface_workflow.orderInitiatorTeamIF/oper_terminateProcess</operation>
      <tasksList>
        <tasks>proposeNextQuestion</tasks>
      </tasksList>
      <methodList>
        <method name="cwOnParticipantOpAct" type="action">
          <system>true</system>
          <script><![CDATA[
            var finder = new Finder("tfs.rejectionAnswerFinder");
            finder.searchDocument.cwDocID = this.processOrder.rejectionProcData.selectedAnswerId;
            var answerResultList = finder.search();

            if(answerResultList != null && answerResultList.length > 0) {
                if(answerResultList[0].resultAnalysisToReturn == null) {
                    Global.throwException("AE0014");
                }
            }
          ]]></script>
        </method>
        <method name="cwOnPartOpConf" type="action">
          <system>true</system>
          <script><![CDATA[
            if(order.rejectionProcData.selectedAnswerId == null) {
                Global.throwException("UU0116");
            }

            return "UU0167";
          ]]></script>
        </method>
      </methodList>
