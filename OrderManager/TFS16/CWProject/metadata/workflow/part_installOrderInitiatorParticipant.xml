      <methodList>
        <method name="cwOnParticipantOpAct" type="action">
          <system>true</system>
          <script><![CDATA[
            //workflow.computeNextUser(this, "installOrderProcess", "rejectCWOReport");
            workflow.completeParticipantActivity(this.processOrder.id, this.workItemDocument, "Reject CWO Report");

            if(Global.getWorkListFinder().selected.length == 1) {
