            }
          ]]></script>
        </method>
        <method name="cwOnPartOpCond" type="action">
          <system>true</system>
          <script><![CDATA[
            var visible = true;

            var processInstance = new Process(this.workItemDocument.SenderId);
            if(processInstance != null) {
                var processType = processInstance.metadataTypeName;
                if(processType.indexOf("disconnectOrderProcess") >= 0 || processType.indexOf("rehomingOrder") >= 0) {
                    visible = false;
                }
            }

            return visible;
          ]]></script>
        </method>
