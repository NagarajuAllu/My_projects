<?xml version="1.0" encoding="UTF-8" ?>
<findSql name="workflow.rejectionProcessActivityForGUIFinder">
  <dbSchema>ORDER</dbSchema>
  <extends>find_com.conceptwave.system.Finder</extends>
  <input>doc_cwt_common.docID</input>
  <label>Rejection Process Activity For GUI Finder</label>
  <maxRowsLimit>9999</maxRowsLimit>
  <metaVersion>21</metaVersion>
  <output>doc_workflow.rejectionProcessActivityForGUI</output>
  <type>SQL</type>
  <volatility>1</volatility>
  <methodList>
    <method name="cwOnFinderSQLSel" type="action">
      <system>true</system>
      <script><![CDATA[
        select pa.activity_id, pa.process_id, q.text_question, a.text_answer, pa.creation_date, pa.completion_date, pa.remarks
          from stc_rejection_process_activity pa, stc_reject_answer a, stc_reject_question q
         where a.answer_id = pa.answer_id
           and q.question_id = pa.question_id
        {[0] and pa.process_id = ?}
        order by creation_date
      ]]></script>
    </method>
    <method name="editablePerm" type="permMethod">
      <script>return false;</script>
    </method>
    <method name="addPerm" type="permMethod">
      <script>return false;</script>
    </method>
    <method name="deletePerm" type="permMethod">
      <script>return false;</script>
    </method>
  </methodList>
  <UserInterface name="UserInterface" type="ui">
    <extends>ui_com.conceptwave.system.FinderUserInterface</extends>
    <variableList>
      <variable name="result" type="uivar">
        <flags enum-type="metadataFlag">ARRAY</flags>
        <valueType>doc_workflow.rejectionProcessActivityForGUI</valueType>
      </variable>
      <variable name="search" type="uivar">
        <valueType>doc_cwt_common.docID/ui_UserInterface</valueType>
      </variable>
      <variable name="detail" type="uivar">
        <flags enum-type="metadataFlag">VALIDATE</flags>
        <valueType>doc_workflow.rejectionProcessActivityForGUI/ui_UserInterface</valueType>
      </variable>
    </variableList>
    <vformList>
      <vform name="Forms" type="frmui">
        <extends>ui_com.conceptwave.system.FinderUserInterface/frmui_Forms</extends>
        <overlayList>
          <overlay name="Result" type="exov">
            <base>ui_com.conceptwave.system.FinderUserInterface/frmui_Forms/vform_Result</base>
            <extends>ui_com.conceptwave.system.FinderUserInterface/frmui_Forms/vform_Result</extends>
            <label>Result</label>
            <overrideList>
              <override name="table" type="elvlt">
                <height>100%</height>
                <width>100%</width>
                <elementList>
                  <element name="rejectionProcessActivityForGUIFinderResultTable" type="eltabl">
                    <onSelectChange type="varPath">
                      <variablePathList>
                        <variablePath>ui_com.conceptwave.system.MasterDetailUserInterface/action_OnSelChanged</variablePath>
                      </variablePathList>
                    </onSelectChange>
                    <variable type="varPath">
                      <variablePathList>
                        <variablePath>findSql_workflow.rejectionProcessActivityForGUIFinder/ui_UserInterface/uivar_result</variablePath>
                      </variablePathList>
                    </variable>
                    <width>100%</width>
                    <elementList>
                      <element name="textQuestion" type="elfld">
                        <label>Question</label>
                        <variable type="varPath">
                          <variablePathList>
                            <variablePath>findSql_workflow.rejectionProcessActivityForGUIFinder/ui_UserInterface/uivar_result</variablePath>
                            <variablePath>doc_workflow.rejectionProcessActivityForGUI/leaf_textQuestion</variablePath>
                          </variablePathList>
                        </variable>
                        <width>300px</width>
                      </element>
                      <element name="textAnswer" type="elfld">
                        <label>Answer</label>
                        <variable type="varPath">
                          <variablePathList>
                            <variablePath>findSql_workflow.rejectionProcessActivityForGUIFinder/ui_UserInterface/uivar_result</variablePath>
                            <variablePath>doc_workflow.rejectionProcessActivityForGUI/leaf_textAnswer</variablePath>
                          </variablePathList>
                        </variable>
                        <width>300px</width>
                      </element>
                      <element name="creationDate" type="eldatetime">
                        <label>Creation Date</label>
                        <variable type="varPath">
                          <variablePathList>
                            <variablePath>findSql_workflow.rejectionProcessActivityForGUIFinder/ui_UserInterface/uivar_result</variablePath>
                            <variablePath>doc_workflow.rejectionProcessActivityForGUI/leaf_creationDate</variablePath>
                          </variablePathList>
                        </variable>
                        <width>150px</width>
                      </element>
                      <element name="completionDate" type="eldate">
                        <label>Completion Date</label>
                        <variable type="varPath">
                          <variablePathList>
                            <variablePath>findSql_workflow.rejectionProcessActivityForGUIFinder/ui_UserInterface/uivar_result</variablePath>
                            <variablePath>doc_workflow.rejectionProcessActivityForGUI/leaf_completionDate</variablePath>
                          </variablePathList>
                        </variable>
                        <width>150px</width>
                      </element>
                      <element name="remarks" type="eltxtArea">
                        <label>Remarks</label>
                        <variable type="varPath">
                          <variablePathList>
                            <variablePath>findSql_workflow.rejectionProcessActivityForGUIFinder/ui_UserInterface/uivar_result</variablePath>
                            <variablePath>doc_workflow.rejectionProcessActivityForGUI/leaf_remarks</variablePath>
                          </variablePathList>
                        </variable>
                        <width>500px</width>
                      </element>
                    </elementList>
                  </element>
                </elementList>
              </override>
            </overrideList>
          </overlay>
          <overlay name="Default" type="exov">
            <base>ui_com.conceptwave.system.FinderUserInterface/frmui_Forms/exov_Default</base>
            <extends>ui_com.conceptwave.system.FinderUserInterface/frmui_Forms/exov_Default</extends>
            <label>Default</label>
            <overrideList>
              <override name="detailSection" type="elsect">
                <formReference type="varPath">
                  <variablePathList>
                    <variablePath>ui_com.conceptwave.system.MasterDetailUserInterface/uivar_detailForm</variablePath>
                  </variablePathList>
                </formReference>
                <variable type="varPath">
                  <variablePathList>
                    <variablePath>findSql_workflow.rejectionProcessActivityForGUIFinder/ui_UserInterface/uivar_detail</variablePath>
                  </variablePathList>
                </variable>
                <visible type="varPath">
                  <variablePathList>
                    <variablePath>ui_com.conceptwave.system.MasterDetailUserInterface/permMethod_detailVisible</variablePath>
                  </variablePathList>
                </visible>
              </override>
              <override name="searchFormSection" type="elsect">
                <formReference type="varPath">
                  <variablePathList>
                    <variablePath>ui_com.conceptwave.system.BaseFinderUserInterface/uivar_searchForm</variablePath>
                  </variablePathList>
                </formReference>
                <variable type="varPath">
                  <variablePathList>
                    <variablePath>findSql_workflow.rejectionProcessActivityForGUIFinder/ui_UserInterface/uivar_search</variablePath>
                  </variablePathList>
                </variable>
                <visible type="varPath">
                  <variablePathList>
                    <variablePath>ui_com.conceptwave.system.FinderUserInterface/permMethod_showSearchForm</variablePath>
                  </variablePathList>
                </visible>
              </override>
            </overrideList>
          </overlay>
        </overlayList>
      </vform>
    </vformList>
    <methodList>
      <method name="canDelete" type="permMethod">
        <script>return false;</script>
      </method>
    </methodList>
  </UserInterface>
  <viewList>
    <view name="rejectionProcessActivityForGUIFinderView" type="findVView">
      <label>Rejection Process Activities</label>
      <outputOverlay>findSql_workflow.rejectionProcessActivityForGUIFinder/ui_UserInterface/frmui_Forms/exov_Result</outputOverlay>
      <searchImmediately>true</searchImmediately>
      <sortListList>
        <sortList type="stitem">
          <descending>true</descending>
          <path>doc_workflow.rejectionProcessActivityForGUI/leaf_procActivityId</path>
        </sortList>
      </sortListList>
    </view>
  </viewList>
</findSql>