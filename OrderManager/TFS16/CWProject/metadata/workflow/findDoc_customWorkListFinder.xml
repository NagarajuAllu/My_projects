<?xml version="1.0" encoding="UTF-8" ?>
<findDoc name="workflow.customWorkListFinder">
  <extends>find_com.conceptwave.system.Finder</extends>
  <input>doc_workflow.searchWLDoc</input>
  <label>STC TFS WorkList Finder</label>
  <maxRowsLimit>500</maxRowsLimit>
  <metaVersion>21</metaVersion>
  <output>doc_workflow.customWL</output>
  <overrides>findDoc_cwf_pm.BaseWorklistFinder</overrides>
  <rtConfigurable>true</rtConfigurable>
  <shareViews>true</shareViews>
  <sortList>
    <sort type="stitem">
      <path>doc_cwl_worklist.worklist/leaf_Flags</path>
    </sort>
    <sort type="stitem">
      <path>doc_cwl_worklist.worklist/leaf_Priority</path>
    </sort>
    <sort type="stitem">
      <path>doc_cwl_worklist.worklist/leaf_DueDate</path>
    </sort>
    <sort type="stitem">
      <path>doc_cwl_worklist.worklist/leaf_SenderId</path>
    </sort>
  </sortList>
  <type>document</type>
  <volatility>1</volatility>
  <searchList>
    <search type="sitem">
      <document>doc_workflow.searchWLDoc</document>
      <mappedDocument>doc_workflow.customWL</mappedDocument>
      <mappedPath>doc_cwl_worklist.worklist/leaf_Flags</mappedPath>
      <operation>equal</operation>
      <path>doc_cwf_pm.WorklistSearch/leaf_Flags</path>
    </search>
    <search type="sitem">
      <document>doc_workflow.searchWLDoc</document>
      <mappedDocument>doc_workflow.customWL</mappedDocument>
      <mappedPath>doc_workflow.customWL/leaf_objectCreationDate</mappedPath>
      <operation>greater equal</operation>
      <path>doc_workflow.searchWLDoc/leaf_creationDateGE</path>
    </search>
    <search type="sitem">
      <document>doc_workflow.searchWLDoc</document>
      <mappedDocument>doc_workflow.customWL</mappedDocument>
      <mappedPath>doc_workflow.customWL/leaf_objectCreationDate</mappedPath>
      <operation>less equal</operation>
      <path>doc_workflow.searchWLDoc/leaf_creationDateLE</path>
    </search>
    <search type="sitem">
      <document>doc_workflow.searchWLDoc</document>
      <mappedDocument>doc_workflow.customWL</mappedDocument>
      <mappedPath>doc_workflow.customWL/leaf_customerName</mappedPath>
      <operation>equal</operation>
      <path>doc_workflow.searchWLDoc/leaf_customerName</path>
    </search>
    <search type="sitem">
      <document>doc_workflow.searchWLDoc</document>
      <mappedDocument>doc_workflow.customWL</mappedDocument>
      <mappedPath>doc_cwl_worklist.worklist/leaf_disable</mappedPath>
      <operation>equal</operation>
      <path>doc_cwf_pm.WorklistSearch/leaf_disable</path>
    </search>
    <search type="sitem">
      <document>doc_workflow.searchWLDoc</document>
      <mappedDocument>doc_workflow.customWL</mappedDocument>
      <mappedPath>doc_workflow.customWL/leaf_objectName</mappedPath>
      <operation>like</operation>
      <path>doc_workflow.searchWLDoc/leaf_objectName</path>
    </search>
    <search type="sitem">
      <document>doc_workflow.searchWLDoc</document>
      <mappedDocument>doc_workflow.customWL</mappedDocument>
      <mappedPath>doc_workflow.customWL/leaf_objectType</mappedPath>
      <operation>equal</operation>
      <path>doc_workflow.searchWLDoc/leaf_objectType</path>
    </search>
    <search type="sitem">
      <document>doc_workflow.searchWLDoc</document>
      <mappedDocument>doc_workflow.customWL</mappedDocument>
      <mappedPath>doc_workflow.customWL/leaf_objectPlannedRFSDate</mappedPath>
      <operation>greater equal</operation>
      <path>doc_workflow.searchWLDoc/leaf_plannedRFSDateGE</path>
    </search>
    <search type="sitem">
      <document>doc_workflow.searchWLDoc</document>
      <mappedDocument>doc_workflow.customWL</mappedDocument>
      <mappedPath>doc_workflow.customWL/leaf_objectPlannedRFSDate</mappedPath>
      <operation>less equal</operation>
      <path>doc_workflow.searchWLDoc/leaf_plannedRFSDateLE</path>
    </search>
    <search type="sitem">
      <document>doc_workflow.searchWLDoc</document>
      <mappedDocument>doc_workflow.customWL</mappedDocument>
      <mappedPath>doc_cwl_worklist.worklist/leaf_userId</mappedPath>
      <operation>equal</operation>
      <path>doc_cwf_pm.WorklistSearch/leaf_userId</path>
    </search>
  </searchList>
  <UserInterface name="UserInterface" type="ui">
    <extends>findDoc_cwf_pm.BaseWorklistFinder/ui_UserInterface</extends>
    <overrides>findDoc_cwf_pm.BaseWorklistFinder/ui_UserInterface</overrides>
    <timerDuration>1</timerDuration>
    <variableList>
      <variable name="result" type="uivar">
        <flags enum-type="metadataFlag">ARRAY</flags>
        <valueType>doc_workflow.customWL</valueType>
      </variable>
      <variable name="search" type="uivar">
        <valueType>doc_workflow.searchWLDoc/ui_UserInterface</valueType>
      </variable>
      <variable name="detail" type="uivar">
        <flags enum-type="metadataFlag">VALIDATE</flags>
        <valueType>doc_workflow.customWL/ui_UserInterface</valueType>
      </variable>
    </variableList>
    <vformList>
      <vform name="Forms" type="frmui">
        <extends>findDoc_cwf_pm.BaseWorklistFinder/ui_UserInterface/frmui_Forms</extends>
        <overlayList>
          <overlay name="Result" type="exov">
            <base>ui_com.conceptwave.system.SplitFinderUserInterface/frmui_Forms/vform_Result</base>
            <extends>ui_com.conceptwave.system.SplitFinderUserInterface/frmui_Forms/vform_Result</extends>
            <label>Result</label>
            <overrideList>
              <override name="table" type="elvlt">
                <height>100%</height>
                <width>100%</width>
                <elementList>
                  <element name="customWorkListFinderResultTable" type="eltabl">
                    <onSelectChange type="varPath">
                      <variablePathList>
                        <variablePath>findDoc_cwf_pm.BaseWorklistFinder/ui_UserInterface/action_OnSelChanged</variablePath>
                      </variablePathList>
                    </onSelectChange>
                    <variable type="varPath">
                      <variablePathList>
                        <variablePath>findDoc_workflow.customWorkListFinder/ui_UserInterface/uivar_result</variablePath>
                      </variablePathList>
                    </variable>
                    <elementList>
                      <element name="imageCalculate" type="elimg">
                        <disabled type="varPath">
                          <variablePathList>
                            <variablePath>dtype_com.conceptwave.system.Boolean/leaf_FALSE</variablePath>
                          </variablePathList>
                        </disabled>
                        <label>!</label>
                        <variable type="varPath">
                          <variablePathList>
                            <variablePath>findDoc_cwf_pm.BaseWorklistFinder/ui_UserInterface/action_imageCalculate</variablePath>
                          </variablePathList>
                        </variable>
                        <width>30px</width>
                      </element>
                      <element name="objectType" type="elfld">
                        <label>Object Type</label>
                        <variable type="varPath">
                          <variablePathList>
                            <variablePath>findDoc_workflow.customWorkListFinder/ui_UserInterface/uivar_result</variablePath>
                            <variablePath>doc_workflow.customWL/leaf_objectType</variablePath>
                          </variablePathList>
                        </variable>
                        <width>100px</width>
                      </element>
                      <element name="objectName" type="elfld">
                        <label>Object Name</label>
                        <variable type="varPath">
                          <variablePathList>
                            <variablePath>findDoc_workflow.customWorkListFinder/ui_UserInterface/uivar_result</variablePath>
                            <variablePath>doc_workflow.customWL/leaf_objectName</variablePath>
                          </variablePathList>
                        </variable>
                        <width>200px</width>
                      </element>
                      <element name="OrderVK" type="elfld">
                        <dataLength>40</dataLength>
                        <variable type="varPath">
                          <variablePathList>
                            <variablePath>findDoc_workflow.customWorkListFinder/ui_UserInterface/uivar_result</variablePath>
                            <variablePath>doc_cwl_worklist.worklist/leaf_OrderVK</variablePath>
                          </variablePathList>
                        </variable>
                        <width>100px</width>
                      </element>
                      <element name="DueDate" type="eldatetime">
                        <variable type="varPath">
                          <variablePathList>
                            <variablePath>findDoc_workflow.customWorkListFinder/ui_UserInterface/uivar_result</variablePath>
                            <variablePath>doc_cwl_worklist.worklist/leaf_DueDate</variablePath>
                          </variablePathList>
                        </variable>
                        <width>140px</width>
                      </element>
                      <element name="StartDate" type="eldatetime">
                        <variable type="varPath">
                          <variablePathList>
                            <variablePath>findDoc_workflow.customWorkListFinder/ui_UserInterface/uivar_result</variablePath>
                            <variablePath>doc_cwl_worklist.worklist/leaf_StartDate</variablePath>
                          </variablePathList>
                        </variable>
                        <width>120px</width>
                      </element>
                      <element name="AssignedDate" type="eldatetime">
                        <variable type="varPath">
                          <variablePathList>
                            <variablePath>findDoc_workflow.customWorkListFinder/ui_UserInterface/uivar_result</variablePath>
                            <variablePath>doc_cwl_worklist.worklist/leaf_AssignedDate</variablePath>
                          </variablePathList>
                        </variable>
                        <width>140px</width>
                      </element>
                      <element name="Operation" type="elfld">
                        <variable type="varPath">
                          <variablePathList>
                            <variablePath>findDoc_workflow.customWorkListFinder/ui_UserInterface/uivar_result</variablePath>
                            <variablePath>doc_cwl_worklist.worklist/leaf_Operation</variablePath>
                          </variablePathList>
                        </variable>
                        <width>200px</width>
                      </element>
                      <element name="createDate" type="eldatetime">
                        <variable type="varPath">
                          <variablePathList>
                            <variablePath>findDoc_workflow.customWorkListFinder/ui_UserInterface/uivar_result</variablePath>
                            <variablePath>doc_cwl_worklist.worklist/leaf_createDate</variablePath>
                          </variablePathList>
                        </variable>
                        <width>140px</width>
                      </element>
                      <element name="objectCreationDate" type="eldatetime">
                        <label>Object Creation Date</label>
                        <variable type="varPath">
                          <variablePathList>
                            <variablePath>findDoc_workflow.customWorkListFinder/ui_UserInterface/uivar_result</variablePath>
                            <variablePath>doc_workflow.customWL/leaf_objectCreationDate</variablePath>
                          </variablePathList>
                        </variable>
                        <width>155px</width>
                      </element>
                      <element name="objectPlannedRFSDate" type="eldatetime">
                        <label>Object Planned RFS Date</label>
                        <variable type="varPath">
                          <variablePathList>
                            <variablePath>findDoc_workflow.customWorkListFinder/ui_UserInterface/uivar_result</variablePath>
                            <variablePath>doc_workflow.customWL/leaf_objectPlannedRFSDate</variablePath>
                          </variablePathList>
                        </variable>
                        <width>155px</width>
                      </element>
                      <element name="customerName" type="elfld">
                        <label>Customer Name</label>
                        <variable type="varPath">
                          <variablePathList>
                            <variablePath>findDoc_workflow.customWorkListFinder/ui_UserInterface/uivar_result</variablePath>
                            <variablePath>doc_workflow.customWL/leaf_customerName</variablePath>
                          </variablePathList>
                        </variable>
                        <width>200px</width>
                      </element>
                      <element name="objectStatus" type="elfld">
                        <label>Object Status</label>
                        <variable type="varPath">
                          <variablePathList>
                            <variablePath>findDoc_workflow.customWorkListFinder/ui_UserInterface/uivar_result</variablePath>
                            <variablePath>doc_workflow.customWL/leaf_objectStatus</variablePath>
                          </variablePathList>
                        </variable>
                        <width>200px</width>
                      </element>
                      <element name="requester" type="elfld">
                        <label>Requester</label>
                        <variable type="varPath">
                          <variablePathList>
                            <variablePath>findDoc_workflow.customWorkListFinder/ui_UserInterface/uivar_result</variablePath>
                            <variablePath>doc_workflow.customWL/leaf_requester</variablePath>
                          </variablePathList>
                        </variable>
                        <width>200px</width>
                      </element>
                      <element name="SenderType" type="elfld">
                        <variable type="varPath">
                          <variablePathList>
                            <variablePath>findDoc_workflow.customWorkListFinder/ui_UserInterface/uivar_result</variablePath>
                            <variablePath>doc_cwl_worklist.worklist/leaf_SenderType</variablePath>
                          </variablePathList>
                        </variable>
                      </element>
                      <element name="Effort" type="elfld">
                        <variable type="varPath">
                          <variablePathList>
                            <variablePath>findDoc_workflow.customWorkListFinder/ui_UserInterface/uivar_result</variablePath>
                            <variablePath>doc_cwl_worklist.worklist/leaf_Effort</variablePath>
                          </variablePathList>
                        </variable>
                        <width>50px</width>
                      </element>
                      <element name="Priority" type="elslct">
                        <variable type="varPath">
                          <variablePathList>
                            <variablePath>findDoc_workflow.customWorkListFinder/ui_UserInterface/uivar_result</variablePath>
                            <variablePath>doc_cwl_worklist.worklist/leaf_Priority</variablePath>
                          </variablePathList>
                        </variable>
                        <width>100px</width>
                      </element>
                    </elementList>
                  </element>
                </elementList>
              </override>
            </overrideList>
          </overlay>
          <overlay name="Default" type="exov">
            <base>findDoc_cwf_pm.BaseWorklistFinder/ui_UserInterface/frmui_Forms/exov_Default</base>
            <extends>findDoc_cwf_pm.BaseWorklistFinder/ui_UserInterface/frmui_Forms/exov_Default</extends>
            <label>Default</label>
          </overlay>
          <overlay name="Menu" type="exov">
            <base>findDoc_cwf_pm.BaseWorklistFinder/ui_UserInterface/frmui_Forms/exov_Menu</base>
            <extends>findDoc_cwf_pm.BaseWorklistFinder/ui_UserInterface/frmui_Forms/exov_Menu</extends>
            <label>Menu</label>
            <overrideList>
              <override name="AssignTasksMenu" type="eldel"/>
              <override name="cwfSearch" type="elmnu">
                <menuAccel type="mAccelP"/>
                <clickMethod type="varPath">
                  <variablePathList>
                    <variablePath>ui_com.conceptwave.system.BaseFinderUserInterface/usrActionMethod_searchAction</variablePath>
                  </variablePathList>
                </clickMethod>
                <icon>/cwf/MenuIconSearch.gif</icon>
                <label>Search</label>
                <showDown>true</showDown>
                <showFocused>true</showFocused>
                <tooltip>Search</tooltip>
                <visible type="varPath">
                  <variablePathList>
                    <variablePath>ui_com.conceptwave.system.MasterDetailUserInterface/permMethod_searchVisible</variablePath>
                  </variablePathList>
                </visible>
              </override>
              <override name="ReturnToQueueMenu" type="eldel"/>
            </overrideList>
          </overlay>
        </overlayList>
      </vform>
    </vformList>
    <methodList>
      <method name="cwAcknowledgeAlert" type="usrActionMethod">
        <allowInvalidObject>true</allowInvalidObject>
        <validate>true</validate>
        <script><![CDATA[
          // complete action on acknowledge action

          if( this.result.selected.length > 0 ){
              // perform bulk complete
              for( var i=0; i<this.result.selected.length; i++){
                  var rec_wlId = this.result.selected[i].id;
                  var trackingRecord = workflow.getTrackingRecordByWLId(rec_wlId);
                  // set completion time
                  trackingRecord.completionDateTime = new Date();
                  trackingRecord.actionPerformed = "Acknowledge";
                  trackingRecord.save();
              }
          }

          this.cw$super_cwAcknowledgeAlert();
        ]]></script>
      </method>
    </methodList>
  </UserInterface>
  <viewList>
    <view name="tfsWorkListFinderView" type="findVView">
      <inputOverlay>doc_workflow.searchWLDoc/ui_UserInterface/frmui_Forms/exov_Default</inputOverlay>
      <label>TFS Worklist</label>
      <outputOverlay>findDoc_workflow.customWorkListFinder/ui_UserInterface/frmui_Forms/exov_Result</outputOverlay>
      <searchImmediately>true</searchImmediately>
    </view>
  </viewList>
</findDoc>