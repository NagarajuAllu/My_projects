<?xml version="1.0" encoding="UTF-8" ?>
<participant name="workflow.switchActivationParticipant">
  <distributionType>Shared</distributionType>
  <duration type="dur">
    <length>P0Y0M1DT0H0M0S</length>
  </duration>
  <interface>iface_workflow.switchActivationTeamIF</interface>
  <label>Switch Activation Team Participant</label>
  <metaVersion>25</metaVersion>
  <operationConditionsList>
    <operationConditions type="opco">
      <bulkComplete>true</bulkComplete>
      <operation>iface_workflow.switchActivationTeamIF/oper_switchActivationCompleted</operation>
      <tasksList>
        <tasks>prepareToCompleteActivation</tasks>
      </tasksList>
      <methodList>
        <method name="cwOnParticipantOpAct" type="action">
          <system>true</system>
          <script><![CDATA[
            workflow.computeNextUser(this, "installOrderProcess", "switchActivationCompleted");
            workflow.completeParticipantActivity(this.processOrder.id, this.workItemDocument, "Switch Activation Completed");

            if(Global.getWorkListFinder().selected.length == 1) {
                Global.showUserMessage("AI0006");
            }
          ]]></script>
        </method>
        <method name="cwOnPartOpConf" type="action">
          <system>true</system>
          <script><![CDATA[return "UU0167";]]></script>
        </method>
      </methodList>
    </operationConditions>
  </operationConditionsList>
  <privilege>STC_SwchActvPriv</privilege>
  <removeTasks>true</removeTasks>
  <UserInterface name="UserInterface" type="ui">
    <extends>ui_com.conceptwave.system.WLTaskUserInterface</extends>
    <vformList>
      <vform name="Forms" type="frmui">
        <extends>ui_com.conceptwave.system.WLTaskUserInterface/frmui_Forms</extends>
      </vform>
    </vformList>
  </UserInterface>
</participant>