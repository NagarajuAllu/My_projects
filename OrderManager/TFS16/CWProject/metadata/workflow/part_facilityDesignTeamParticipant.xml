            }
          ]]></script>
        </method>
        <method name="cwOnPartOpCond" type="action">
          <system>true</system>
          <script><![CDATA[
            var visible = true;

            var processInstance = new Process(this.workItemDocument.SenderId);
            if(processInstance != null) {
                var processType = processInstance.metadataTypeName;
                if(processType.indexOf("disconnectOrderProcess") >= 0) {
                    visible = false;
                }
            }

            if(visible) {
                // checking if there are BI linked to the PRN
                if(this.processOrder.prnDocument != null) {
                    var biFinder = new Finder("tfs.findBIForPRNOrTFR");
                    biFinder.searchDocument.sourcePRNId = this.processOrder.prnDocument.prnId;
                    var biFoundList = biFinder.search();

                    visible = (biFoundList == null || biFoundList.length ==0);
                }
            }

            return visible;
          ]]></script>
        </method>
        <method name="cwOnPartOpConf" type="action">
          <system>true</system>
          <script><![CDATA[return "UU0167";]]></script>
