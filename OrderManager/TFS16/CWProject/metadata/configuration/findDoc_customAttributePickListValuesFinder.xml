<?xml version="1.0" encoding="UTF-8" ?>
<findDoc name="configuration.customAttributePickListValuesFinder">
  <extends>find_com.conceptwave.system.Finder</extends>
  <input>doc_cwt_common.docID</input>
  <label>Custom Attribute PickList Values Finder</label>
  <maxRowsLimit>999</maxRowsLimit>
  <metaVersion>21</metaVersion>
  <output>doc_configuration.customAttributePickListValue</output>
  <sortList>
    <sort type="stitem">
      <path>doc_configuration.customAttributePickListValue/leaf_sequenceNumber</path>
    </sort>
  </sortList>
  <type>document</type>
  <volatility>10</volatility>
  <searchList>
    <search type="sitem">
      <document>doc_cwt_common.docID</document>
      <mappedDocument>doc_configuration.customAttributePickListValue</mappedDocument>
      <mappedPath>doc_configuration.customAttributePickListValue/leaf_customAttr_id</mappedPath>
      <operation>equal</operation>
      <path>doc_cwt_common.docID/leaf_cwDocID</path>
    </search>
  </searchList>
  <methodList>
    <method name="editablePerm" type="permMethod">
      <privilegesList>
        <privileges type="ppriv">
          <privilege>STC_ConfigPriv</privilege>
        </privileges>
      </privilegesList>
      <privilegesType>SOME</privilegesType>
      <script>return $psCondition;</script>
    </method>
    <method name="addPerm" type="permMethod">
      <privilegesList>
        <privileges type="ppriv">
          <privilege>STC_ConfigPriv</privilege>
        </privileges>
      </privilegesList>
      <privilegesType>SOME</privilegesType>
      <script>return $psCondition;</script>
    </method>
  </methodList>
  <UserInterface name="UserInterface" type="ui">
    <extends>ui_com.conceptwave.system.FinderUserInterface</extends>
    <variableList>
      <variable name="result" type="uivar">
        <flags enum-type="metadataFlag">ARRAY</flags>
        <valueType>doc_configuration.customAttributePickListValue</valueType>
      </variable>
      <variable name="search" type="uivar">
        <valueType>doc_cwt_common.docID/ui_UserInterface</valueType>
      </variable>
      <variable name="detail" type="uivar">
        <flags enum-type="metadataFlag">VALIDATE</flags>
        <valueType>doc_configuration.customAttributePickListValue/ui_UserInterface</valueType>
      </variable>
    </variableList>
    <vformList>
      <vform name="Forms" type="frmui">
        <extends>ui_com.conceptwave.system.FinderUserInterface/frmui_Forms</extends>
        <overlayList>
          <overlay name="Result" type="exov">
            <base>ui_com.conceptwave.system.FinderUserInterface/frmui_Forms/vform_Result</base>
            <extends>ui_com.conceptwave.system.FinderUserInterface/frmui_Forms/vform_Result</extends>
            <label>Result</label>
            <overrideList>
              <override name="table" type="elvlt">
                <height>100%</height>
                <width>100%</width>
                <elementList>
                  <element name="customAttributePickListValuesFinderResultTable" type="eltabl">
                    <cellWidth>200</cellWidth>
                    <height>400px</height>
                    <onSelectChange type="varPath">
                      <variablePathList>
                        <variablePath>ui_com.conceptwave.system.MasterDetailUserInterface/action_OnSelChanged</variablePath>
                      </variablePathList>
                    </onSelectChange>
                    <variable type="varPath">
                      <variablePathList>
                        <variablePath>findDoc_configuration.customAttributePickListValuesFinder/ui_UserInterface/uivar_result</variablePath>
                      </variablePathList>
                    </variable>
                    <width>600px</width>
                    <elementList>
                      <element name="customAttr_id" type="elfld">
                        <label>Custom Attribute Id</label>
                        <variable type="varPath">
                          <variablePathList>
                            <variablePath>findDoc_configuration.customAttributePickListValuesFinder/ui_UserInterface/uivar_result</variablePath>
                            <variablePath>doc_configuration.customAttributePickListValue/leaf_customAttr_id</variablePath>
                          </variablePathList>
                        </variable>
                      </element>
                      <element name="attributeValue" type="elfld">
                        <label>Attribute Value</label>
                        <variable type="varPath">
                          <variablePathList>
                            <variablePath>findDoc_configuration.customAttributePickListValuesFinder/ui_UserInterface/uivar_result</variablePath>
                            <variablePath>doc_configuration.customAttributePickListValue/leaf_attributeValue</variablePath>
                          </variablePathList>
                        </variable>
                      </element>
                      <element name="sequenceNumber" type="elfld">
                        <label>Sequence Number</label>
                        <variable type="varPath">
                          <variablePathList>
                            <variablePath>findDoc_configuration.customAttributePickListValuesFinder/ui_UserInterface/uivar_result</variablePath>
                            <variablePath>doc_configuration.customAttributePickListValue/leaf_sequenceNumber</variablePath>
                          </variablePathList>
                        </variable>
                      </element>
                    </elementList>
                  </element>
                </elementList>
              </override>
            </overrideList>
          </overlay>
          <overlay name="Default" type="exov">
            <base>ui_com.conceptwave.system.FinderUserInterface/frmui_Forms/exov_Default</base>
            <extends>ui_com.conceptwave.system.FinderUserInterface/frmui_Forms/exov_Default</extends>
            <label>Default</label>
            <overrideList>
              <override name="detailSection" type="elsect">
                <formReference type="varPath">
                  <variablePathList>
                    <variablePath>ui_com.conceptwave.system.MasterDetailUserInterface/uivar_detailForm</variablePath>
                  </variablePathList>
                </formReference>
                <variable type="varPath">
                  <variablePathList>
                    <variablePath>findDoc_configuration.customAttributePickListValuesFinder/ui_UserInterface/uivar_detail</variablePath>
                  </variablePathList>
                </variable>
                <visible type="varPath">
                  <variablePathList>
                    <variablePath>ui_com.conceptwave.system.MasterDetailUserInterface/permMethod_detailVisible</variablePath>
                  </variablePathList>
                </visible>
              </override>
              <override name="searchFormSection" type="elsect">
                <formReference type="varPath">
                  <variablePathList>
                    <variablePath>ui_com.conceptwave.system.BaseFinderUserInterface/uivar_searchForm</variablePath>
                  </variablePathList>
                </formReference>
                <variable type="varPath">
                  <variablePathList>
                    <variablePath>findDoc_configuration.customAttributePickListValuesFinder/ui_UserInterface/uivar_search</variablePath>
                  </variablePathList>
                </variable>
                <visible type="varPath">
                  <variablePathList>
                    <variablePath>ui_com.conceptwave.system.FinderUserInterface/permMethod_showSearchForm</variablePath>
                  </variablePathList>
                </visible>
              </override>
            </overrideList>
          </overlay>
          <overlay name="showInParent" type="vform">
            <label>showInParent</label>
            <root name="tableVerticalLayout" type="elvlt">
              <height>100%</height>
              <width>100%</width>
              <elementList>
                <element name="findPickListValuesForCustomAttrResultTable" type="eltabl">
                  <canReOrder type="varPath">
                    <variablePathList>
                      <variablePath>dtype_com.conceptwave.system.Boolean/leaf_FALSE</variablePath>
                    </variablePathList>
                  </canReOrder>
                  <doubleClickMethod type="varPath">
                    <variablePathList>
                      <variablePath>findDoc_configuration.customAttributePickListValuesFinder/ui_UserInterface/usrActionMethod_editPickListValue</variablePath>
                    </variablePathList>
                  </doubleClickMethod>
                  <height>80%</height>
                  <variable type="varPath">
                    <variablePathList>
                      <variablePath>findDoc_configuration.customAttributePickListValuesFinder/ui_UserInterface/uivar_result</variablePath>
                    </variablePathList>
                  </variable>
                  <width>430px</width>
                  <elementList>
                    <element name="attributeValue" type="elfld">
                      <label>Attribute Values</label>
                      <variable type="varPath">
                        <variablePathList>
                          <variablePath>findDoc_configuration.customAttributePickListValuesFinder/ui_UserInterface/uivar_result</variablePath>
                          <variablePath>doc_configuration.customAttributePickListValue/leaf_attributeValue</variablePath>
                        </variablePathList>
                      </variable>
                      <width>400px</width>
                    </element>
                  </elementList>
                </element>
              </elementList>
            </root>
          </overlay>
        </overlayList>
      </vform>
    </vformList>
    <methodList>
      <method name="editPickListValue" type="usrActionMethod">
        <dialogObject>ui_com.conceptwave.system.Dialog</dialogObject>
        <showInPopup>true</showInPopup>
        <validate>true</validate>
        <script>return this.model.selected[0];</script>
      </method>
    </methodList>
  </UserInterface>
  <viewList>
    <view name="customAttributePickListValuesFinderView" type="findVView">
      <label>Custom Atrributes List</label>
      <outputOverlay>findDoc_configuration.customAttributePickListValuesFinder/ui_UserInterface/frmui_Forms/exov_Result</outputOverlay>
    </view>
  </viewList>
</findDoc>