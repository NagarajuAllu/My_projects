<?xml version="1.0" encoding="UTF-8" ?>
<script name="up_priv.setUserAvailable">
  <label>Set User Available</label>
  <metaVersion>25</metaVersion>
  <parameterList>
    <parameter name="userId" type="rifp">
      <type>dtype_com.conceptwave.system.String</type>
    </parameter>
    <parameter name="newAvailableStatus" type="rifp">
      <type>dtype_com.conceptwave.system.Boolean</type>
    </parameter>
  </parameterList>
  <script>return security.setAvailable(userId, newAvailableStatus);</script>
</script>