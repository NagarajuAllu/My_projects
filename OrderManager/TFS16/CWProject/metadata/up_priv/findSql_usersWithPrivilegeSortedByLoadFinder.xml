<?xml version="1.0" encoding="UTF-8" ?>
<findSql name="up_priv.usersWithPrivilegeSortedByLoadFinder">
  <dbSchema>ORDER</dbSchema>
  <extends>find_com.conceptwave.system.Finder</extends>
  <input>doc_common.docString</input>
  <label>Users With Privilege Sorted By Load Finder</label>
  <maxRowsLimit>999</maxRowsLimit>
  <metaVersion>21</metaVersion>
  <output>doc_cwf_up.userProfile</output>
  <type>SQL</type>
  <methodList>
    <method name="cwOnFinderSQLSel" type="action">
      <system>true</system>
      <script><![CDATA[
        select {+*+}
          from cwuser u
         where u.userid in (
           select g.userid
             from cwuserrole g, cwusergroupprivilege gp
            where g.roleid = gp.usergroup
        {[0]  and gp.privilege = ?}
        )
          and u.available = 1
          and u.active = 1
        order by (select count(*) from cwpworklist w where w.user_id = u.userid)
      ]]></script>
    </method>
  </methodList>
