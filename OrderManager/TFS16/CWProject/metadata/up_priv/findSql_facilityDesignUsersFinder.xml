    <methodList>
      <method name="onInit" type="action">
        <script><![CDATA[
          var onInitResult = this.cw$super_onInit();

          this.selectedRecords = null;

          return onInitResult;
        ]]></script>
      </method>
      <method name="OnSelChanged" type="action">
        <script><![CDATA[
          var superSelChanged = this.cw$super_OnSelChanged();

          var isSelectionChangedMethodInParent = Global.isMethodDefined(this.parent, "userSelectionChanged");

          if(this.model.selected != null && this.model.selected.length > 0) {
              var sel = this.model.selected.join(";");
              if(sel != this.selectedRecords) {
                  this.selectedRecords = sel;
                  if(isSelectionChangedMethodInParent) {
                      this.parent.userSelectionChanged();
                  }
              }
          }
          else {
              if(this.selectedRecords != null) {
                  this.selectedRecords = null;
                  if(isSelectionChangedMethodInParent) {
                      this.parent.userSelectionChanged();
                  }
              }
          }
        ]]></script>
      </method>
