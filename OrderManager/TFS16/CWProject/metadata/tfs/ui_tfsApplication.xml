    <method name="showAllMembersAvailability" type="usrActionMethod">
      <script><![CDATA[
        var finderForCurrentUserPrivilege = new Finder("up_priv.getUserSTCPrivileges_NoManager");
        finderForCurrentUserPrivilege.searchDocument.value = Global.getUserId();
        var resultList = finderForCurrentUserPrivilege.search();

        if(resultList != null && resultList.length > 0) {
            var privList = "";
            for (var i=0; i<resultList.length; i++) {
                privList = (i == 0 ? "" : ",") + resultList[i].privilegeCode;
            }

            var userFinder = new Finder("up_priv.getAllUsersByPrivileges");
            userFinder.searchDocument.value = privList;
            userFinder.search();

            return userFinder;
        }
        else {
            Global.showUserMessage("No application privileges found for the current user");
        }
      ]]></script>
    </method>
