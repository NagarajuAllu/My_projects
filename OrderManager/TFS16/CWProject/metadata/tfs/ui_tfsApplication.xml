    <method name="selectApplication" type="usrActionMethod">
      <allowInvalidObject>true</allowInvalidObject>
      <autosave>true</autosave>
      <validate>true</validate>
      <script><![CDATA[return new Document("tfs.SelectApp");]]></script>
    </method>
    <method name="changePassword" type="usrActionMethod">
      <allowInvalidObject>true</allowInvalidObject>
      <form>doc_cwf_up.chgPwdDoc/ui_UserInterface/frmui_Forms/vform_Default</form>
