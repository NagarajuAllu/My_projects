      <autosave>true</autosave>
      <validate>true</validate>
      <script>UserProfile.logout();</script>
    </method>
    <method name="orderInitiatorPrivilege" type="permMethod">
      <privilegesList>
        <privileges type="ppriv">
          <privilege>STC_IOrdInitPriv</privilege>
        </privileges>
      </privilegesList>
      <privilegesType>SOME</privilegesType>
      <script>return $psCondition;</script>
    </method>
    <method name="createNewPRNR" type="usrActionMethod">
      <script><![CDATA[
        var tfsOrder = new Order("tfs.tfsOrder");
        var prnrDoc = tfsOrder.createOrderItemByPath("prnrDocument");
        tfsOrder.save();

        return prnrDoc;
      ]]></script>
    </method>
