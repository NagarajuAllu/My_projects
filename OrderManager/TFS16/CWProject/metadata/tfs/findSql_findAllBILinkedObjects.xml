<?xml version="1.0" encoding="UTF-8" ?>
<findSql name="tfs.findAllBILinkedObjects">
  <dbSchema>ORDER</dbSchema>
  <extends>find_com.conceptwave.system.Finder</extends>
  <input>doc_tfs.biSearchDoc</input>
  <label>Find All BI Linked Objects</label>
  <maxRowsLimit>9999</maxRowsLimit>
  <metaVersion>25</metaVersion>
  <output>doc_tfs.STC_BI_RELATIONSHIP</output>
  <type>SQL</type>
  <volatility>1</volatility>
  <methodList>
    <method name="cwOnFinderSQLSel" type="action">
      <system>true</system>
      <script><![CDATA[
        select {+*+}
          from stc_bi_relationship r
         where 1 = 1
        {[0] and r.bi_id = ?}
      ]]></script>
    </method>
  </methodList>
  <UserInterface name="UserInterface" type="ui">
    <extends>ui_com.conceptwave.system.FinderUserInterface</extends>
    <variableList>
      <variable name="finder" type="uivar">
        <valueType>ui_com.conceptwave.system.FinderUserInterface</valueType>
      </variable>
      <variable name="detail" type="uivar">
        <flags enum-type="metadataFlag">VALIDATE</flags>
        <valueType>doc_tfs.STC_BI_RELATIONSHIP/ui_UserInterface</valueType>
      </variable>
      <variable name="result" type="uivar">
        <flags enum-type="metadataFlag">ARRAY</flags>
        <valueType>doc_tfs.STC_BI_RELATIONSHIP</valueType>
      </variable>
      <variable name="search" type="uivar">
        <valueType>doc_tfs.biSearchDoc/ui_UserInterface</valueType>
      </variable>
    </variableList>
    <vformList>
      <vform name="Forms" type="frmui">
        <extends>ui_com.conceptwave.system.FinderUserInterface/frmui_Forms</extends>
        <overlayList>
          <overlay name="Default" type="exov">
            <base>ui_com.conceptwave.system.FinderUserInterface/frmui_Forms/exov_Default</base>
            <extends>ui_com.conceptwave.system.FinderUserInterface/frmui_Forms/exov_Default</extends>
            <label>Default</label>
            <overrideList>
              <override name="detailSection" type="elsect">
                <formReference type="varPath">
                  <variablePathList>
                    <variablePath>ui_com.conceptwave.system.MasterDetailUserInterface/uivar_detailForm</variablePath>
                  </variablePathList>
                </formReference>
                <variable type="varPath">
                  <variablePathList>
                    <variablePath>findSql_tfs.findAllBILinkedObjects/ui_UserInterface/uivar_detail</variablePath>
                  </variablePathList>
                </variable>
                <visible type="varPath">
                  <variablePathList>
                    <variablePath>ui_com.conceptwave.system.MasterDetailUserInterface/permMethod_detailVisible</variablePath>
                  </variablePathList>
                </visible>
              </override>
              <override name="searchFormSection" type="elsect">
                <formReference type="varPath">
                  <variablePathList>
                    <variablePath>ui_com.conceptwave.system.BaseFinderUserInterface/uivar_searchForm</variablePath>
                  </variablePathList>
                </formReference>
                <variable type="varPath">
                  <variablePathList>
                    <variablePath>findSql_tfs.findAllBILinkedObjects/ui_UserInterface/uivar_search</variablePath>
                  </variablePathList>
                </variable>
                <visible type="varPath">
                  <variablePathList>
                    <variablePath>ui_com.conceptwave.system.FinderUserInterface/permMethod_showSearchForm</variablePath>
                  </variablePathList>
                </visible>
              </override>
            </overrideList>
          </overlay>
          <overlay name="Result" type="exov">
            <base>ui_com.conceptwave.system.FinderUserInterface/frmui_Forms/vform_Result</base>
            <extends>ui_com.conceptwave.system.FinderUserInterface/frmui_Forms/vform_Result</extends>
            <label>Result</label>
            <overrideList>
              <override name="table" type="elvlt">
                <height>100%</height>
                <width>100%</width>
                <elementList>
                  <element name="findAllBILinkedObjectsResultTable" type="eltabl">
                    <doubleClickMethod type="varPath">
                      <variablePathList>
                        <variablePath>findSql_tfs.findAllBILinkedObjects/ui_UserInterface/usrActionMethod_showLinkedElement</variablePath>
                      </variablePathList>
                    </doubleClickMethod>
                    <onSelectChange type="varPath">
                      <variablePathList>
                        <variablePath>ui_com.conceptwave.system.MasterDetailUserInterface/action_OnSelChanged</variablePath>
                      </variablePathList>
                    </onSelectChange>
                    <variable type="varPath">
                      <variablePathList>
                        <variablePath>findSql_tfs.findAllBILinkedObjects/ui_UserInterface/uivar_result</variablePath>
                      </variablePathList>
                    </variable>
                    <elementList>
                      <element name="biId" type="elfld">
                        <label>BI Id</label>
                        <variable type="varPath">
                          <variablePathList>
                            <variablePath>findSql_tfs.findAllBILinkedObjects/ui_UserInterface/uivar_result</variablePath>
                            <variablePath>doc_tfs.STC_BI_RELATIONSHIP/leaf_biId</variablePath>
                          </variablePathList>
                        </variable>
                      </element>
                      <element name="prnName" type="elfld">
                        <label>PRN Name</label>
                        <variable type="varPath">
                          <variablePathList>
                            <variablePath>findSql_tfs.findAllBILinkedObjects/ui_UserInterface/uivar_result</variablePath>
                            <variablePath>doc_tfs.STC_BI_RELATIONSHIP/leaf_prnName</variablePath>
                          </variablePathList>
                        </variable>
                        <width>150px</width>
                      </element>
                      <element name="prnStatus" type="elfld">
                        <label>PRN Status</label>
                        <variable type="varPath">
                          <variablePathList>
                            <variablePath>findSql_tfs.findAllBILinkedObjects/ui_UserInterface/uivar_result</variablePath>
                            <variablePath>doc_tfs.STC_BI_RELATIONSHIP/leaf_prnStatus</variablePath>
                          </variablePathList>
                        </variable>
                        <width>150px</width>
                      </element>
                      <element name="tfrNumber" type="elfld">
                        <label>TFR Number</label>
                        <variable type="varPath">
                          <variablePathList>
                            <variablePath>findSql_tfs.findAllBILinkedObjects/ui_UserInterface/uivar_result</variablePath>
                            <variablePath>doc_tfs.STC_BI_RELATIONSHIP/leaf_tfrNumber</variablePath>
                          </variablePathList>
                        </variable>
                        <width>150px</width>
                      </element>
                      <element name="tfrStatus" type="elfld">
                        <label>TFR Status</label>
                        <variable type="varPath">
                          <variablePathList>
                            <variablePath>findSql_tfs.findAllBILinkedObjects/ui_UserInterface/uivar_result</variablePath>
                            <variablePath>doc_tfs.STC_BI_RELATIONSHIP/leaf_tfrStatus</variablePath>
                          </variablePathList>
                        </variable>
                        <width>150px</width>
                      </element>
                    </elementList>
                  </element>
                </elementList>
              </override>
            </overrideList>
          </overlay>
          <overlay name="Menu" type="exov">
            <base>ui_com.conceptwave.system.BaseFinderUserInterface/frmui_Forms/exov_Menu</base>
            <extends>ui_com.conceptwave.system.BaseFinderUserInterface/frmui_Forms/exov_Menu</extends>
            <label>Menu</label>
            <overrideList>
              <override name="cwfAdd" type="eldel"/>
              <override name="cwfCopy" type="eldel"/>
              <override name="cwfDelete" type="eldel"/>
              <override name="cwfUpdate" type="eldel"/>
              <override name="finderMenuLayout" type="elext">
                <elementList>
                  <element name="showLinkedElement" type="elmnu">
                    <clickMethod type="varPath">
                      <variablePathList>
                        <variablePath>findSql_tfs.findAllBILinkedObjects/ui_UserInterface/usrActionMethod_showLinkedElement</variablePath>
                      </variablePathList>
                    </clickMethod>
                    <label>Show Linked Element</label>
                  </element>
                </elementList>
              </override>
            </overrideList>
          </overlay>
        </overlayList>
      </vform>
    </vformList>
    <methodList>
      <method name="showLinkedElement" type="usrActionMethod">
        <dialogHeight>768</dialogHeight>
        <dialogWidth>1024</dialogWidth>
        <showInPopup>true</showInPopup>
        <script><![CDATA[
          if(this.result.selected == null || this.result.selected.length > 1) {
              Global.throwException("UU0116");
          }

          var selected = this.result.selected[0];
          var resultList = null;
          var objType = null;
          var objId = null;

          if(selected.prnId != null && selected.prnId != "-1") {
              objType = "PRN";
              objId = selected.prnId;
              var prnFinder = new Finder("tfs.findPRN");
              prnFinder.searchDocument.prnId = selected.prnId;
              resultList = prnFinder.search();
          }
          else{
              objType = "TFR";
              objId = selected.tfrId;
              var tfrFinder = new Finder("tfs.findTFR");
              tfrFinder.searchDocument.tfrId = selected.tfrId;
              resultList = tfrFinder.search();
          }

          if(resultList == null) {
              // selected element
              Global.throwException("AE0026", [objType, objId]);
          }

          return resultList[0];
        ]]></script>
      </method>
    </methodList>
  </UserInterface>
  <viewList>
    <view name="findAllBILinkedObjectsView" type="findVView">
      <label>Find All Objects Linked to the BI</label>
      <outputOverlay>findSql_tfs.findAllBILinkedObjects/ui_UserInterface/frmui_Forms/exov_Result</outputOverlay>
    </view>
  </viewList>
</findSql>