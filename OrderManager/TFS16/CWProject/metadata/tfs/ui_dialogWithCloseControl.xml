<?xml version="1.0" encoding="UTF-8" ?>
<userInterface name="tfs.dialogWithCloseControl">
  <extends>ui_com.conceptwave.system.Dialog</extends>
  <label>Dialog With Close Control</label>
  <metaVersion>25</metaVersion>
  <vformList>
    <vform name="Forms" type="frmui">
      <extends>ui_com.conceptwave.system.Dialog/frmui_Forms</extends>
      <overlayList>
        <overlay name="Default" type="exov">
          <base>ui_com.conceptwave.system.Dialog/frmui_Forms/exov_Default</base>
          <extends>ui_com.conceptwave.system.Dialog/frmui_Forms/exov_Default</extends>
          <label>Default</label>
          <overrideList>
            <override name="dialog" type="elDialog">
              <autoFitContent type="varPath">
                <variablePathList>
                  <variablePath>ui_com.conceptwave.system.Dialog/leaf_autoFit</variablePath>
                </variablePathList>
              </autoFitContent>
              <dialogX type="varPath">
                <variablePathList>
                  <variablePath>ui_com.conceptwave.system.Dialog/leaf_dialogX</variablePath>
                </variablePathList>
              </dialogX>
              <dialogY type="varPath">
                <variablePathList>
                  <variablePath>ui_com.conceptwave.system.Dialog/leaf_dialogY</variablePath>
                </variablePathList>
              </dialogY>
              <dynamicHeight type="varPath">
                <variablePathList>
                  <variablePath>ui_com.conceptwave.system.Dialog/leaf_dialogHeight</variablePath>
                </variablePathList>
              </dynamicHeight>
              <dynamicWidth type="varPath">
                <variablePathList>
                  <variablePath>ui_com.conceptwave.system.Dialog/leaf_dialogWidth</variablePath>
                </variablePathList>
              </dynamicWidth>
              <labelVar type="varPath">
                <variablePathList>
                  <variablePath>ui_com.conceptwave.system.Window/uivar_title</variablePath>
                </variablePathList>
              </labelVar>
              <maskOpacity type="varPath">
                <variablePathList>
                  <variablePath>ui_com.conceptwave.system.Dialog/leaf_modalMaskOpacity</variablePath>
                </variablePathList>
              </maskOpacity>
              <modal type="varPath">
                <variablePathList>
                  <variablePath>ui_com.conceptwave.system.Dialog/leaf_isModal</variablePath>
                </variablePathList>
              </modal>
              <showCloseButton>false</showCloseButton>
              <showModalMask>false</showModalMask>
              <elementList>
                <element name="vLayout" type="elvlt">
                  <height>100%</height>
                  <width>100%</width>
                  <elementList>
                    <element name="content" type="elovf">
                      <formReference type="varPath">
                        <variablePathList>
                          <variablePath>ui_com.conceptwave.system.Window/uivar_contentForm</variablePath>
                        </variablePathList>
                      </formReference>
                      <height>100%</height>
                      <overflow>visible</overflow>
                      <variable type="varPath">
                        <variablePathList>
                          <variablePath>ui_com.conceptwave.system.Window/uivar_content</variablePath>
                        </variablePathList>
                      </variable>
                      <width>100%</width>
                    </element>
                    <element name="statusMessage" type="ellab">
                      <variable type="varPath">
                        <variablePathList>
                          <variablePath>ui_com.conceptwave.system.Dialog/leaf_statusMessage</variablePath>
                        </variablePathList>
                      </variable>
                      <visible type="varPath">
                        <variablePathList>
                          <variablePath>ui_com.conceptwave.system.Dialog/permMethod_statusMessagePerm</variablePath>
                        </variablePathList>
                      </visible>
                    </element>
                    <element name="mLayout" type="elhlt">
                      <width>100%</width>
                      <elementList>
                        <element name="contentMenu" type="elovf">
                          <cellAlignment></cellAlignment>
                          <formReference type="varPath">
                            <variablePathList>
                              <variablePath>ui_com.conceptwave.system.Window/uivar_content</variablePath>
                              <variablePath>ui_com.conceptwave.system.UserInterface/frmui_Forms/vform_Menu</variablePath>
                            </variablePathList>
                          </formReference>
                          <height>1px</height>
                          <overflow>visible</overflow>
                          <variable type="varPath">
                            <variablePathList>
                              <variablePath>ui_com.conceptwave.system.Window/uivar_content</variablePath>
                            </variablePathList>
                          </variable>
                          <width>100%</width>
                        </element>
                        <element name="HelpImage" type="elimg">
                          <clickMethod type="varPath">
                            <variablePathList>
                              <variablePath>ui_com.conceptwave.system.Application/usrActionMethod_generateHelp</variablePath>
                            </variablePathList>
                          </clickMethod>
                          <imageUrl>/cwf/help.gif</imageUrl>
                          <tooltip>Show Help</tooltip>
                          <variable type="varPath">
                            <variablePathList>
                              <variablePath>ui_com.conceptwave.system.Dialog/leaf_dialogHelpVariable</variablePath>
                            </variablePathList>
                          </variable>
                          <visible type="varPath">
                            <variablePathList>
                              <variablePath>ui_com.conceptwave.system.Dialog/permMethod_showHelpImg</variablePath>
                            </variablePathList>
                          </visible>
                        </element>
                      </elementList>
                    </element>
                    <element name="dialogContent" type="elovf">
                      <formReference type="varPath">
                        <variablePathList>
                          <variablePath>ui_com.conceptwave.system.Window/uivar_dialog</variablePath>
                          <variablePath>ui_com.conceptwave.system.Dialog/frmui_Forms/exov_Default</variablePath>
                        </variablePathList>
                      </formReference>
                      <unmanaged>true</unmanaged>
                      <variable type="varPath">
                        <variablePathList>
                          <variablePath>ui_com.conceptwave.system.Window/uivar_dialog</variablePath>
                        </variablePathList>
                      </variable>
                    </element>
                  </elementList>
                </element>
              </elementList>
            </override>
          </overrideList>
        </overlay>
      </overlayList>
    </vform>
  </vformList>
</userInterface>