    <methodList>
      <method name="onInit" type="action">
        <valueType>nmeta_com.conceptwave.system.Object</valueType>
        <script><![CDATA[
          var ret = this.cw$super_onInit();

          this.allAnswersForQuestionFinderUI = new tfs.allAnswersForQuestionFinder.UserInterface(this, null);
          this.initializeDocument();

          return ret;
        ]]></script>
      </method>
      <method name="initializeDocument" type="uimthd">
        <script><![CDATA[
          if(this.model.questionId != null) {
              this.allAnswersForQuestionFinderUI.search.model.cwDocID = this.model.questionId;
              this.allAnswersForQuestionFinderUI.model.search();

              var rejectionQuestionFinder = new Finder("tfs.rejectionQuestionFinder");
              //rejectionQuestionFinder.searchDocument.cwDocID = this.model.questionId;
              rejectionQuestionFinder.searchDocument.questionId = this.model.questionId; // updated by Barchynskyi M.
              var questionList = rejectionQuestionFinder.search();

              if(questionList != null && questionList.length > 0) {
                  this.questionText = questionList[0].textQuestion;
              }
          }
          else {
              this.questionText = null;
          }
        ]]></script>
      </method>
      <method name="save" type="usrActionMethod">
        <allowInvalidObject>true</allowInvalidObject>
        <validate>true</validate>
        <script><![CDATA[
          if(this.allAnswersForQuestionFinderUI.model.selected == null || this.allAnswersForQuestionFinderUI.model.selected.length == 0) {
              Global.throwException("UU0115");
          }

          if(this.allAnswersForQuestionFinderUI.model.selected.length > 1) {
              Global.throwException("UU0116");
          }

          this.model.selectedAnswerId = this.allAnswersForQuestionFinderUI.model.selected[0].answerId;

          this.cw$super_save();
        ]]></script>
      </method>
      <method name="showTFR" type="usrActionMethod">
        <dialogHeight>768</dialogHeight>
        <dialogWidth>1024</dialogWidth>
        <showInPopup>true</showInPopup>
        <script><![CDATA[
          if(this.model.tfrId == null) {
              Global.throwException("AE0010", ["TFR", "Rejection Process"]);
          }

          var tfrFinder = new Finder("tfs.findTFR");
          tfrFinder.searchDocument.tfrId = this.model.tfrId;
          var resultList = tfrFinder.search();

          var tfrFound = null;

          if(resultList != null && resultList.length > 0) {
              tfrFound = resultList[0];
          }
          else {
              Global.throwException("AE0008", ["TFR", "Rejection Process"]);
          }

          return tfrFound;
        ]]></script>
      </method>
      <method name="showActivities" type="usrActionMethod">
        <dialogHeight>600</dialogHeight>
        <dialogWidth>930</dialogWidth>
        <showInPopup>true</showInPopup>
        <script><![CDATA[
          var activitiesFinder = new Finder("workflow.rejectionProcessActivityForGUIFinder");
          activitiesFinder.searchDocument.cwDocID = this.model.processId;
          activitiesFinder.search();

          return activitiesFinder;
        ]]></script>
      </method>
