<?xml version="1.0" encoding="UTF-8" ?>
<findSql name="tfs.findAllTasks">
  <dbSchema>ORDER</dbSchema>
  <extends>find_com.conceptwave.system.Finder</extends>
  <getOutput>doc_tfs.STC_TFR_TASK</getOutput>
  <input>doc_tfs.tfrTaskSearchDoc</input>
  <label>Find All Tasks</label>
  <maxRowsLimit>9999</maxRowsLimit>
  <metaVersion>25</metaVersion>
  <output>doc_tfs.STC_TFR_TASK</output>
  <type>SQL</type>
  <vDetailForm>doc_tfs.STC_TFR_TASK/ui_UserInterface/frmui_Forms/exov_Default</vDetailForm>
  <volatility>10</volatility>
  <methodList>
    <method name="cwOnFinderSQLSel" type="action">
      <system>true</system>
      <script><![CDATA[
        select {+*+}
          from stc_tfr_task
         where 1=1
        {[1] and task_name like ?}
        {[2] and tfr_id in (select tfr_id
                              from stc_tfr
                             where tfr_number like ?)}
        {[3] and task_status = ?}
        {[4] and creation_date >= ?}
        {[5] and creation_date <= ?}
      ]]></script>
    </method>
    <method name="cwOnFinderSQLGet" type="action">
      <system>true</system>
      <script><![CDATA[
        select {+*+}
          from stc_tfr_task
         where tfr_task_id = ?
      ]]></script>
    </method>
  </methodList>
  <UserInterface name="UserInterface" type="ui">
    <extends>ui_com.conceptwave.system.FinderUserInterface</extends>
    <variableList>
      <variable name="finder" type="uivar">
