<?xml version="1.0" encoding="UTF-8" ?>
<findSql name="tfs.rejectionAnswerFinder">
  <dbSchema>ORDER</dbSchema>
  <extends>find_com.conceptwave.system.Finder</extends>
  <input>doc_cwt_common.docID</input>
  <label>Rejection Answer Finder</label>
  <maxRowsLimit>9999</maxRowsLimit>
  <metaVersion>21</metaVersion>
  <output>doc_tfs.rejectionAnswer</output>
  <type>SQL</type>
  <vDetailForm>doc_tfs.rejectionAnswer/ui_UserInterface/frmui_Forms/exov_Default</vDetailForm>
  <volatility>10</volatility>
  <methodList>
    <method name="cwOnFinderSQLSel" type="action">
      <system>true</system>
      <script><![CDATA[
        select a.answer_id, a.parent_question_id, q.text_question, a.text_answer, a.response_to_return
          from stc_reject_answer a, stc_reject_question q
         where q.question_id = a.parent_question_id
        {[0] and a.answer_id = ?}
      ]]></script>
    </method>
    <method name="editablePerm" type="permMethod">
      <privilegesList>
        <privileges type="ppriv">
          <privilege>STC_ConfigPriv</privilege>
        </privileges>
      </privilegesList>
      <privilegesType>SOME</privilegesType>
      <script>return $psCondition;</script>
    </method>
    <method name="addPerm" type="permMethod">
      <privilegesList>
        <privileges type="ppriv">
          <privilege>STC_ConfigPriv</privilege>
        </privileges>
      </privilegesList>
      <privilegesType>SOME</privilegesType>
      <script>return $psCondition;</script>
    </method>
  </methodList>
  <UserInterface name="UserInterface" type="ui">
    <extends>ui_com.conceptwave.system.FinderUserInterface</extends>
    <variableList>
      <variable name="result" type="uivar">
        <flags enum-type="metadataFlag">ARRAY</flags>
        <valueType>doc_tfs.rejectionAnswer</valueType>
      </variable>
      <variable name="search" type="uivar">
        <valueType>doc_cwt_common.docID/ui_UserInterface</valueType>
      </variable>
      <variable name="detail" type="uivar">
        <flags enum-type="metadataFlag">VALIDATE</flags>
        <valueType>doc_tfs.rejectionAnswer/ui_UserInterface</valueType>
      </variable>
    </variableList>
    <vformList>
      <vform name="Forms" type="frmui">
        <extends>ui_com.conceptwave.system.FinderUserInterface/frmui_Forms</extends>
        <overlayList>
          <overlay name="Result" type="exov">
            <base>ui_com.conceptwave.system.FinderUserInterface/frmui_Forms/vform_Result</base>
            <extends>ui_com.conceptwave.system.FinderUserInterface/frmui_Forms/vform_Result</extends>
            <label>Result</label>
            <overrideList>
              <override name="table" type="elvlt">
                <height>100%</height>
                <width>100%</width>
                <elementList>
                  <element name="rejectionAnswerFinderResultTable" type="eltabl">
                    <doubleClickMethod type="varPath">
                      <variablePathList>
                        <variablePath>ui_com.conceptwave.system.MasterDetailUserInterface/action_showDetailAction</variablePath>
                      </variablePathList>
                    </doubleClickMethod>
                    <onSelectChange type="varPath">
                      <variablePathList>
                        <variablePath>ui_com.conceptwave.system.MasterDetailUserInterface/action_OnSelChanged</variablePath>
                      </variablePathList>
                    </onSelectChange>
                    <variable type="varPath">
                      <variablePathList>
                        <variablePath>findSql_tfs.rejectionAnswerFinder/ui_UserInterface/uivar_result</variablePath>
                      </variablePathList>
                    </variable>
                    <width>1080px</width>
                    <elementList>
                      <element name="answerId" type="elfld">
                        <label>Answer Id</label>
                        <variable type="varPath">
                          <variablePathList>
                            <variablePath>findSql_tfs.rejectionAnswerFinder/ui_UserInterface/uivar_result</variablePath>
                            <variablePath>doc_tfs.rejectionAnswer/leaf_answerId</variablePath>
                          </variablePathList>
                        </variable>
                        <width>100px</width>
                      </element>
                      <element name="textQuestion" type="elfld">
                        <label>Parent Question</label>
                        <variable type="varPath">
                          <variablePathList>
                            <variablePath>findSql_tfs.rejectionAnswerFinder/ui_UserInterface/uivar_result</variablePath>
                            <variablePath>doc_tfs.rejectionAnswer/leaf_textQuestion</variablePath>
                          </variablePathList>
                        </variable>
                        <width>350px</width>
                      </element>
                      <element name="textAnswer" type="eltxtArea">
                        <label>Answer</label>
                        <variable type="varPath">
                          <variablePathList>
                            <variablePath>findSql_tfs.rejectionAnswerFinder/ui_UserInterface/uivar_result</variablePath>
                            <variablePath>doc_tfs.rejectionAnswer/leaf_textAnswer</variablePath>
                          </variablePathList>
                        </variable>
                        <width>450px</width>
                      </element>
                      <element name="rejectionReturnPoint" type="elslct">
                        <label>Rejection Return Point</label>
                        <variable type="varPath">
                          <variablePathList>
                            <variablePath>findSql_tfs.rejectionAnswerFinder/ui_UserInterface/uivar_result</variablePath>
                            <variablePath>doc_tfs.rejectionAnswer/leaf_resultAnalysisToReturn</variablePath>
                          </variablePathList>
                        </variable>
                        <width>150px</width>
                      </element>
                    </elementList>
                  </element>
                </elementList>
              </override>
            </overrideList>
          </overlay>
          <overlay name="Default" type="exov">
            <base>ui_com.conceptwave.system.FinderUserInterface/frmui_Forms/exov_Default</base>
            <extends>ui_com.conceptwave.system.FinderUserInterface/frmui_Forms/exov_Default</extends>
            <label>Default</label>
            <overrideList>
              <override name="detailSection" type="elsect">
                <formReference type="varPath">
                  <variablePathList>
                    <variablePath>ui_com.conceptwave.system.MasterDetailUserInterface/uivar_detailForm</variablePath>
                  </variablePathList>
                </formReference>
                <variable type="varPath">
                  <variablePathList>
                    <variablePath>findSql_tfs.rejectionAnswerFinder/ui_UserInterface/uivar_detail</variablePath>
                  </variablePathList>
                </variable>
                <visible type="varPath">
                  <variablePathList>
                    <variablePath>ui_com.conceptwave.system.MasterDetailUserInterface/permMethod_detailVisible</variablePath>
                  </variablePathList>
                </visible>
              </override>
              <override name="searchFormSection" type="elsect">
                <formReference type="varPath">
                  <variablePathList>
                    <variablePath>ui_com.conceptwave.system.BaseFinderUserInterface/uivar_searchForm</variablePath>
                  </variablePathList>
                </formReference>
                <variable type="varPath">
                  <variablePathList>
                    <variablePath>findSql_tfs.rejectionAnswerFinder/ui_UserInterface/uivar_search</variablePath>
                  </variablePathList>
                </variable>
                <visible type="varPath">
                  <variablePathList>
                    <variablePath>ui_com.conceptwave.system.FinderUserInterface/permMethod_showSearchForm</variablePath>
                  </variablePathList>
                </visible>
              </override>
            </overrideList>
          </overlay>
          <overlay name="Menu" type="exov">
            <base>ui_com.conceptwave.system.BaseFinderUserInterface/frmui_Forms/exov_Menu</base>
            <extends>ui_com.conceptwave.system.BaseFinderUserInterface/frmui_Forms/exov_Menu</extends>
            <label>Menu</label>
            <overrideList>
              <override name="cwfCopy" type="eldel"/>
            </overrideList>
          </overlay>
        </overlayList>
      </vform>
    </vformList>
    <methodList>
      <method name="canDelete" type="permMethod">
        <privilegesList>
          <privileges type="ppriv">
            <privilege>STC_ConfigPriv</privilege>
          </privileges>
        </privilegesList>
        <privilegesType>SOME</privilegesType>
        <script>return $psCondition</script>
      </method>
    </methodList>
  </UserInterface>
  <viewList>
    <view name="rejectionAnswerFinderView" type="findVView">
      <label>Rejection Answer Finder</label>
      <outputOverlay>findSql_tfs.rejectionAnswerFinder/ui_UserInterface/frmui_Forms/exov_Result</outputOverlay>
      <searchImmediately>true</searchImmediately>
      <sortListList>
        <sortList type="stitem">
          <path>doc_tfs.rejectionAnswer/leaf_parentQuestionId</path>
        </sortList>
        <sortList type="stitem">
          <path>doc_tfs.rejectionAnswer/leaf_answerId</path>
        </sortList>
      </sortListList>
    </view>
  </viewList>
</findSql>