  </sortList>
  <type>document</type>
  <volatility>1</volatility>
  <searchList>
    <search type="sitem">
      <document>doc_tfs.tfrSearchDoc</document>
      <mappedDocument>doc_tfs.rejectionProcData</mappedDocument>
      <mappedPath>doc_tfs.rejectionProcData/leaf_tfrId</mappedPath>
      <operation>equal</operation>
      <path>doc_tfs.tfrSearchDoc/leaf_tfrId</path>
    </search>
  </searchList>
</findDoc>