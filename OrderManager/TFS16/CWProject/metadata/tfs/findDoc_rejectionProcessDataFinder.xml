<?xml version="1.0" encoding="UTF-8" ?>
<findDoc name="tfs.rejectionProcessDataFinder">
  <extends>find_com.conceptwave.system.Finder</extends>
  <input>doc_tfs.tfrSearchDoc</input>
  <label>Rejection Process Data Finder</label>
  <maxRowsLimit>5000</maxRowsLimit>
  <metaVersion>25</metaVersion>
  <output>doc_tfs.rejectionProcData</output>
  <sortList>
    <sort type="stitem">
      <descending>true</descending>
      <path>doc_tfs.rejectionProcData/leaf_creationDate</path>
