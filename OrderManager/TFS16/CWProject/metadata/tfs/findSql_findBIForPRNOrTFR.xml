<findSql name="tfs.findBIForPRNOrTFR">
  <dbSchema>ORDER</dbSchema>
  <extends>find_com.conceptwave.system.Finder</extends>
  <input>doc_tfs.biSearchDocForPRNOrTFR</input>
  <label>Find Blocking Issue For PRN Or TFR</label>
  <maxRowsLimit>5000</maxRowsLimit>
  <metaVersion>21</metaVersion>
  <output>doc_tfs.STC_BI</output>
  <type>SQL</type>
  <vDetailForm>doc_tfs.STC_BI/ui_UserInterface/frmui_Forms/exov_Default</vDetailForm>
  <volatility>0</volatility>
  <methodList>
    <method name="cwOnFinderSQLSel" type="action">
      <system>true</system>
      <script><![CDATA[
        select {+*+}
          from stc_bi bi
         where 1 = 1
           and bi_id in (select bi_id
                           from stc_bi_relationship r
                          where 1 = 1
                       {[0] and r.source_prn_id = ?}
                       {[1] and r.source_tfr_id = ?})
      ]]></script>
    </method>
    <method name="cwOnFinderSQLGet" type="action">
      <system>true</system>
      <script><![CDATA[
        select {+*+}
          from stc_bi bi
         where bi_id = ?
      ]]></script>
    </method>
  </methodList>
