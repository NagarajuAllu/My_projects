<?xml version="1.0" encoding="UTF-8" ?>
<findSql name="tfs.findAllTFRCustomAttributes">
  <dbSchema>ORDER</dbSchema>
  <extends>find_com.conceptwave.system.Finder</extends>
  <input>doc_tfs.createdCustomAttributeSearchDoc</input>
  <label>Find All TFR Custom Attributes</label>
  <metaVersion>21</metaVersion>
  <output>doc_tfs.customTFRAttribute</output>
  <type>SQL</type>
  <volatility>0</volatility>
  <methodList>
    <method name="cwOnFinderSQLSel" type="action">
      <system>true</system>
      <script><![CDATA[
        select (select tfr_id
                  from stc_tfr
                 where 1=1
                {[0] and tfr_id = ?}) tfr_id,
               ca.customattr_id,
               tfr_ca.attribute_value,
               ca.attribute_name,
               ca.attribute_type,
               ca.parent_element
          from stc_customattr ca,
               stc_tfr_customattr tfr_ca
         where ca.parent_element = 'TFR'
           and tfr_ca.customattr_id (+) = ca.customattr_id
        {[0] and tfr_ca.tfr_id (+) = ?}
      ]]></script>
    </method>
  </methodList>
  <UserInterface name="UserInterface" type="ui">
    <extends>ui_com.conceptwave.system.FinderUserInterface</extends>
    <variableList>
      <variable name="result" type="uivar">
        <flags enum-type="metadataFlag">ARRAY</flags>
        <valueType>doc_tfs.customTFRAttribute</valueType>
      </variable>
      <variable name="search" type="uivar">
        <valueType>doc_tfs.createdCustomAttributeSearchDoc/ui_UserInterface</valueType>
      </variable>
      <variable name="detail" type="uivar">
        <flags enum-type="metadataFlag">VALIDATE</flags>
        <valueType>doc_tfs.customTFRAttribute/ui_UserInterface</valueType>
      </variable>
    </variableList>
    <vformList>
      <vform name="Forms" type="frmui">
        <extends>ui_com.conceptwave.system.FinderUserInterface/frmui_Forms</extends>
        <overlayList>
          <overlay name="Result" type="exov">
            <base>ui_com.conceptwave.system.FinderUserInterface/frmui_Forms/vform_Result</base>
            <extends>ui_com.conceptwave.system.FinderUserInterface/frmui_Forms/vform_Result</extends>
            <label>Result</label>
            <overrideList>
              <override name="table" type="elvlt">
                <height>100%</height>
                <width>100%</width>
                <elementList>
                  <element name="findAllTFRCustomAttributesResultTable" type="eltabl">
                    <onSelectChange type="varPath">
                      <variablePathList>
                        <variablePath>ui_com.conceptwave.system.MasterDetailUserInterface/action_OnSelChanged</variablePath>
                      </variablePathList>
                    </onSelectChange>
                    <variable type="varPath">
                      <variablePathList>
                        <variablePath>findSql_tfs.findAllTFRCustomAttributes/ui_UserInterface/uivar_result</variablePath>
                      </variablePathList>
                    </variable>
                    <elementList>
                      <element name="tfrId" type="elfld">
                        <variable type="varPath">
                          <variablePathList>
                            <variablePath>findSql_tfs.findAllTFRCustomAttributes/ui_UserInterface/uivar_result</variablePath>
                            <variablePath>doc_tfs.customTFRAttribute/leaf_tfrId</variablePath>
                          </variablePathList>
                        </variable>
                      </element>
                      <element name="customAttributeId" type="elfld">
                        <variable type="varPath">
                          <variablePathList>
                            <variablePath>findSql_tfs.findAllTFRCustomAttributes/ui_UserInterface/uivar_result</variablePath>
                            <variablePath>doc_tfs.customTFRAttribute/leaf_customAttributeId</variablePath>
                          </variablePathList>
                        </variable>
                      </element>
                      <element name="attributeValue" type="elfld">
                        <variable type="varPath">
                          <variablePathList>
                            <variablePath>findSql_tfs.findAllTFRCustomAttributes/ui_UserInterface/uivar_result</variablePath>
                            <variablePath>doc_tfs.customTFRAttribute/leaf_attributeValue</variablePath>
                          </variablePathList>
                        </variable>
                      </element>
                      <element name="attributeName" type="elfld">
                        <variable type="varPath">
                          <variablePathList>
                            <variablePath>findSql_tfs.findAllTFRCustomAttributes/ui_UserInterface/uivar_result</variablePath>
                            <variablePath>doc_tfs.customTFRAttribute/leaf_attributeName</variablePath>
                          </variablePathList>
                        </variable>
                      </element>
                      <element name="customAttrType" type="elslct">
                        <variable type="varPath">
                          <variablePathList>
                            <variablePath>findSql_tfs.findAllTFRCustomAttributes/ui_UserInterface/uivar_result</variablePath>
                            <variablePath>doc_tfs.customTFRAttribute/leaf_customAttrType</variablePath>
                          </variablePathList>
                        </variable>
                      </element>
                      <element name="objectType" type="elslct">
                        <variable type="varPath">
                          <variablePathList>
                            <variablePath>findSql_tfs.findAllTFRCustomAttributes/ui_UserInterface/uivar_result</variablePath>
                            <variablePath>doc_tfs.customTFRAttribute/leaf_objectType</variablePath>
                          </variablePathList>
                        </variable>
                      </element>
                    </elementList>
                  </element>
                </elementList>
              </override>
            </overrideList>
          </overlay>
          <overlay name="Default" type="exov">
            <base>ui_com.conceptwave.system.FinderUserInterface/frmui_Forms/exov_Default</base>
            <extends>ui_com.conceptwave.system.FinderUserInterface/frmui_Forms/exov_Default</extends>
            <label>Default</label>
            <overrideList>
              <override name="detailSection" type="elsect">
                <formReference type="varPath">
                  <variablePathList>
                    <variablePath>ui_com.conceptwave.system.MasterDetailUserInterface/uivar_detailForm</variablePath>
                  </variablePathList>
                </formReference>
                <variable type="varPath">
                  <variablePathList>
                    <variablePath>findSql_tfs.findAllTFRCustomAttributes/ui_UserInterface/uivar_detail</variablePath>
                  </variablePathList>
                </variable>
                <visible type="varPath">
                  <variablePathList>
                    <variablePath>ui_com.conceptwave.system.MasterDetailUserInterface/permMethod_detailVisible</variablePath>
                  </variablePathList>
                </visible>
              </override>
              <override name="searchFormSection" type="elsect">
                <formReference type="varPath">
                  <variablePathList>
                    <variablePath>ui_com.conceptwave.system.BaseFinderUserInterface/uivar_searchForm</variablePath>
                  </variablePathList>
                </formReference>
                <variable type="varPath">
                  <variablePathList>
                    <variablePath>findSql_tfs.findAllTFRCustomAttributes/ui_UserInterface/uivar_search</variablePath>
                  </variablePathList>
                </variable>
                <visible type="varPath">
                  <variablePathList>
                    <variablePath>ui_com.conceptwave.system.FinderUserInterface/permMethod_showSearchForm</variablePath>
                  </variablePathList>
                </visible>
              </override>
            </overrideList>
          </overlay>
          <overlay name="showInParent" type="vform">
            <label>showInParent</label>
            <root name="table" type="elvlt">
              <height>100%</height>
              <width>100%</width>
              <elementList>
                <element name="findAllPRNCustomAttributesResultTable" type="eltabl">
                  <doubleClickMethod type="varPath">
                    <variablePathList>
                      <variablePath>findSql_tfs.findAllTFRCustomAttributes/ui_UserInterface/usrActionMethod_editCustomAttribute</variablePath>
                    </variablePathList>
                  </doubleClickMethod>
                  <height>100%</height>
                  <onSelectChange type="varPath">
                    <variablePathList>
                      <variablePath>ui_com.conceptwave.system.MasterDetailUserInterface/action_OnSelChanged</variablePath>
                    </variablePathList>
                  </onSelectChange>
                  <variable type="varPath">
                    <variablePathList>
                      <variablePath>findSql_tfs.findAllTFRCustomAttributes/ui_UserInterface/uivar_result</variablePath>
                    </variablePathList>
                  </variable>
                  <width>1024px</width>
                  <elementList>
                    <element name="attributeName" type="elfld">
                      <label>Attribute Name</label>
                      <variable type="varPath">
                        <variablePathList>
                          <variablePath>findSql_tfs.findAllTFRCustomAttributes/ui_UserInterface/uivar_result</variablePath>
                          <variablePath>doc_tfs.customPRNAttribute/leaf_attributeName</variablePath>
                        </variablePathList>
                      </variable>
                      <width>400px</width>
                    </element>
                    <element name="attributeValue" type="elfld">
                      <label>Attribute Value</label>
                      <variable type="varPath">
                        <variablePathList>
                          <variablePath>findSql_tfs.findAllTFRCustomAttributes/ui_UserInterface/uivar_result</variablePath>
                          <variablePath>doc_tfs.customPRNAttribute/leaf_attributeValue</variablePath>
                        </variablePathList>
                      </variable>
                      <width>400px</width>
                    </element>
                    <element name="customAttrType" type="elslct">
                      <label>Custom Attribute Type</label>
                      <variable type="varPath">
                        <variablePathList>
                          <variablePath>findSql_tfs.findAllTFRCustomAttributes/ui_UserInterface/uivar_result</variablePath>
                          <variablePath>doc_tfs.customPRNAttribute/leaf_customAttrType</variablePath>
                        </variablePathList>
                      </variable>
                      <width>200px</width>
                    </element>
                  </elementList>
                </element>
              </elementList>
            </root>
          </overlay>
        </overlayList>
      </vform>
    </vformList>
    <methodList>
      <method name="editCustomAttribute" type="usrActionMethod">
        <dialogObject>ui_com.conceptwave.system.Dialog</dialogObject>
        <showInPopup>true</showInPopup>
        <validate>true</validate>
        <script><![CDATA[
          var selectedElement = this.model.selected[0];

          return selectedElement;
        ]]></script>
      </method>
    </methodList>
  </UserInterface>
  <viewList>
    <view name="findAllTFRCustomAttributesView" type="findVView">
      <outputOverlay>findSql_tfs.findAllTFRCustomAttributes/ui_UserInterface/frmui_Forms/exov_Result</outputOverlay>
    </view>
  </viewList>
</findSql>