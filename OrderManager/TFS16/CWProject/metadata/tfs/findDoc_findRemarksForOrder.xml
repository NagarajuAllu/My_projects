<?xml version="1.0" encoding="UTF-8" ?>
<findDoc name="tfs.findRemarksForOrder">
  <extends>find_com.conceptwave.system.Finder</extends>
  <input>doc_cwt_common.docID</input>
  <label>Find Remarks For Order</label>
  <maxRowsLimit>9999</maxRowsLimit>
  <metaVersion>25</metaVersion>
  <output>doc_tfs.STC_OBJECT_REMARKS</output>
  <sortList>
    <sort type="stitem">
      <descending>true</descending>
      <path>doc_tfs.STC_OBJECT_REMARKS/leaf_cwDocId</path>
    </sort>
    <sort type="stitem">
      <descending>true</descending>
      <path>doc_tfs.STC_OBJECT_REMARKS/leaf_creationDate</path>
    </sort>
  </sortList>
  <type>document</type>
  <vDetailForm>doc_tfs.STC_OBJECT_REMARKS/ui_UserInterface/frmui_Forms/exov_Default</vDetailForm>
  <volatility>1</volatility>
  <searchList>
    <search type="sitem">
      <document>doc_cwt_common.docID</document>
      <mappedDocument>doc_tfs.STC_OBJECT_REMARKS</mappedDocument>
      <mappedPath>doc_tfs.STC_OBJECT_REMARKS/leaf_orderId</mappedPath>
      <operation>equal</operation>
      <path>doc_cwt_common.docID/leaf_cwDocID</path>
    </search>
  </searchList>
  <methodList>
    <method name="addPerm" type="permMethod">
      <privilegesList>
        <privileges type="ppriv">
          <privilege>STC_ConfigPriv</privilege>
        </privileges>
      </privilegesList>
      <privilegesType>NEVER</privilegesType>
      <script>return false;</script>
    </method>
    <method name="editablePerm" type="permMethod">
      <privilegesList>
        <privileges type="ppriv">
          <privilege>STC_ConfigPriv</privilege>
        </privileges>
      </privilegesList>
      <privilegesType>NEVER</privilegesType>
      <script>return false;</script>
    </method>
  </methodList>
  <UserInterface name="UserInterface" type="ui">
    <extends>ui_com.conceptwave.system.FinderUserInterface</extends>
    <variableList>
      <variable name="finder" type="uivar">
        <valueType>ui_com.conceptwave.system.FinderUserInterface</valueType>
      </variable>
      <variable name="result" type="uivar">
        <flags enum-type="metadataFlag">ARRAY</flags>
        <valueType>doc_tfs.STC_OBJECT_REMARKS</valueType>
      </variable>
      <variable name="search" type="uivar">
        <valueType>doc_cwt_common.docID/ui_UserInterface</valueType>
      </variable>
      <variable name="detail" type="uivar">
        <flags enum-type="metadataFlag">VALIDATE</flags>
        <valueType>doc_tfs.STC_OBJECT_REMARKS/ui_UserInterface</valueType>
      </variable>
    </variableList>
    <vformList>
      <vform name="Forms" type="frmui">
        <extends>ui_com.conceptwave.system.FinderUserInterface/frmui_Forms</extends>
        <overlayList>
          <overlay name="Result" type="exov">
            <base>ui_com.conceptwave.system.FinderUserInterface/frmui_Forms/vform_Result</base>
            <extends>ui_com.conceptwave.system.FinderUserInterface/frmui_Forms/vform_Result</extends>
            <label>Result</label>
            <overrideList>
              <override name="table" type="elvlt">
                <height>100%</height>
                <width>100%</width>
                <elementList>
                  <element name="findTFRRemarksResultTable" type="eltabl">
                    <doubleClickMethod type="varPath">
                      <variablePathList>
                        <variablePath>ui_com.conceptwave.system.MasterDetailUserInterface/action_showDetailAction</variablePath>
                      </variablePathList>
                    </doubleClickMethod>
                    <height>80%</height>
                    <onSelectChange type="varPath">
                      <variablePathList>
                        <variablePath>ui_com.conceptwave.system.MasterDetailUserInterface/action_OnSelChanged</variablePath>
                      </variablePathList>
                    </onSelectChange>
                    <variable type="varPath">
                      <variablePathList>
                        <variablePath>findDoc_tfs.findRemarksForOrder/ui_UserInterface/uivar_result</variablePath>
                      </variablePathList>
                    </variable>
                    <elementList>
                      <element name="objectType" type="elfld">
                        <variable type="varPath">
                          <variablePathList>
                            <variablePath>findDoc_tfs.findRemarksForOrder/ui_UserInterface/uivar_result</variablePath>
                            <variablePath>doc_tfs.STC_OBJECT_REMARKS/leaf_objectType</variablePath>
                          </variablePathList>
                        </variable>
                        <width>130px</width>
                      </element>
                      <element name="objectName" type="elfld">
                        <label>Object Name</label>
                        <variable type="varPath">
                          <variablePathList>
                            <variablePath>findDoc_tfs.findRemarksForOrder/ui_UserInterface/uivar_result</variablePath>
                            <variablePath>doc_tfs.STC_OBJECT_REMARKS/leaf_objectName</variablePath>
                          </variablePathList>
                        </variable>
                        <width>150px</width>
                      </element>
                      <element name="creationDate" type="eldatetime">
                        <label>Creation Date</label>
                        <variable type="varPath">
                          <variablePathList>
                            <variablePath>findDoc_tfs.findRemarksForOrder/ui_UserInterface/uivar_result</variablePath>
                            <variablePath>doc_tfs.STC_OBJECT_REMARKS/leaf_creationDate</variablePath>
                          </variablePathList>
                        </variable>
                        <width>150px</width>
                      </element>
                      <element name="userId" type="elfld">
                        <label>User Id</label>
                        <variable type="varPath">
                          <variablePathList>
                            <variablePath>findDoc_tfs.findRemarksForOrder/ui_UserInterface/uivar_result</variablePath>
                            <variablePath>doc_tfs.STC_OBJECT_REMARKS/leaf_userId</variablePath>
                          </variablePathList>
                        </variable>
                        <width>150px</width>
                      </element>
                      <element name="remarks" type="elfld">
                        <label>Remarks</label>
                        <variable type="varPath">
                          <variablePathList>
                            <variablePath>findDoc_tfs.findRemarksForOrder/ui_UserInterface/uivar_result</variablePath>
                            <variablePath>doc_tfs.STC_OBJECT_REMARKS/leaf_remarks</variablePath>
                          </variablePathList>
                        </variable>
                        <width>650px</width>
                      </element>
                    </elementList>
                  </element>
                </elementList>
              </override>
            </overrideList>
          </overlay>
          <overlay name="Default" type="exov">
            <base>ui_com.conceptwave.system.FinderUserInterface/frmui_Forms/exov_Default</base>
            <extends>ui_com.conceptwave.system.FinderUserInterface/frmui_Forms/exov_Default</extends>
            <label>Default</label>
            <overrideList>
              <override name="detailSection" type="elsect">
                <formReference type="varPath">
                  <variablePathList>
                    <variablePath>ui_com.conceptwave.system.MasterDetailUserInterface/uivar_detailForm</variablePath>
                  </variablePathList>
                </formReference>
                <variable type="varPath">
                  <variablePathList>
                    <variablePath>findDoc_tfs.findRemarksForOrder/ui_UserInterface/uivar_detail</variablePath>
                  </variablePathList>
                </variable>
                <visible type="varPath">
                  <variablePathList>
                    <variablePath>ui_com.conceptwave.system.MasterDetailUserInterface/permMethod_detailVisible</variablePath>
                  </variablePathList>
                </visible>
              </override>
              <override name="searchFormSection" type="elsect">
                <formReference type="varPath">
                  <variablePathList>
                    <variablePath>ui_com.conceptwave.system.BaseFinderUserInterface/uivar_searchForm</variablePath>
                  </variablePathList>
                </formReference>
                <variable type="varPath">
                  <variablePathList>
                    <variablePath>findDoc_tfs.findRemarksForOrder/ui_UserInterface/uivar_search</variablePath>
                  </variablePathList>
                </variable>
                <visible type="varPath">
                  <variablePathList>
                    <variablePath>ui_com.conceptwave.system.FinderUserInterface/permMethod_showSearchForm</variablePath>
                  </variablePathList>
                </visible>
              </override>
            </overrideList>
          </overlay>
          <overlay name="Menu" type="exov">
            <base>ui_com.conceptwave.system.BaseFinderUserInterface/frmui_Forms/exov_Menu</base>
            <extends>ui_com.conceptwave.system.BaseFinderUserInterface/frmui_Forms/exov_Menu</extends>
            <label>Menu</label>
            <overrideList>
              <override name="cwfAdd" type="eldel"/>
              <override name="cwfCopy" type="eldel"/>
              <override name="cwfDelete" type="eldel"/>
              <override name="cwfUpdate" type="eldel"/>
            </overrideList>
          </overlay>
        </overlayList>
      </vform>
    </vformList>
    <methodList>
      <method name="canDelete" type="permMethod">
        <privilegesList>
          <privileges type="ppriv">
            <privilege>STC_ConfigPriv</privilege>
          </privileges>
        </privilegesList>
        <privilegesType>NEVER</privilegesType>
        <script>return $psCondition;</script>
      </method>
    </methodList>
  </UserInterface>
  <viewList>
    <view name="findRemarksView" type="findVView">
      <label>View All Remarks for the order</label>
      <outputOverlay>findDoc_tfs.findRemarksForOrder/ui_UserInterface/frmui_Forms/exov_Result</outputOverlay>
    </view>
  </viewList>
</findDoc>