<?xml version="1.0" encoding="UTF-8" ?>
<findSql name="tfs.rejectionRootQuestionsFinder">
  <dbSchema>ORDER</dbSchema>
  <extends>find_com.conceptwave.system.Finder</extends>
  <label>Rejection RootQuestion Finder</label>
  <metaVersion>21</metaVersion>
  <output>doc_tfs.rejectionQuestion</output>
  <type>SQL</type>
  <volatility>10</volatility>
  <methodList>
    <method name="cwOnFinderSQLSel" type="action">
      <system>true</system>
      <script><![CDATA[
        select {+*+}
          from stc_reject_question
         where parent_answer_id is null
      ]]></script>
    </method>
  </methodList>
  <UserInterface name="UserInterface" type="ui">
    <extends>ui_com.conceptwave.system.FinderUserInterface</extends>
    <variableList>
      <variable name="result" type="uivar">
        <flags enum-type="metadataFlag">ARRAY</flags>
        <valueType>doc_tfs.rejectionQuestion</valueType>
      </variable>
      <variable name="detail" type="uivar">
        <flags enum-type="metadataFlag">VALIDATE</flags>
        <valueType>doc_tfs.rejectionQuestion/ui_UserInterface</valueType>
      </variable>
    </variableList>
    <vformList>
      <vform name="Forms" type="frmui">
        <extends>ui_com.conceptwave.system.FinderUserInterface/frmui_Forms</extends>
        <overlayList>
          <overlay name="Result" type="exov">
            <base>ui_com.conceptwave.system.FinderUserInterface/frmui_Forms/vform_Result</base>
            <extends>ui_com.conceptwave.system.FinderUserInterface/frmui_Forms/vform_Result</extends>
            <label>Result</label>
            <overrideList>
              <override name="table" type="elvlt">
                <height>100%</height>
                <width>100%</width>
                <elementList>
                  <element name="rejectionRootQuestionsFinderResultTable" type="eltabl">
                    <onSelectChange type="varPath">
                      <variablePathList>
                        <variablePath>ui_com.conceptwave.system.MasterDetailUserInterface/action_OnSelChanged</variablePath>
                      </variablePathList>
                    </onSelectChange>
                    <variable type="varPath">
                      <variablePathList>
                        <variablePath>findSql_tfs.rejectionRootQuestionsFinder/ui_UserInterface/uivar_result</variablePath>
                      </variablePathList>
                    </variable>
                    <elementList>
                      <element name="questionId" type="elfld">
                        <variable type="varPath">
                          <variablePathList>
                            <variablePath>findSql_tfs.rejectionRootQuestionsFinder/ui_UserInterface/uivar_result</variablePath>
                            <variablePath>doc_tfs.rejectionQuestion/leaf_questionId</variablePath>
                          </variablePathList>
                        </variable>
                      </element>
                      <element name="parentAnswerId" type="elref">
                        <variable type="varPath">
                          <variablePathList>
                            <variablePath>findSql_tfs.rejectionRootQuestionsFinder/ui_UserInterface/uivar_result</variablePath>
                            <variablePath>doc_tfs.rejectionQuestion/leaf_parentAnswerId</variablePath>
                          </variablePathList>
                        </variable>
                      </element>
                      <element name="textQuestion" type="eltxtArea">
                        <variable type="varPath">
                          <variablePathList>
                            <variablePath>findSql_tfs.rejectionRootQuestionsFinder/ui_UserInterface/uivar_result</variablePath>
                            <variablePath>doc_tfs.rejectionQuestion/leaf_textQuestion</variablePath>
                          </variablePathList>
                        </variable>
                      </element>
                    </elementList>
                  </element>
                </elementList>
              </override>
            </overrideList>
          </overlay>
          <overlay name="Default" type="exov">
            <base>ui_com.conceptwave.system.FinderUserInterface/frmui_Forms/exov_Default</base>
            <extends>ui_com.conceptwave.system.FinderUserInterface/frmui_Forms/exov_Default</extends>
