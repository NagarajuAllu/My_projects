  <dbSchema>INTERFACE</dbSchema>
  <extends>find_com.conceptwave.system.Finder</extends>
  <input>doc_tfsReport.prnr_prnSummaryReportSearchDoc</input>
  <label>PRNR/PRN Summary Report Finder</label>
  <maxRowsLimit>9999</maxRowsLimit>
  <metaVersion>25</metaVersion>
  <output>doc_tfsReport.prnr_prnSummaryReportResultDoc</output>
  <rtConfigurable>true</rtConfigurable>
  <type>SQL</type>
  <methodList>
    <method name="cwOnFinderSQLSel" type="action">
      <system>true</system>
      <script><![CDATA[
        select v.prnr_id,
               v.prnr_name,
               v.prnr_status,
               v.customer_name,
               v.customer_project,
               v.customer_project_phase,
               v.prnr_creation_date,
               v.prnr_current_userid,
               v.prnr_created_by,
               v.prn_id,
               v.prn_name,
               v.prn_status,
               v.planner,
               v.analyzed_by,
               v.analysis_date,
               v.prn_released_date,
               v.prn_creation_date,
               v.prn_current_userid
          from stc_prnr_prn_sum_report_v v
         where 1 = 1
           {[0] and v.customer_name = ?}
           {[1] and v.prnr_released_date >= ?}
           {[2] and v.prnr_released_date <= ?}
           {[3] and v.prn_creation_date >= ?}
           {[4] and v.prn_creation_date <= ?}
      ]]></script>
    </method>
  </methodList>
