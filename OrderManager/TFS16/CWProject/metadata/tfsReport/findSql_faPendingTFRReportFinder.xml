  <methodList>
    <method name="cwOnFinderSQLSel" type="action">
      <system>true</system>
      <script><![CDATA[
        select v.user_id,
               v.roleid,
               v.cwdocid,
               v.operation_name,
               v.performed_task_name,
               v.assign_to_user_date,
               v.completion_date,
               v.duration_in_hours,
               v.duration_in_days,
               v.tfr_id,
               v.tfr_number,
               v.tfr_status,
               v.ni_number,
               v.requirement_type,
               v.required,
               v.project,
               v.pending_reason,
               v.current_userid current_userid,
               v.current_department
          from stc_fa_pending_tfr_report_v v
         where 1 = 1
           {[0] and v.roleid = ?}
           {[1] and v.tfr_type = ?}
           {[2] and v.assign_to_user_date >= ?}
           {[3] and v.assign_to_user_date <= ?}
      ]]></script>
    </method>
