    <method name="cwOnFinderSQLSel" type="action">
      <system>true</system>
      <script><![CDATA[
        select v.prnr_id,
               v.prnr_name,
               v.prnr_status,
               v.customer_name,
               v.customer_project,
               v.customer_project_phase,
               v.prnr_creation_date,
               v.site_a_name,
               v.site_z_name,
               v.gsm_cell_id,
               v.gsm_cell_vendor,
               v.prn_id,
               v.prn_name,
               v.prn_status,
               v.requirement_type,
               v.quantity,
               v.plan_type,
               v.issue,
               v.planner,
               v.site_a_mpls_device,
               v.site_z_mpls_device,
               v.pe_agg_slot,
               v.pe_agg_port,
               v.pe_agg_odf,
               v.tfr_id,
               v.tfr_number,
               v.tfr_status,
               v.dept_created_by
          from stc_tx_summary_report_v v
         where 1 = 1
           {[0] and v.customer_name = ?}
           {[1] and v.customer_project = ?}
           {[2] and v.customer_project_phase = ?}
           {[3] and v.site_a_name = ?}
           {[4] and v.site_z_name = ?}
           {[5] and v.plan_type = ?}
           {[6] and v.issue = ?}
           {[7] and v.fa_completion_date >= ?}
           {[8] and v.fa_completion_date <= ?}
           {[9] and v.prnr_creation_date >= ?}
           {[10] and v.prnr_creation_date <= ?}
      ]]></script>
    </method>
