  <methodList>
    <method name="cwOnFinderSQLSel" type="action">
      <system>true</system>
      <script><![CDATA[
        select v.prn_name,
               v.customer_name,
               v.customer_project,
               v.requirement_type,
               v.quantity,
               v.site_a_name,
               v.site_a_if,
               v.site_z_name,
               v.site_z_if,
               v.traffic_type,
               v.remarks,
               v.tfr_id,
               v.tfr_number,
               v.project,
               v.bi_id,
               v.bi_status,
               v.ring_name,
               v.site_name,
               v.network_type,
               v.details,
               v.category,
               v.mop_number,
               v.mop1_status,
               v.mop2_status,
               v.mop3_status,
               v.mop_issue_date,
               v.mop_impl_status,
               v.mop_impl_compl_date,
               v.mop_impl_estim_date,
               v.contractor1,
               v.contractor2,
               v.contractor3,
               v.last_fwd_dept,
               v.last_fwd_eng,
               v.creation_date,
               v.sow_number,
               v.sow1_status,
               v.sow2_status,
               v.sow3_status,
               v.sow_issue_date
          from stc_bi_summary_report_v v
         where 1 = 1
           {[0] and v.creation_date >= ?}
           {[1] and v.creation_date <= ?}
      ]]></script>
    </method>
