<?xml version="1.0" encoding="UTF-8" ?>
<findSql name="tfsReport.advancedReportFinder">
  <dbSchema>INTERFACE</dbSchema>
  <extends>find_com.conceptwave.system.Finder</extends>
  <input>doc_tfsReport.advancedReportSearchDoc</input>
  <label>Advanced Report</label>
  <maxRowsLimit>9999</maxRowsLimit>
  <metaVersion>25</metaVersion>
  <output>doc_tfsReport.advancedReportResultDoc</output>
  <rtConfigurable>true</rtConfigurable>
  <type>SQL</type>
  <methodList>
    <method name="cwOnFinderSQLSel" type="action">
      <system>true</system>
      <script><![CDATA[
        select v.tfr_id,
               v.tfr_number,
               v.ni_number,
               v.prn_id,
               v.prn_name,
               v.prn_status,
               v.bi_id,
               v.bi_status,
               v.icms_number,
               v.district,
               v.region,
               v.priority,
               v.project,
               v.tfr_type,
               v.tfr_status,
               v.current_userid,
               v.mapping_status,
               v.dept_created_by,
               v.updatedBy,
               v.requirement_type,
               v.site_a_name,
               v.site_z_name,
               v.site_a_jvc,
               v.site_z_jvc,
               v.site_a_exchangetype,
               v.site_z_exchangetype,
               v.cwo_type,
               v.bu_requester,
               v.reason_for_issue,
               v.hajj_committment,
               v.group_requester,
               v.customer_name,
               v.site_a_if,
               v.site_z_if,
               v.existing,
               v.required,
               v.total,
               v.order_number,
               v.circuit_number,
               v.creation_date,
               v.planned_rfs_date,
               v.customer_expd_date,
               v.fa_completion_date,
               v.tfr_remarks
          from stc_advanced_report_v v
         where 1 = 1
           {[0] and v.tfr_number = ?}
           {[1] and v.ni_number = ?}
           {[2] and v.prn_name = ?}
           {[3] and v.bi_id = ?}
           {[4] and v.icms_number = ?}
           {[5] and v.district = ?}
           {[6] and v.region = ?}
           {[7] and v.priority = ?}
           {[8] and v.project = ?}
           {[9] and v.tfr_type = ?}
           {[10] and v.tfr_status = ?}
           {[11] and v.current_userid = ?}
           {[12] and v.mapping_status = ?}
           {[13] and v.dept_created_by = ?}
           {[14] and v.updatedBy = ?}
           {[15] and v.requirement_type = ?}
           {[16] and v.site_a_name = ?}
           {[17] and v.site_z_name = ?}
           {[18] and v.site_a_jvc = ?}
           {[19] and v.site_z_jvc = ?}
           {[20] and v.site_a_exchangetype = ?}
           {[21] and v.site_z_exchangetype = ?}
           {[22] and v.cwo_type = ?}
           {[23] and v.bu_requester = ?}
           {[24] and v.reason_for_issue = ?}
           {[25] and v.hajj_committment = ?}
           {[26] and v.group_requester = ?}
           {[27] and v.customer_name = ?}
           {[28] and v.site_a_if = ?}
           {[29] and v.site_z_if = ?}
           {[30] and v.existing = ?}
           {[31] and v.required = ?}
           {[32] and v.total = ?}
           {[33] and v.creation_date >= ?}
           {[34] and v.creation_date <= ?}
           {[35] and v.planned_rfs_date >= ?}
           {[36] and v.planned_rfs_date <= ?}
           {[37] and v.customer_expd_date >= ?}
           {[38] and v.customer_expd_date <= ?}
           {[39] and v.fa_completion_date >= ?}
           {[40] and v.fa_completion_date <= ?}
           {[41] and v.tfr_remarks = ?}
      ]]></script>
    </method>
  </methodList>
