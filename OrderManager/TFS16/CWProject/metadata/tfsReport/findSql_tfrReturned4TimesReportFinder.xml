<?xml version="1.0" encoding="UTF-8" ?>
<findSql name="tfsReport.tfrReturned4TimesReportFinder">
  <dbSchema>INTERFACE</dbSchema>
  <extends>find_com.conceptwave.system.Finder</extends>
  <label>TFR Returned 4 Times Report Finder</label>
  <maxRowsLimit>9999</maxRowsLimit>
  <metaVersion>25</metaVersion>
  <output>doc_tfsReport.tfrReturned4TimesReportResultDoc</output>
  <rtConfigurable>true</rtConfigurable>
  <type>SQL</type>
  <methodList>
    <method name="cwOnFinderSQLSel" type="action">
      <system>true</system>
      <script><![CDATA[
        select v.tfr_id,
               v.tfr_number,
               v.tfr_status,
               v.ni_number,
               v.requirement_type,
               v.total,
               v.current_userid,
               v.current_department,
               v.recno,
               v.audit_trail_remarks,
               v.audit_trail_who,
               v.audit_trail_when
          from stc_tfr_ret_4_times_report_v v
      ]]></script>
    </method>
  </methodList>
  <UserInterface name="UserInterface" type="ui">
    <extends>ui_com.conceptwave.system.FinderUserInterface</extends>
    <variableList>
      <variable name="finder" type="uivar">
        <valueType>ui_com.conceptwave.system.FinderUserInterface</valueType>
      </variable>
      <variable name="result" type="uivar">
        <flags enum-type="metadataFlag">ARRAY</flags>
        <valueType>doc_tfsReport.tfrReturned4TimesReportResultDoc</valueType>
      </variable>
      <variable name="detail" type="uivar">
        <flags enum-type="metadataFlag">VALIDATE</flags>
        <valueType>doc_tfsReport.tfrReturned4TimesReportResultDoc/ui_UserInterface</valueType>
      </variable>
    </variableList>
    <vformList>
      <vform name="Forms" type="frmui">
        <extends>ui_com.conceptwave.system.FinderUserInterface/frmui_Forms</extends>
        <overlayList>
          <overlay name="Result" type="exov">
            <base>ui_com.conceptwave.system.FinderUserInterface/frmui_Forms/vform_Result</base>
            <extends>ui_com.conceptwave.system.FinderUserInterface/frmui_Forms/vform_Result</extends>
            <label>Result</label>
            <overrideList>
              <override name="table" type="elvlt">
                <height>100%</height>
                <width>100%</width>
                <elementList>
                  <element name="tfrReturned4TimesReportFinderResultTable" type="eltabl">
                    <onSelectChange type="varPath">
                      <variablePathList>
                        <variablePath>ui_com.conceptwave.system.MasterDetailUserInterface/action_OnSelChanged</variablePath>
                      </variablePathList>
                    </onSelectChange>
                    <variable type="varPath">
                      <variablePathList>
                        <variablePath>findSql_tfsReport.tfrReturned4TimesReportFinder/ui_UserInterface/uivar_result</variablePath>
                      </variablePathList>
                    </variable>
                    <elementList>
                      <element name="tfrNumber" type="elfld">
                        <label>TFR Number</label>
                        <variable type="varPath">
                          <variablePathList>
                            <variablePath>findSql_tfsReport.tfrReturned4TimesReportFinder/ui_UserInterface/uivar_result</variablePath>
                            <variablePath>doc_tfsReport.tfrReturned4TimesReportResultDoc/leaf_tfrNumber</variablePath>
                          </variablePathList>
                        </variable>
                        <width>150px</width>
                      </element>
                      <element name="tfrStatus" type="elfld">
                        <label>TFR Status</label>
                        <variable type="varPath">
                          <variablePathList>
                            <variablePath>findSql_tfsReport.tfrReturned4TimesReportFinder/ui_UserInterface/uivar_result</variablePath>
                            <variablePath>doc_tfsReport.tfrReturned4TimesReportResultDoc/leaf_tfrStatus</variablePath>
                          </variablePathList>
                        </variable>
                        <width>200px</width>
                      </element>
                      <element name="niNumber" type="elfld">
                        <label>NI Number</label>
                        <variable type="varPath">
                          <variablePathList>
                            <variablePath>findSql_tfsReport.tfrReturned4TimesReportFinder/ui_UserInterface/uivar_result</variablePath>
                            <variablePath>doc_tfsReport.tfrReturned4TimesReportResultDoc/leaf_niNumber</variablePath>
                          </variablePathList>
                        </variable>
                        <width>150px</width>
                      </element>
                      <element name="requirementType" type="elfld">
                        <label>Requirement Type</label>
                        <variable type="varPath">
                          <variablePathList>
                            <variablePath>findSql_tfsReport.tfrReturned4TimesReportFinder/ui_UserInterface/uivar_result</variablePath>
                            <variablePath>doc_tfsReport.tfrReturned4TimesReportResultDoc/leaf_requirementType</variablePath>
                          </variablePathList>
                        </variable>
                        <width>150px</width>
                      </element>
                      <element name="total" type="elfld">
                        <label>Total</label>
                        <variable type="varPath">
                          <variablePathList>
                            <variablePath>findSql_tfsReport.tfrReturned4TimesReportFinder/ui_UserInterface/uivar_result</variablePath>
                            <variablePath>doc_tfsReport.tfrReturned4TimesReportResultDoc/leaf_total</variablePath>
                          </variablePathList>
                        </variable>
                        <width>100px</width>
                      </element>
                      <element name="currentUserId" type="elref">
                        <label>Current User</label>
                        <variable type="varPath">
                          <variablePathList>
                            <variablePath>findSql_tfsReport.tfrReturned4TimesReportFinder/ui_UserInterface/uivar_result</variablePath>
                            <variablePath>doc_tfsReport.tfrReturned4TimesReportResultDoc/leaf_currentUserId</variablePath>
                          </variablePathList>
                        </variable>
                        <width>150px</width>
                      </element>
                      <element name="currentDepartment" type="elref">
                        <label>Current Department</label>
                        <variable type="varPath">
                          <variablePathList>
                            <variablePath>findSql_tfsReport.tfrReturned4TimesReportFinder/ui_UserInterface/uivar_result</variablePath>
                            <variablePath>doc_tfsReport.tfrReturned4TimesReportResultDoc/leaf_currentDepartment</variablePath>
                          </variablePathList>
                        </variable>
                        <width>250px</width>
                      </element>
                      <element name="remarks_auditTrail" type="elfld">
                        <label>Remarks - from Audit Trail</label>
                        <variable type="varPath">
                          <variablePathList>
                            <variablePath>findSql_tfsReport.tfrReturned4TimesReportFinder/ui_UserInterface/uivar_result</variablePath>
                            <variablePath>doc_tfsReport.tfrReturned4TimesReportResultDoc/leaf_remarks_auditTrail</variablePath>
                          </variablePathList>
                        </variable>
                        <width>250px</width>
                      </element>
                      <element name="remarksBy_auditTrail" type="elref">
                        <label>Remarks By - from Audit Trail</label>
                        <variable type="varPath">
                          <variablePathList>
                            <variablePath>findSql_tfsReport.tfrReturned4TimesReportFinder/ui_UserInterface/uivar_result</variablePath>
                            <variablePath>doc_tfsReport.tfrReturned4TimesReportResultDoc/leaf_remarksBy_auditTrail</variablePath>
                          </variablePathList>
                        </variable>
                        <width>200px</width>
                      </element>
                      <element name="remarksDate_auditTrail" type="eldate">
                        <label>Remarks Date - from Audit Trail</label>
                        <variable type="varPath">
                          <variablePathList>
                            <variablePath>findSql_tfsReport.tfrReturned4TimesReportFinder/ui_UserInterface/uivar_result</variablePath>
                            <variablePath>doc_tfsReport.tfrReturned4TimesReportResultDoc/leaf_remarksDate_auditTrail</variablePath>
                          </variablePathList>
                        </variable>
                        <width>200px</width>
                      </element>
                    </elementList>
                  </element>
                </elementList>
              </override>
            </overrideList>
          </overlay>
          <overlay name="Default" type="exov">
            <base>ui_com.conceptwave.system.FinderUserInterface/frmui_Forms/exov_Default</base>
            <extends>ui_com.conceptwave.system.FinderUserInterface/frmui_Forms/exov_Default</extends>
            <label>Default</label>
            <overrideList>
              <override name="detailSection" type="elsect">
                <formReference type="varPath">
                  <variablePathList>
                    <variablePath>ui_com.conceptwave.system.MasterDetailUserInterface/uivar_detailForm</variablePath>
                  </variablePathList>
                </formReference>
                <variable type="varPath">
                  <variablePathList>
                    <variablePath>findSql_tfsReport.tfrReturned4TimesReportFinder/ui_UserInterface/uivar_detail</variablePath>
                  </variablePathList>
                </variable>
                <visible type="varPath">
                  <variablePathList>
                    <variablePath>ui_com.conceptwave.system.MasterDetailUserInterface/permMethod_detailVisible</variablePath>
                  </variablePathList>
                </visible>
              </override>
            </overrideList>
          </overlay>
          <overlay name="Menu" type="exov">
            <base>ui_com.conceptwave.system.BaseFinderUserInterface/frmui_Forms/exov_Menu</base>
            <extends>ui_com.conceptwave.system.BaseFinderUserInterface/frmui_Forms/exov_Menu</extends>
            <label>Menu</label>
            <overrideList>
              <override name="cwfAdd" type="eldel"/>
              <override name="cwfCopy" type="eldel"/>
              <override name="cwfDelete" type="eldel"/>
              <override name="cwfDetail" type="eldel"/>
              <override name="cwfUpdate" type="eldel"/>
              <override name="finderMenuLayout" type="elext">
                <elementList>
                  <element name="showTFRMenuItem" type="elmnu">
                    <clickMethod type="varPath">
                      <variablePathList>
                        <variablePath>findSql_tfsReport.tfrReturned4TimesReportFinder/ui_UserInterface/usrActionMethod_showTFR</variablePath>
                      </variablePathList>
                    </clickMethod>
                    <label>Show TFR</label>
                  </element>
                </elementList>
              </override>
            </overrideList>
          </overlay>
        </overlayList>
      </vform>
    </vformList>
    <methodList>
      <method name="showTFR" type="usrActionMethod">
        <dialogHeight>768</dialogHeight>
        <dialogWidth>1024</dialogWidth>
        <parameterList>
          <parameter name="dialog" type="rifp">
            <type>ui_com.conceptwave.system.Dialog</type>
          </parameter>
        </parameterList>
        <showInPopup>true</showInPopup>
        <script><![CDATA[
          if(this.model.selected == null || this.model.selected.length == 0) {
              Global.throwException("UU0115");
          }

          if(this.model.selected.length > 1) {
              Global.throwException("UU0116");
          }

          var tfrFinder = new Finder("tfs.findTFR");
          tfrFinder.searchDocument.tfrId = this.model.selected[0].tfrId;
          var resultList = tfrFinder.search();

          var tfrFound = null;

          if(resultList != null && resultList.length > 0) {
              tfrFound = resultList[0];
          }
          else {
              Global.throwException("AE0008", ["TFR", "TFR Returned 4 Times Report"]);
          }

          return tfrFound;
        ]]></script>
      </method>
    </methodList>
  </UserInterface>
  <viewList>
    <view name="tfrReturned4TimesReportFinderView" type="findVView">
      <label>TFR Returned 4 Times Report</label>
      <outputOverlay>findSql_tfsReport.tfrReturned4TimesReportFinder/ui_UserInterface/frmui_Forms/exov_Result</outputOverlay>
      <searchImmediately>true</searchImmediately>
    </view>
  </viewList>
</findSql>