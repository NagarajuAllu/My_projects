<?xml version="1.0" encoding="UTF-8" ?>
<findSql name="tfsReport.faPendingSummaryReportFinder">
  <dbSchema>INTERFACE</dbSchema>
  <extends>find_com.conceptwave.system.Finder</extends>
  <input>doc_tfsReport.faPendingSummaryReportSearchDoc</input>
  <label>FA Pending Summary Report Finder</label>
  <maxRowsLimit>9999</maxRowsLimit>
  <metaVersion>25</metaVersion>
  <output>doc_tfsReport.faPendingSummaryReportResultDoc</output>
  <rtConfigurable>true</rtConfigurable>
  <type>SQL</type>
  <methodList>
    <method name="cwOnFinderSQLSel" type="action">
      <system>true</system>
      <script><![CDATA[
        select v.tfr_id,
               v.tfr_number,
               v.tfr_status,
               v.ni_number,
               v.creation_date,
               v.fa_completion_date,
               v.cancelled_date,
               v.num_days,
               v.num_weeks,
               v.num_months,
               v.required
          from stc_fa_pending_sum_report_v v
         where 1 = 1
           {[0] and v.creation_date >= ?}
           {[1] and v.creation_date <= ?}
      ]]></script>
    </method>
  </methodList>
  <UserInterface name="UserInterface" type="ui">
    <extends>ui_com.conceptwave.system.FinderUserInterface</extends>
    <variableList>
      <variable name="finder" type="uivar">
        <valueType>ui_com.conceptwave.system.FinderUserInterface</valueType>
      </variable>
      <variable name="search" type="uivar">
        <valueType>doc_tfsReport.faPendingSummaryReportSearchDoc/ui_UserInterface</valueType>
      </variable>
      <variable name="detail" type="uivar">
        <flags enum-type="metadataFlag">VALIDATE</flags>
        <valueType>doc_tfsReport.faPendingSummaryReportResultDoc/ui_UserInterface</valueType>
      </variable>
      <variable name="result" type="uivar">
        <flags enum-type="metadataFlag">ARRAY</flags>
        <valueType>doc_tfsReport.faPendingSummaryReportResultDoc</valueType>
      </variable>
    </variableList>
    <vformList>
      <vform name="Forms" type="frmui">
        <extends>ui_com.conceptwave.system.FinderUserInterface/frmui_Forms</extends>
        <overlayList>
          <overlay name="Default" type="exov">
            <base>ui_com.conceptwave.system.FinderUserInterface/frmui_Forms/exov_Default</base>
            <extends>ui_com.conceptwave.system.FinderUserInterface/frmui_Forms/exov_Default</extends>
            <label>Default</label>
            <overrideList>
              <override name="detailSection" type="elsect">
                <formReference type="varPath">
                  <variablePathList>
                    <variablePath>ui_com.conceptwave.system.MasterDetailUserInterface/uivar_detailForm</variablePath>
                  </variablePathList>
                </formReference>
                <variable type="varPath">
                  <variablePathList>
                    <variablePath>findSql_tfsReport.faPendingSummaryReportFinder/ui_UserInterface/uivar_detail</variablePath>
                  </variablePathList>
                </variable>
                <visible type="varPath">
                  <variablePathList>
                    <variablePath>ui_com.conceptwave.system.MasterDetailUserInterface/permMethod_detailVisible</variablePath>
                  </variablePathList>
                </visible>
              </override>
              <override name="searchFormSection" type="elsect">
                <formReference type="varPath">
                  <variablePathList>
                    <variablePath>ui_com.conceptwave.system.BaseFinderUserInterface/uivar_searchForm</variablePath>
                  </variablePathList>
                </formReference>
                <variable type="varPath">
                  <variablePathList>
                    <variablePath>findSql_tfsReport.faPendingSummaryReportFinder/ui_UserInterface/uivar_search</variablePath>
                  </variablePathList>
                </variable>
                <visible type="varPath">
                  <variablePathList>
                    <variablePath>ui_com.conceptwave.system.FinderUserInterface/permMethod_showSearchForm</variablePath>
                  </variablePathList>
                </visible>
              </override>
            </overrideList>
          </overlay>
          <overlay name="Result" type="exov">
            <base>ui_com.conceptwave.system.FinderUserInterface/frmui_Forms/vform_Result</base>
            <extends>ui_com.conceptwave.system.FinderUserInterface/frmui_Forms/vform_Result</extends>
            <label>Result</label>
            <overrideList>
              <override name="table" type="elvlt">
                <height>100%</height>
                <width>100%</width>
                <elementList>
                  <element name="faPendingSummaryReportFinderResultTable" type="eltabl">
                    <onSelectChange type="varPath">
                      <variablePathList>
                        <variablePath>ui_com.conceptwave.system.MasterDetailUserInterface/action_OnSelChanged</variablePath>
                      </variablePathList>
                    </onSelectChange>
                    <variable type="varPath">
                      <variablePathList>
                        <variablePath>findSql_tfsReport.faPendingSummaryReportFinder/ui_UserInterface/uivar_result</variablePath>
                      </variablePathList>
                    </variable>
                    <elementList>
                      <element name="tfrNumber" type="elfld">
                        <label>TFR Number</label>
                        <variable type="varPath">
                          <variablePathList>
                            <variablePath>findSql_tfsReport.faPendingSummaryReportFinder/ui_UserInterface/uivar_result</variablePath>
                            <variablePath>doc_tfsReport.faPendingSummaryReportResultDoc/leaf_tfrNumber</variablePath>
                          </variablePathList>
                        </variable>
                        <width>150px</width>
                      </element>
                      <element name="tfrStatus" type="elfld">
                        <label>TFR Status</label>
                        <variable type="varPath">
                          <variablePathList>
                            <variablePath>findSql_tfsReport.faPendingSummaryReportFinder/ui_UserInterface/uivar_result</variablePath>
                            <variablePath>doc_tfsReport.faPendingSummaryReportResultDoc/leaf_tfrStatus</variablePath>
                          </variablePathList>
                        </variable>
                        <width>200px</width>
                      </element>
                      <element name="niNumber" type="elfld">
                        <label>NI Number</label>
                        <variable type="varPath">
                          <variablePathList>
                            <variablePath>findSql_tfsReport.faPendingSummaryReportFinder/ui_UserInterface/uivar_result</variablePath>
                            <variablePath>doc_tfsReport.faPendingSummaryReportResultDoc/leaf_niNumber</variablePath>
                          </variablePathList>
                        </variable>
                        <width>150px</width>
                      </element>
                      <element name="creationDate" type="eldatetime">
                        <label>TFR Creation Date</label>
                        <variable type="varPath">
                          <variablePathList>
                            <variablePath>findSql_tfsReport.faPendingSummaryReportFinder/ui_UserInterface/uivar_result</variablePath>
                            <variablePath>doc_tfsReport.faPendingSummaryReportResultDoc/leaf_creationDate</variablePath>
                          </variablePathList>
                        </variable>
                        <width>200px</width>
                      </element>
                      <element name="faCompletionDate" type="eldatetime">
                        <label>FA Completion Date</label>
                        <variable type="varPath">
                          <variablePathList>
                            <variablePath>findSql_tfsReport.faPendingSummaryReportFinder/ui_UserInterface/uivar_result</variablePath>
                            <variablePath>doc_tfsReport.faPendingSummaryReportResultDoc/leaf_faCompletionDate</variablePath>
                          </variablePathList>
                        </variable>
                        <width>200px</width>
                      </element>
                      <element name="cancelledDate" type="eldatetime">
                        <label>Cancelled Date</label>
                        <variable type="varPath">
                          <variablePathList>
                            <variablePath>findSql_tfsReport.faPendingSummaryReportFinder/ui_UserInterface/uivar_result</variablePath>
                            <variablePath>doc_tfsReport.faPendingSummaryReportResultDoc/leaf_cancelledDate</variablePath>
                          </variablePathList>
                        </variable>
                        <width>200px</width>
                      </element>
                      <element name="numberOfDays" type="elfld">
                        <label>Number of days</label>
                        <variable type="varPath">
                          <variablePathList>
                            <variablePath>findSql_tfsReport.faPendingSummaryReportFinder/ui_UserInterface/uivar_result</variablePath>
                            <variablePath>doc_tfsReport.faPendingSummaryReportResultDoc/leaf_numberOfDays</variablePath>
                          </variablePathList>
                        </variable>
                        <width>150px</width>
                      </element>
                      <element name="numberOfWeeks" type="elfld">
                        <label>Number of weeks</label>
                        <variable type="varPath">
                          <variablePathList>
                            <variablePath>findSql_tfsReport.faPendingSummaryReportFinder/ui_UserInterface/uivar_result</variablePath>
                            <variablePath>doc_tfsReport.faPendingSummaryReportResultDoc/leaf_numberOfWeeks</variablePath>
                          </variablePathList>
                        </variable>
                        <width>150px</width>
                      </element>
                      <element name="numberOfMonths" type="elfld">
                        <label>Number of months</label>
                        <variable type="varPath">
                          <variablePathList>
                            <variablePath>findSql_tfsReport.faPendingSummaryReportFinder/ui_UserInterface/uivar_result</variablePath>
                            <variablePath>doc_tfsReport.faPendingSummaryReportResultDoc/leaf_numberOfMonths</variablePath>
                          </variablePathList>
                        </variable>
                        <width>150px</width>
                      </element>
                      <element name="required" type="elfld">
                        <label>Required</label>
                        <variable type="varPath">
                          <variablePathList>
                            <variablePath>findSql_tfsReport.faPendingSummaryReportFinder/ui_UserInterface/uivar_result</variablePath>
                            <variablePath>doc_tfsReport.faPendingSummaryReportResultDoc/leaf_required</variablePath>
                          </variablePathList>
                        </variable>
                        <width>150px</width>
                      </element>
                    </elementList>
                  </element>
                </elementList>
              </override>
            </overrideList>
          </overlay>
          <overlay name="Menu" type="exov">
            <base>ui_com.conceptwave.system.BaseFinderUserInterface/frmui_Forms/exov_Menu</base>
            <extends>ui_com.conceptwave.system.BaseFinderUserInterface/frmui_Forms/exov_Menu</extends>
            <label>Menu</label>
            <overrideList>
              <override name="cwfAdd" type="eldel"/>
              <override name="cwfCopy" type="eldel"/>
              <override name="cwfDelete" type="eldel"/>
              <override name="cwfDetail" type="eldel"/>
              <override name="cwfUpdate" type="eldel"/>
              <override name="finderMenuLayout" type="elext">
                <elementList>
                  <element name="showTFRMenuItem" type="elmnu">
                    <clickMethod type="varPath">
                      <variablePathList>
                        <variablePath>findSql_tfsReport.faPendingSummaryReportFinder/ui_UserInterface/usrActionMethod_showTFR</variablePath>
                      </variablePathList>
                    </clickMethod>
                    <label>Show TFR</label>
                  </element>
                </elementList>
              </override>
            </overrideList>
          </overlay>
        </overlayList>
      </vform>
    </vformList>
    <methodList>
      <method name="showTFR" type="usrActionMethod">
        <dialogHeight>768</dialogHeight>
        <dialogWidth>1024</dialogWidth>
        <parameterList>
          <parameter name="dialog" type="rifp">
            <type>ui_com.conceptwave.system.Dialog</type>
          </parameter>
        </parameterList>
        <showInPopup>true</showInPopup>
        <script><![CDATA[
          if(this.model.selected == null || this.model.selected.length == 0) {
              Global.throwException("UU0115");
          }

          if(this.model.selected.length > 1) {
              Global.throwException("UU0116");
          }

          var tfrFinder = new Finder("tfs.findTFR");
          tfrFinder.searchDocument.tfrId = this.model.selected[0].tfrId;
          var resultList = tfrFinder.search();

          var tfrFound = null;

          if(resultList != null && resultList.length > 0) {
              tfrFound = resultList[0];
          }
          else {
              Global.throwException("AE0008", ["TFR", "FA Pending Summary Report"]);
          }

          return tfrFound;
        ]]></script>
      </method>
    </methodList>
  </UserInterface>
  <viewList>
    <view name="faPendingSummaryReportFinderView" type="findVView">
      <inputOverlay>doc_tfsReport.faPendingSummaryReportSearchDoc/ui_UserInterface/frmui_Forms/exov_Default</inputOverlay>
      <label>FA Pending Summary Report</label>
      <outputOverlay>findSql_tfsReport.faPendingSummaryReportFinder/ui_UserInterface/frmui_Forms/exov_Result</outputOverlay>
      <showSearchForm>true</showSearchForm>
    </view>
  </viewList>
</findSql>