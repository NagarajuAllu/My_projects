  <methodList>
    <method name="cwOnFinderSQLSel" type="action">
      <system>true</system>
      <script><![CDATA[
        select v.user_id,
               v.roleid,
               v.cwDocId,
               v.operation_name,
               v.performed_task_name,
               v.assign_to_user_date,
               v.track_completion_date,
               v.duration_in_hours,
               v.duration_in_days,
               v.object_type,
               v.object_id,
               v.object_name,
               v.object_status,
               v.ni_number,
               v.tfr_project,
               v.current_userid,
               v.current_department
          from stc_work_productivity_report_v v
         where 1 = 1
           {[0] and ur.roleid in ?}
           {[1] and t.user_id in ?}
           {[2] and t.assign_to_user_date >= ? }
           {[3] and t.assign_to_user_date <= ? }
           {[4] and t.completion_date >= ? }
           {[5] and t.completion_date <= ? }
      ]]></script>
    </method>
