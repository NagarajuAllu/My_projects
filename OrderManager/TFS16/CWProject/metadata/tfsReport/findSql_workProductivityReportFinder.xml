    <methodList>
      <method name="showTFR" type="usrActionMethod">
        <dialogHeight>768</dialogHeight>
        <dialogWidth>1024</dialogWidth>
        <parameterList>
          <parameter name="dialog" type="rifp">
            <type>ui_com.conceptwave.system.Dialog</type>
          </parameter>
        </parameterList>
        <showInPopup>true</showInPopup>
        <script><![CDATA[
          if(this.model.selected == null || this.model.selected.length == 0) {
              Global.throwException("UU0115");
          }

          if(this.model.selected.length > 1) {
              Global.throwException("UU0116");
          }

          var tfrId = this.model.selected[0].objectId;
          if(this.model.selected[0].objectType == "TFR Task") {
              var taskFinder = new Finder("tfs.findAllTasks");
              taskFinder.searchDocument.tfrTaskId = this.model.selected[0].objectId;
              var resultList = taskFinder.search();

              if(resultList != null && resultList.length > 0) {
                  tfrId = resultList[0].tfrId;
              }
              else {
                  Global.throwException("AE0008", ["TFR", "TFR Task"]);
              }
          }

          var tfrFinder = new Finder("tfs.findTFR");
          tfrFinder.searchDocument.tfrId = tfrId;
          var resultList = tfrFinder.search();

          var tfrFound = null;

          if(resultList != null && resultList.length > 0) {
              tfrFound = resultList[0];
          }
          else {
              Global.throwException("AE0008", ["TFR", "Work Productivity Report"]);
          }

          return tfrFound;
        ]]></script>
      </method>
      <method name="showTFRTask" type="usrActionMethod">
        <dialogHeight>768</dialogHeight>
        <dialogWidth>1024</dialogWidth>
        <parameterList>
