                        <variable type="varPath">
                          <variablePathList>
                            <variablePath>findSql_tfsReport.tfrRejectionReportFinder/ui_UserInterface/uivar_result</variablePath>
                            <variablePath>doc_tfsReport.tfrRejectionReportResultDoc/leaf_whenRejected</variablePath>
