                        </variable>
                        <width>200px</width>
                      </element>
                      <element name="rejectionCode" type="elfld">
                        <label>Rejection Code</label>
                        <variable type="varPath">
                          <variablePathList>
                            <variablePath>findSql_tfsReport.tfrRejectionReportFinder/ui_UserInterface/uivar_result</variablePath>
                            <variablePath>doc_tfsReport.tfrRejectionReportResultDoc/leaf_rejectionCode</variablePath>
                          </variablePathList>
                        </variable>
                        <width>200px</width>
                      </element>
                      <element name="userId" type="elref">
                        <label>Current User</label>
                        <variable type="varPath">
                          <variablePathList>
                            <variablePath>findSql_tfsReport.tfrRejectionReportFinder/ui_UserInterface/uivar_result</variablePath>
                            <variablePath>doc_tfsReport.tfrRejectionReportResultDoc/leaf_userId</variablePath>
                          </variablePathList>
                        </variable>
                        <width>150px</width>
                      </element>
                      <element name="department" type="elref">
                        <label>Current Department</label>
                        <variable type="varPath">
                          <variablePathList>
                            <variablePath>findSql_tfsReport.tfrRejectionReportFinder/ui_UserInterface/uivar_result</variablePath>
                            <variablePath>doc_tfsReport.tfrRejectionReportResultDoc/leaf_department</variablePath>
                          </variablePathList>
                        </variable>
                        <width>200px</width>
                      </element>
                      <element name="textQuestion" type="elfld">
                        <label>Question</label>
                        <variable type="varPath">
                          <variablePathList>
                            <variablePath>findSql_tfsReport.tfrRejectionReportFinder/ui_UserInterface/uivar_result</variablePath>
                            <variablePath>doc_tfsReport.tfrRejectionReportResultDoc/leaf_textQuestion</variablePath>
                          </variablePathList>
                        </variable>
                        <width>300px</width>
                      </element>
                      <element name="textAnswer" type="elfld">
                        <label>Answer Text</label>
                        <variable type="varPath">
                          <variablePathList>
                            <variablePath>findSql_tfsReport.tfrRejectionReportFinder/ui_UserInterface/uivar_result</variablePath>
                            <variablePath>doc_tfsReport.tfrRejectionReportResultDoc/leaf_textAnswer</variablePath>
                          </variablePathList>
                        </variable>
                        <width>300px</width>
                      </element>
                    </elementList>
                  </element>
                </elementList>
              </override>
            </overrideList>
          </overlay>
          <overlay name="Menu" type="exov">
            <base>ui_com.conceptwave.system.BaseFinderUserInterface/frmui_Forms/exov_Menu</base>
            <extends>ui_com.conceptwave.system.BaseFinderUserInterface/frmui_Forms/exov_Menu</extends>
            <label>Menu</label>
            <overrideList>
              <override name="cwfAdd" type="eldel"/>
              <override name="cwfCopy" type="eldel"/>
              <override name="cwfDelete" type="eldel"/>
              <override name="cwfDetail" type="eldel"/>
              <override name="cwfUpdate" type="eldel"/>
              <override name="finderMenuLayout" type="elext">
                <elementList>
                  <element name="showTFRMenuItem" type="elmnu">
                    <clickMethod type="varPath">
                      <variablePathList>
                        <variablePath>findSql_tfsReport.tfrRejectionReportFinder/ui_UserInterface/usrActionMethod_showTFR</variablePath>
                      </variablePathList>
                    </clickMethod>
                    <label>Show TFR</label>
                  </element>
                </elementList>
              </override>
            </overrideList>
          </overlay>
        </overlayList>
      </vform>
    </vformList>
    <methodList>
      <method name="showTFR" type="usrActionMethod">
        <dialogHeight>768</dialogHeight>
        <dialogWidth>1024</dialogWidth>
        <parameterList>
          <parameter name="dialog" type="rifp">
            <type>ui_com.conceptwave.system.Dialog</type>
          </parameter>
        </parameterList>
        <showInPopup>true</showInPopup>
        <script><![CDATA[
          if(this.model.selected == null || this.model.selected.length == 0) {
              Global.throwException("UU0115");
          }

          if(this.model.selected.length > 1) {
              Global.throwException("UU0116");
          }

          var tfrFinder = new Finder("tfs.findTFR");
          tfrFinder.searchDocument.tfrId = this.model.selected[0].tfrId;
          var resultList = tfrFinder.search();

          var tfrFound = null;

          if(resultList != null && resultList.length > 0) {
              tfrFound = resultList[0];
          }
          else {
              Global.throwException("AE0008", ["TFR", "TFR Rejection Report"]);
          }

          return tfrFound;
        ]]></script>
      </method>
    </methodList>
  </UserInterface>
  <viewList>
    <view name="tfrRejectionReportFinderView" type="findVView">
      <inputOverlay>doc_tfsReport.tfrRejectionReportSearchDoc/ui_UserInterface/frmui_Forms/exov_Default</inputOverlay>
      <label>TFR Rejection Report</label>
      <outputOverlay>findSql_tfsReport.tfrRejectionReportFinder/ui_UserInterface/frmui_Forms/exov_Result</outputOverlay>
      <showSearchForm>true</showSearchForm>
    </view>
  </viewList>
</findSql>