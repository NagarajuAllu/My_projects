<?xml version="1.0" encoding="UTF-8" ?>
<findSql name="tfsReport.tfrRejectionReportFinder">
  <dbSchema>INTERFACE</dbSchema>
  <extends>find_com.conceptwave.system.Finder</extends>
  <input>doc_tfsReport.tfrRejectionReportSearchDoc</input>
  <label>TFR Rejection Report Finder</label>
  <maxRowsLimit>9999</maxRowsLimit>
  <metaVersion>25</metaVersion>
  <output>doc_tfsReport.tfrRejectionReportResultDoc</output>
  <rtConfigurable>true</rtConfigurable>
  <type>SQL</type>
  <methodList>
    <method name="cwOnFinderSQLSel" type="action">
      <system>true</system>
      <script><![CDATA[
        select v.activity_id,
               v.tfr_id,
               v.tfr_number,
               v.tfr_status,
               v.ni_number,
               v.site_a_name,
               v.site_z_name,
               v.site_a_jvc,
               v.site_z_jvc,
               v.proc_creation_date creation_date,
               v.rejection_code,
               v.current_userid,
               v.current_department,
               v.question_id,
               v.text_question,
               v.answer_id,
               v.text_answer
          from stc_tfr_rejection_report_v v
         where 1 = 1
           {[0] and proc.creation_date >= ?}
           {[1] and proc.creation_date <= ?}
        order by v.tfr_id, v.proc_creation_date, v.act_creation_date
      ]]></script>
    </method>
  </methodList>
  <UserInterface name="UserInterface" type="ui">
    <extends>ui_com.conceptwave.system.FinderUserInterface</extends>
    <variableList>
      <variable name="finder" type="uivar">
        <valueType>ui_com.conceptwave.system.FinderUserInterface</valueType>
      </variable>
      <variable name="search" type="uivar">
        <valueType>doc_tfsReport.tfrRejectionReportSearchDoc/ui_UserInterface</valueType>
      </variable>
      <variable name="detail" type="uivar">
        <flags enum-type="metadataFlag">VALIDATE</flags>
        <valueType>doc_tfsReport.tfrRejectionReportResultDoc/ui_UserInterface</valueType>
      </variable>
      <variable name="result" type="uivar">
        <flags enum-type="metadataFlag">ARRAY</flags>
        <valueType>doc_tfsReport.tfrRejectionReportResultDoc</valueType>
      </variable>
    </variableList>
    <vformList>
      <vform name="Forms" type="frmui">
        <extends>ui_com.conceptwave.system.FinderUserInterface/frmui_Forms</extends>
        <overlayList>
          <overlay name="Default" type="exov">
            <base>ui_com.conceptwave.system.FinderUserInterface/frmui_Forms/exov_Default</base>
            <extends>ui_com.conceptwave.system.FinderUserInterface/frmui_Forms/exov_Default</extends>
            <label>Default</label>
            <overrideList>
              <override name="detailSection" type="elsect">
                <formReference type="varPath">
                  <variablePathList>
                    <variablePath>ui_com.conceptwave.system.MasterDetailUserInterface/uivar_detailForm</variablePath>
                  </variablePathList>
                </formReference>
                <variable type="varPath">
                  <variablePathList>
                    <variablePath>findSql_tfsReport.tfrRejectionReportFinder/ui_UserInterface/uivar_detail</variablePath>
                  </variablePathList>
                </variable>
                <visible type="varPath">
                  <variablePathList>
                    <variablePath>ui_com.conceptwave.system.MasterDetailUserInterface/permMethod_detailVisible</variablePath>
                  </variablePathList>
                </visible>
              </override>
              <override name="searchFormSection" type="elsect">
                <formReference type="varPath">
                  <variablePathList>
                    <variablePath>ui_com.conceptwave.system.BaseFinderUserInterface/uivar_searchForm</variablePath>
                  </variablePathList>
                </formReference>
                <variable type="varPath">
                  <variablePathList>
                    <variablePath>findSql_tfsReport.tfrRejectionReportFinder/ui_UserInterface/uivar_search</variablePath>
                  </variablePathList>
                </variable>
                <visible type="varPath">
                  <variablePathList>
                    <variablePath>ui_com.conceptwave.system.FinderUserInterface/permMethod_showSearchForm</variablePath>
                  </variablePathList>
                </visible>
              </override>
            </overrideList>
          </overlay>
          <overlay name="Result" type="exov">
            <base>ui_com.conceptwave.system.FinderUserInterface/frmui_Forms/vform_Result</base>
            <extends>ui_com.conceptwave.system.FinderUserInterface/frmui_Forms/vform_Result</extends>
            <label>Result</label>
            <overrideList>
              <override name="table" type="elvlt">
                <height>100%</height>
                <width>100%</width>
                <elementList>
                  <element name="tfrRejectionReportFinderResultTable" type="eltabl">
                    <onSelectChange type="varPath">
                      <variablePathList>
                        <variablePath>ui_com.conceptwave.system.MasterDetailUserInterface/action_OnSelChanged</variablePath>
                      </variablePathList>
                    </onSelectChange>
                    <variable type="varPath">
                      <variablePathList>
                        <variablePath>findSql_tfsReport.tfrRejectionReportFinder/ui_UserInterface/uivar_result</variablePath>
                      </variablePathList>
                    </variable>
                    <elementList>
                      <element name="tfrNumber" type="elfld">
                        <label>TFR Number</label>
                        <variable type="varPath">
                          <variablePathList>
                            <variablePath>findSql_tfsReport.tfrRejectionReportFinder/ui_UserInterface/uivar_result</variablePath>
                            <variablePath>doc_tfsReport.tfrRejectionReportResultDoc/leaf_tfrNumber</variablePath>
                          </variablePathList>
                        </variable>
                        <width>150px</width>
                      </element>
                      <element name="tfrStatus" type="elfld">
                        <label>TFR Status</label>
                        <variable type="varPath">
                          <variablePathList>
                            <variablePath>findSql_tfsReport.tfrRejectionReportFinder/ui_UserInterface/uivar_result</variablePath>
                            <variablePath>doc_tfsReport.tfrRejectionReportResultDoc/leaf_tfrStatus</variablePath>
                          </variablePathList>
                        </variable>
                        <width>200px</width>
                      </element>
                      <element name="niNumber" type="elfld">
                        <label>NI Number</label>
                        <variable type="varPath">
                          <variablePathList>
                            <variablePath>findSql_tfsReport.tfrRejectionReportFinder/ui_UserInterface/uivar_result</variablePath>
                            <variablePath>doc_tfsReport.tfrRejectionReportResultDoc/leaf_niNumber</variablePath>
                          </variablePathList>
                        </variable>
                        <width>150px</width>
                      </element>
                      <element name="siteA" type="elref">
                        <label>Site A</label>
                        <variable type="varPath">
                          <variablePathList>
                            <variablePath>findSql_tfsReport.tfrRejectionReportFinder/ui_UserInterface/uivar_result</variablePath>
                            <variablePath>doc_tfsReport.tfrRejectionReportResultDoc/leaf_siteA</variablePath>
                          </variablePathList>
                        </variable>
                        <width>200px</width>
                      </element>
                      <element name="siteZ" type="elref">
                        <label>Site Z</label>
                        <variable type="varPath">
                          <variablePathList>
                            <variablePath>findSql_tfsReport.tfrRejectionReportFinder/ui_UserInterface/uivar_result</variablePath>
                            <variablePath>doc_tfsReport.tfrRejectionReportResultDoc/leaf_siteZ</variablePath>
                          </variablePathList>
                        </variable>
                        <width>200px</width>
                      </element>
                      <element name="siteAJVCode" type="elfld">
                        <label>Site A JV Code</label>
                        <variable type="varPath">
                          <variablePathList>
                            <variablePath>findSql_tfsReport.tfrRejectionReportFinder/ui_UserInterface/uivar_result</variablePath>
                            <variablePath>doc_tfsReport.tfrRejectionReportResultDoc/leaf_siteAJVCode</variablePath>
                          </variablePathList>
                        </variable>
                        <width>150px</width>
                      </element>
                      <element name="siteZJVCode" type="elfld">
