                        <variable type="varPath">
                          <variablePathList>
                            <variablePath>findSql_tfsReport.niAsBuiltProductivityReportFinder/ui_UserInterface/uivar_result</variablePath>
                            <variablePath>doc_tfsReport.niAsBuiltProductivityReportResultDoc/leaf_userId</variablePath>
                          </variablePathList>
                        </variable>
                        <width>150px</width>
                      </element>
                    </elementList>
                  </element>
                </elementList>
              </override>
            </overrideList>
          </overlay>
          <overlay name="Menu" type="exov">
            <base>ui_com.conceptwave.system.BaseFinderUserInterface/frmui_Forms/exov_Menu</base>
            <extends>ui_com.conceptwave.system.BaseFinderUserInterface/frmui_Forms/exov_Menu</extends>
            <label>Menu</label>
            <overrideList>
              <override name="cwfAdd" type="eldel"/>
              <override name="cwfCopy" type="eldel"/>
              <override name="cwfDelete" type="eldel"/>
              <override name="cwfDetail" type="eldel"/>
              <override name="cwfUpdate" type="eldel"/>
              <override name="finderMenuLayout" type="elext">
                <elementList>
                  <element name="showTFRMenuItem" type="elmnu">
                    <clickMethod type="varPath">
                      <variablePathList>
                        <variablePath>findSql_tfsReport.niAsBuiltProductivityReportFinder/ui_UserInterface/usrActionMethod_showTFR</variablePath>
                      </variablePathList>
                    </clickMethod>
                    <label>Show TFR</label>
                  </element>
                </elementList>
              </override>
            </overrideList>
          </overlay>
        </overlayList>
      </vform>
    </vformList>
    <methodList>
      <method name="showTFR" type="usrActionMethod">
        <dialogHeight>768</dialogHeight>
        <dialogWidth>1024</dialogWidth>
        <parameterList>
          <parameter name="dialog" type="rifp">
            <type>ui_com.conceptwave.system.Dialog</type>
          </parameter>
        </parameterList>
        <showInPopup>true</showInPopup>
        <script><![CDATA[
          if(this.model.selected == null || this.model.selected.length == 0) {
              Global.throwException("UU0115");
          }

          if(this.model.selected.length > 1) {
              Global.throwException("UU0116");
          }

          var tfrFinder = new Finder("tfs.findTFR");
          tfrFinder.searchDocument.tfrId = this.model.selected[0].tfrId;
          var resultList = tfrFinder.search();

          var tfrFound = null;

          if(resultList != null && resultList.length > 0) {
              tfrFound = resultList[0];
          }
          else {
              Global.throwException("AE0008", ["TFR", "NI As-Built Productivity Report"]);
          }

          return tfrFound;
        ]]></script>
      </method>
    </methodList>
  </UserInterface>
  <viewList>
    <view name="niAsBuiltProductivityReportFinderView" type="findVView">
      <inputOverlay>doc_tfsReport.niAsBuiltProductivityReportSearchDoc/ui_UserInterface/frmui_Forms/exov_Default</inputOverlay>
      <label>NI As-Built Productivity Report</label>
      <outputOverlay>findSql_tfsReport.niAsBuiltProductivityReportFinder/ui_UserInterface/frmui_Forms/exov_Result</outputOverlay>
      <showSearchForm>true</showSearchForm>
    </view>
  </viewList>
</findSql>