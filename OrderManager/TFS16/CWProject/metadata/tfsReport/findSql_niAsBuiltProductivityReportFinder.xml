                            <variablePath>findSql_tfsReport.niAsBuiltProductivityReportFinder/ui_UserInterface/uivar_result</variablePath>
                            <variablePath>doc_tfsReport.niAsBuiltProductivityReportResultDoc/leaf_tfrTaskId</variablePath>
                          </variablePathList>
                        </variable>
                        <width>150px</width>
                      </element>
                      <element name="taskName" type="elfld">
                        <label>Task Name</label>
                        <variable type="varPath">
                          <variablePathList>
                            <variablePath>findSql_tfsReport.niAsBuiltProductivityReportFinder/ui_UserInterface/uivar_result</variablePath>
                            <variablePath>doc_tfsReport.niAsBuiltProductivityReportResultDoc/leaf_taskName</variablePath>
                          </variablePathList>
                        </variable>
                        <width>200px</width>
                      </element>
                      <element name="biId" type="elfld">
                        <label>BI Id</label>
                        <variable type="varPath">
                          <variablePathList>
                            <variablePath>findSql_tfsReport.niAsBuiltProductivityReportFinder/ui_UserInterface/uivar_result</variablePath>
                            <variablePath>doc_tfsReport.niAsBuiltProductivityReportResultDoc/leaf_biId</variablePath>
                          </variablePathList>
                        </variable>
                        <width>150px</width>
                      </element>
                      <element name="biCreationDate" type="eldate">
                        <label>BI Creation Date</label>
                        <variable type="varPath">
                          <variablePathList>
                            <variablePath>findSql_tfsReport.niAsBuiltProductivityReportFinder/ui_UserInterface/uivar_result</variablePath>
                            <variablePath>doc_tfsReport.niAsBuiltProductivityReportResultDoc/leaf_biCreationDate</variablePath>
                          </variablePathList>
                        </variable>
                        <width>200px</width>
                      </element>
                      <element name="biCompletionDate" type="eldate">
                        <label>BI Completion Date</label>
                        <variable type="varPath">
                          <variablePathList>
                            <variablePath>findSql_tfsReport.niAsBuiltProductivityReportFinder/ui_UserInterface/uivar_result</variablePath>
                            <variablePath>doc_tfsReport.niAsBuiltProductivityReportResultDoc/leaf_biCompletionDate</variablePath>
