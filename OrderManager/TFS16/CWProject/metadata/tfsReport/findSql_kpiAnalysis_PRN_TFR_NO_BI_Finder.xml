    <method name="cwOnFinderSQLSel" type="action">
      <system>true</system>
      <script><![CDATA[
        select v.customer_name,
               v.prnr_id,
               v.prnr_name,
               v.prnr_creation_date,
               v.prnr_released_date,
               v.prnr_cancelled_date,
               v.prnr_duration_hours,
               v.prnr_duration_days,
               v.prn_id,
               v.prn_name,
               v.prn_released_date,
               v.prn_cancelled_date,
               v.prn_duration_hours,
               v.prn_duration_days,
               v.tfr_id,
               v.tfr_number,
               v.fa_completion_date,
               v.tfr_cancelled_date,
               v.tfr_duration_hours,
               v.tfr_duration_days,
               v.orderInitiator_duration_days,
               v.networkDesign_duration_days,
               v.faTeams_duration_days,
               v.cust_acceptance_duration_days,
               v.end2end_duration_days
          from stc_kpi_prn_tfr_report_v v
         where 1 = 1
           {[0] and v.prn_creation_date >= ?}
           {[1] and v.prn_creation_date <= ?}
           {[2] and v.fa_completion_date >= ?}
           {[3] and v.fa_completion_date <= ?}
           {[6] and (nvl(v.fa_completion_date, v.tfr_cancelled_date) - v.prn_creation_date) <= ?}
      ]]></script>
