    <methodList>
      <method name="showAllBI" type="usrActionMethod">
        <dialogHeight>600</dialogHeight>
        <dialogWidth>850</dialogWidth>
        <parameterList>
          <parameter name="dialog" type="rifp">
            <type>ui_com.conceptwave.system.Dialog</type>
          </parameter>
        </parameterList>
        <showInPopup>true</showInPopup>
        <script><![CDATA[
          if(this.model.selected == null || this.model.selected.length == 0) {
              Global.throwException("UU0115");
          }

          if(this.model.selected.length > 1) {
              Global.throwException("UU0116");
          }

          var biFinder = new Finder("tfsReport.biReportFinder");
          biFinder.searchDocument.tfrNumber = this.model.selected[0].tfrNumber;
          biFinder.searchDocument.prnName = this.model.selected[0].prnName;
          biFinder.search();

          return biFinder;
        ]]></script>
