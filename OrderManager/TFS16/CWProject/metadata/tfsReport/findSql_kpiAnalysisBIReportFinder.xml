<?xml version="1.0" encoding="UTF-8" ?>
<findSql name="tfsReport.kpiAnalysisBIReportFinder">
  <dbSchema>INTERFACE</dbSchema>
  <extends>find_com.conceptwave.system.Finder</extends>
  <input>doc_tfsReport.kpiAnalysisBIReportSearchDoc</input>
  <label>KPI Analysis For BI Report Finder</label>
  <maxRowsLimit>9999</maxRowsLimit>
  <metaVersion>25</metaVersion>
  <output>doc_tfsReport.kpiAnalysisBIReportResultDoc</output>
  <rtConfigurable>true</rtConfigurable>
  <type>SQL</type>
  <methodList>
    <method name="cwOnFinderSQLSel" type="action">
      <system>true</system>
      <script><![CDATA[
        select bi_data.category,
               count(bi_data.bi_id) total,
               count(case when bi_data.bi_status = 'CANCELLED' then 1 else null end) cancelled,
               nvl(avg(bi_data.prov_time), 0) average
          from (
                select bi.bi_id, bi.category, bi.bi_status, (bi.completion_date - bi.creation_date) prov_time
                  from tfs.stc_bi bi
                 where 1=1
                  {[0] and bi.creation_date >= ?}
                  {[1] and bi.creation_date <= ?}
                  {[2] and bi.completion_date >= ?}
                  {[3] and bi.completion_date <= ?}
                  {[4] and (bi.completion_date - bi.creation_date) <= ?}) bi_data
        group by bi_data.category
      ]]></script>
    </method>
  </methodList>
  <UserInterface name="UserInterface" type="ui">
    <extends>ui_com.conceptwave.system.FinderUserInterface</extends>
    <variableList>
      <variable name="finder" type="uivar">
        <valueType>ui_com.conceptwave.system.FinderUserInterface</valueType>
      </variable>
      <variable name="result" type="uivar">
        <flags enum-type="metadataFlag">ARRAY</flags>
        <valueType>doc_tfsReport.kpiAnalysisBIReportResultDoc</valueType>
      </variable>
      <variable name="search" type="uivar">
        <valueType>doc_tfsReport.kpiAnalysisBIReportSearchDoc/ui_UserInterface</valueType>
      </variable>
      <variable name="detail" type="uivar">
        <flags enum-type="metadataFlag">VALIDATE</flags>
        <valueType>doc_tfsReport.kpiAnalysisBIReportResultDoc/ui_UserInterface</valueType>
      </variable>
    </variableList>
    <vformList>
      <vform name="Forms" type="frmui">
        <extends>ui_com.conceptwave.system.FinderUserInterface/frmui_Forms</extends>
        <overlayList>
          <overlay name="Result" type="exov">
            <base>ui_com.conceptwave.system.FinderUserInterface/frmui_Forms/vform_Result</base>
            <extends>ui_com.conceptwave.system.FinderUserInterface/frmui_Forms/vform_Result</extends>
            <label>Result</label>
            <overrideList>
              <override name="table" type="elvlt">
                <height>100%</height>
                <width>100%</width>
                <elementList>
                  <element name="kpiAnalysisBIReportFinderResultTable" type="eltabl">
                    <onSelectChange type="varPath">
                      <variablePathList>
                        <variablePath>ui_com.conceptwave.system.MasterDetailUserInterface/action_OnSelChanged</variablePath>
                      </variablePathList>
                    </onSelectChange>
                    <variable type="varPath">
                      <variablePathList>
                        <variablePath>findSql_tfsReport.kpiAnalysisBIReportFinder/ui_UserInterface/uivar_result</variablePath>
                      </variablePathList>
                    </variable>
                    <elementList>
                      <element name="biCategory" type="elfld">
                        <label>BI Category</label>
                        <variable type="varPath">
                          <variablePathList>
                            <variablePath>findSql_tfsReport.kpiAnalysisBIReportFinder/ui_UserInterface/uivar_result</variablePath>
                            <variablePath>doc_tfsReport.kpiAnalysisBIReportResultDoc/leaf_biCategory</variablePath>
                          </variablePathList>
                        </variable>
                        <width>150px</width>
                      </element>
                      <element name="total" type="elfld">
                        <label># Records Found</label>
                        <variable type="varPath">
                          <variablePathList>
                            <variablePath>findSql_tfsReport.kpiAnalysisBIReportFinder/ui_UserInterface/uivar_result</variablePath>
                            <variablePath>doc_tfsReport.kpiAnalysisBIReportResultDoc/leaf_total</variablePath>
                          </variablePathList>
                        </variable>
                        <width>150px</width>
                      </element>
                      <element name="average" type="elfld">
                        <label>Average Duration Completion Process (days)</label>
                        <variable type="varPath">
                          <variablePathList>
                            <variablePath>findSql_tfsReport.kpiAnalysisBIReportFinder/ui_UserInterface/uivar_result</variablePath>
                            <variablePath>doc_tfsReport.kpiAnalysisBIReportResultDoc/leaf_average</variablePath>
                          </variablePathList>
                        </variable>
                        <width>300px</width>
                      </element>
                      <element name="cancelled" type="elfld">
                        <label># Records Cancelled</label>
                        <variable type="varPath">
                          <variablePathList>
                            <variablePath>findSql_tfsReport.kpiAnalysisBIReportFinder/ui_UserInterface/uivar_result</variablePath>
                            <variablePath>doc_tfsReport.kpiAnalysisBIReportResultDoc/leaf_cancelled</variablePath>
                          </variablePathList>
                        </variable>
                        <width>150px</width>
                      </element>
                    </elementList>
                  </element>
                </elementList>
              </override>
            </overrideList>
          </overlay>
          <overlay name="Default" type="exov">
            <base>ui_com.conceptwave.system.FinderUserInterface/frmui_Forms/exov_Default</base>
            <extends>ui_com.conceptwave.system.FinderUserInterface/frmui_Forms/exov_Default</extends>
            <label>Default</label>
            <overrideList>
              <override name="detailSection" type="elsect">
                <formReference type="varPath">
                  <variablePathList>
                    <variablePath>ui_com.conceptwave.system.MasterDetailUserInterface/uivar_detailForm</variablePath>
                  </variablePathList>
                </formReference>
                <variable type="varPath">
                  <variablePathList>
                    <variablePath>findSql_tfsReport.kpiAnalysisBIReportFinder/ui_UserInterface/uivar_detail</variablePath>
                  </variablePathList>
                </variable>
                <visible type="varPath">
                  <variablePathList>
                    <variablePath>ui_com.conceptwave.system.MasterDetailUserInterface/permMethod_detailVisible</variablePath>
                  </variablePathList>
                </visible>
              </override>
              <override name="searchFormSection" type="elsect">
                <formReference type="varPath">
                  <variablePathList>
                    <variablePath>ui_com.conceptwave.system.BaseFinderUserInterface/uivar_searchForm</variablePath>
                  </variablePathList>
                </formReference>
                <variable type="varPath">
                  <variablePathList>
                    <variablePath>findSql_tfsReport.kpiAnalysisBIReportFinder/ui_UserInterface/uivar_search</variablePath>
                  </variablePathList>
                </variable>
                <visible type="varPath">
                  <variablePathList>
                    <variablePath>ui_com.conceptwave.system.FinderUserInterface/permMethod_showSearchForm</variablePath>
                  </variablePathList>
                </visible>
              </override>
            </overrideList>
          </overlay>
          <overlay name="Menu" type="exov">
            <base>ui_com.conceptwave.system.BaseFinderUserInterface/frmui_Forms/exov_Menu</base>
            <extends>ui_com.conceptwave.system.BaseFinderUserInterface/frmui_Forms/exov_Menu</extends>
            <label>Menu</label>
            <overrideList>
              <override name="cwfAdd" type="eldel"/>
              <override name="cwfCopy" type="eldel"/>
              <override name="cwfDelete" type="eldel"/>
              <override name="cwfDetail" type="eldel"/>
              <override name="cwfUpdate" type="eldel"/>
              <override name="finderMenuLayout" type="elext">
                <elementList>
                  <element name="showDetailsMenuItem" type="elmnu">
                    <clickMethod type="varPath">
                      <variablePathList>
                        <variablePath>findSql_tfsReport.kpiAnalysisBIReportFinder/ui_UserInterface/usrActionMethod_showDetails</variablePath>
                      </variablePathList>
                    </clickMethod>
                    <label>Show Details</label>
                  </element>
                </elementList>
              </override>
            </overrideList>
          </overlay>
        </overlayList>
      </vform>
    </vformList>
    <methodList>
      <method name="showDetails" type="usrActionMethod">
        <dialogHeight>768</dialogHeight>
        <dialogWidth>1600</dialogWidth>
        <parameterList>
          <parameter name="dialog" type="rifp">
            <type>ui_com.conceptwave.system.Dialog</type>
          </parameter>
        </parameterList>
        <showInPopup>true</showInPopup>
        <script><![CDATA[
          if(this.model.selected == null || this.model.selected.length == 0) {
              Global.throwException("UU0115");
          }

          if(this.model.selected.length > 1) {
              Global.throwException("UU0116");
          }

          var inputDocument = this.model.searchDocument;

          var detailFinder = new Finder("tfsReport.kpiAnalysis_BI_ONLY_Finder");
          detailFinder.searchDocument.creationDateFrom = inputDocument.creationDateFrom;
          detailFinder.searchDocument.creationDateTo = inputDocument.creationDateTo;
          detailFinder.searchDocument.completionDateFrom = inputDocument.completionDateFrom;
          detailFinder.searchDocument.completionDateTo = inputDocument.completionDateTo;
          detailFinder.searchDocument.numDaysProvisioningBI = inputDocument.numDaysProvisioningBI;
          detailFinder.searchDocument.biCategory = this.model.selected[0].biCategory;

          detailFinder.search();

          return detailFinder;
        ]]></script>
      </method>
    </methodList>
  </UserInterface>
  <viewList>
    <view name="kpiAnalysisBIReportFinderView" type="findVView">
      <inputOverlay>doc_tfsReport.kpiAnalysisBIReportSearchDoc/ui_UserInterface/frmui_Forms/exov_Default</inputOverlay>
      <label>KPI Analysis For BI Only Report</label>
      <outputOverlay>findSql_tfsReport.kpiAnalysisBIReportFinder/ui_UserInterface/frmui_Forms/exov_Result</outputOverlay>
      <showSearchForm>true</showSearchForm>
      <sortListList>
        <sortList type="stitem">
          <path>doc_tfsReport.kpiAnalysisBIReportResultDoc/leaf_biCategory</path>
        </sortList>
      </sortListList>
    </view>
  </viewList>
</findSql>