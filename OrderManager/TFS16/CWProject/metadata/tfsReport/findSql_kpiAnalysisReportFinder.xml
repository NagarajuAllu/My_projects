<?xml version="1.0" encoding="UTF-8" ?>
<findSql name="tfsReport.kpiAnalysisReportFinder">
  <dbSchema>INTERFACE</dbSchema>
  <extends>find_com.conceptwave.system.Finder</extends>
  <input>doc_tfsReport.kpiAnalysisReportSearchDoc</input>
  <label>KPI Analysis Report Finder</label>
  <maxRowsLimit>100</maxRowsLimit>
  <metaVersion>25</metaVersion>
  <output>doc_tfsReport.kpiAnalysisReportResultDoc</output>
  <rtConfigurable>true</rtConfigurable>
  <type>SQL</type>
  <methodList>
    <method name="cwOnFinderSQLSel" type="action">
      <system>true</system>
      <script><![CDATA[
        select 1 position,
               'PRN_ONLY' type,
               count(prn_only.prn_id) total,
               count(case when prn_only.prn_status = 'CANCELLED' then 1 else null end) cancelled,
               nvl(avg(prn_only.prov_time), 0) average
          from (
                select prn.prn_id, prn.prn_status, (nvl(prn.released_date, prn.cancelled_date) - prn.creation_date) prov_time
                  from tfs.stc_prn prn
                 where (prn.released_date is not null or prn.cancelled_date is not null)
                  {[0] and prn.creation_date >= ?}
                  {[1] and prn.creation_date <= ?}
                  {[2] and prn.released_date >= ?}
                  {[3] and prn.released_date <= ?}
                  {[4] and (nvl(prn.released_date, prn.cancelled_date) - prn.creation_date) <= ?}) prn_only
        union
        select 2 position,
               'TFR_ONLY' type,
               count(tfr_only.tfr_id) total,
               count(case when tfr_only.tfr_status = 'CANCELLED' then 1 else null end) cancelled,
               nvl(avg(tfr_only.prov_time), 0) average
          from (
                select tfr.tfr_id, tfr.tfr_status, (nvl(tfr.fa_completion_date, tfr.cancelled_date) - tfr.creation_date) prov_time
                  from tfs.stc_tfr tfr
                 where (tfr.fa_completion_date is not null or tfr.cancelled_date is not null)
                  {[0] and tfr.creation_date >= ?}
                  {[1] and tfr.creation_date <= ?}
                  {[2] and tfr.fa_completion_date >= ?}
                  {[3] and tfr.fa_completion_date <= ?}
                  {[5] and (nvl(tfr.fa_completion_date, tfr.cancelled_date) - tfr.creation_date) <= ?}) tfr_only
        union
        select 3 position,
               'PRN_TFR_NO_BI' type,
               count(prn_tfr_no_bi.prn_id) total,
               count(case when prn_tfr_no_bi.prn_status = 'CANCELLED' then 1 else null end) cancelled,
               nvl(avg(prn_tfr_no_bi.prov_time), 0) average
          from (
                select prn.prn_id, prn.prn_status, (nvl(tfr.fa_completion_date, tfr.cancelled_date) - prn.creation_date) prov_time
                  from tfs.stc_prn prn, tfs.stc_tfr tfr
                 where prn.tfr_id = tfr.tfr_id
                   and prn.prn_id not in (select distinct bir.source_prn_id
                                            from stc_bi_relationship bir
                                           where bir.source_prn_id <> '-1')
                   and tfr.tfr_id not in (select distinct bir.source_tfr_id
                                            from stc_bi_relationship bir
                                           where bir.source_tfr_id <> '-1')
                   and (tfr.fa_completion_date is not null or tfr.cancelled_date is not null)
                  {[0] and prn.creation_date >= ?}
                  {[1] and prn.creation_date <= ?}
                  {[2] and tfr.fa_completion_date >= ?}
                  {[3] and tfr.fa_completion_date <= ?}
                  {[6] and (nvl(tfr.fa_completion_date, tfr.cancelled_date) - prn.creation_date) <= ?}) prn_tfr_no_bi
        union
        select 4 position,
               'PRN_TFR_WITH_BI' type,
               count(prn_tfr_with_bi.prn_id) total,
               count(case when prn_tfr_with_bi.prn_status = 'CANCELLED' then 1 else null end) cancelled,
               nvl(avg(prn_tfr_with_bi.prov_time), 0) average
          from (
                select prn.prn_id, prn.prn_status, (nvl(tfr.fa_completion_date, tfr.cancelled_date) - prn.creation_date) prov_time
                  from tfs.stc_prn prn, tfs.stc_tfr tfr
                 where prn.tfr_id = tfr.tfr_id
                   and (prn.prn_id in (select distinct bir.source_prn_id
                                         from stc_bi_relationship bir
                                        where bir.source_prn_id <> '-1')
                        or
                        tfr.tfr_id in (select distinct bir.source_tfr_id
                                         from stc_bi_relationship bir
                                        where bir.source_tfr_id <> '-1')
                        )
                   and (tfr.fa_completion_date is not null or tfr.cancelled_date is not null)
                  {[0] and prn.creation_date >= ?}
                  {[1] and prn.creation_date <= ?}
                  {[2] and tfr.fa_completion_date >= ?}
                  {[3] and tfr.fa_completion_date <= ?}
                  {[7] and (nvl(tfr.fa_completion_date, tfr.cancelled_date) - prn.creation_date) <= ?}) prn_tfr_with_bi
      ]]></script>
    </method>
  </methodList>
  <UserInterface name="UserInterface" type="ui">
    <extends>ui_com.conceptwave.system.FinderUserInterface</extends>
    <variableList>
      <variable name="finder" type="uivar">
        <valueType>ui_com.conceptwave.system.FinderUserInterface</valueType>
      </variable>
      <variable name="result" type="uivar">
        <flags enum-type="metadataFlag">ARRAY</flags>
        <valueType>doc_tfsReport.kpiAnalysisReportResultDoc</valueType>
      </variable>
      <variable name="search" type="uivar">
        <valueType>doc_tfsReport.kpiAnalysisReportSearchDoc/ui_UserInterface</valueType>
      </variable>
      <variable name="detail" type="uivar">
        <flags enum-type="metadataFlag">VALIDATE</flags>
        <valueType>doc_tfsReport.kpiAnalysisReportResultDoc/ui_UserInterface</valueType>
      </variable>
    </variableList>
    <vformList>
      <vform name="Forms" type="frmui">
        <extends>ui_com.conceptwave.system.FinderUserInterface/frmui_Forms</extends>
        <overlayList>
          <overlay name="Result" type="exov">
            <base>ui_com.conceptwave.system.FinderUserInterface/frmui_Forms/vform_Result</base>
            <extends>ui_com.conceptwave.system.FinderUserInterface/frmui_Forms/vform_Result</extends>
            <label>Result</label>
            <overrideList>
              <override name="table" type="elvlt">
                <height>100%</height>
                <width>100%</width>
                <elementList>
                  <element name="kpiAnalysisReportFinderResultTable" type="eltabl">
                    <onSelectChange type="varPath">
                      <variablePathList>
                        <variablePath>ui_com.conceptwave.system.MasterDetailUserInterface/action_OnSelChanged</variablePath>
                      </variablePathList>
                    </onSelectChange>
                    <variable type="varPath">
                      <variablePathList>
                        <variablePath>findSql_tfsReport.kpiAnalysisReportFinder/ui_UserInterface/uivar_result</variablePath>
                      </variablePathList>
                    </variable>
                    <elementList>
                      <element name="type" type="elfld">
                        <label>Group Type</label>
                        <variable type="varPath">
                          <variablePathList>
                            <variablePath>findSql_tfsReport.kpiAnalysisReportFinder/ui_UserInterface/uivar_result</variablePath>
                            <variablePath>doc_tfsReport.kpiAnalysisReportResultDoc/leaf_type</variablePath>
                          </variablePathList>
                        </variable>
                        <width>150px</width>
                      </element>
                      <element name="total" type="elfld">
                        <label># Records Found</label>
                        <variable type="varPath">
                          <variablePathList>
                            <variablePath>findSql_tfsReport.kpiAnalysisReportFinder/ui_UserInterface/uivar_result</variablePath>
                            <variablePath>doc_tfsReport.kpiAnalysisReportResultDoc/leaf_total</variablePath>
                          </variablePathList>
                        </variable>
                        <width>150px</width>
                      </element>
                      <element name="average" type="elfld">
                        <label>Average Duration Prov. Process (days)</label>
                        <variable type="varPath">
                          <variablePathList>
                            <variablePath>findSql_tfsReport.kpiAnalysisReportFinder/ui_UserInterface/uivar_result</variablePath>
                            <variablePath>doc_tfsReport.kpiAnalysisReportResultDoc/leaf_average</variablePath>
                          </variablePathList>
                        </variable>
                        <width>300px</width>
                      </element>
                      <element name="cancelled" type="elfld">
                        <label># Records Cancelled</label>
                        <variable type="varPath">
                          <variablePathList>
                            <variablePath>findSql_tfsReport.kpiAnalysisReportFinder/ui_UserInterface/uivar_result</variablePath>
                            <variablePath>doc_tfsReport.kpiAnalysisReportResultDoc/leaf_cancelled</variablePath>
                          </variablePathList>
                        </variable>
                        <width>150px</width>
                      </element>
                    </elementList>
                  </element>
                </elementList>
              </override>
            </overrideList>
          </overlay>
          <overlay name="Default" type="exov">
            <base>ui_com.conceptwave.system.FinderUserInterface/frmui_Forms/exov_Default</base>
            <extends>ui_com.conceptwave.system.FinderUserInterface/frmui_Forms/exov_Default</extends>
            <label>Default</label>
            <overrideList>
              <override name="detailSection" type="elsect">
                <formReference type="varPath">
                  <variablePathList>
                    <variablePath>ui_com.conceptwave.system.MasterDetailUserInterface/uivar_detailForm</variablePath>
                  </variablePathList>
                </formReference>
                <variable type="varPath">
                  <variablePathList>
                    <variablePath>findSql_tfsReport.kpiAnalysisReportFinder/ui_UserInterface/uivar_detail</variablePath>
                  </variablePathList>
                </variable>
                <visible type="varPath">
                  <variablePathList>
                    <variablePath>ui_com.conceptwave.system.MasterDetailUserInterface/permMethod_detailVisible</variablePath>
                  </variablePathList>
                </visible>
              </override>
              <override name="searchFormSection" type="elsect">
                <formReference type="varPath">
                  <variablePathList>
                    <variablePath>ui_com.conceptwave.system.BaseFinderUserInterface/uivar_searchForm</variablePath>
                  </variablePathList>
                </formReference>
                <variable type="varPath">
                  <variablePathList>
                    <variablePath>findSql_tfsReport.kpiAnalysisReportFinder/ui_UserInterface/uivar_search</variablePath>
                  </variablePathList>
                </variable>
                <visible type="varPath">
                  <variablePathList>
                    <variablePath>ui_com.conceptwave.system.FinderUserInterface/permMethod_showSearchForm</variablePath>
                  </variablePathList>
                </visible>
              </override>
            </overrideList>
          </overlay>
          <overlay name="Menu" type="exov">
            <base>ui_com.conceptwave.system.BaseFinderUserInterface/frmui_Forms/exov_Menu</base>
            <extends>ui_com.conceptwave.system.BaseFinderUserInterface/frmui_Forms/exov_Menu</extends>
            <label>Menu</label>
            <overrideList>
              <override name="cwfAdd" type="eldel"/>
              <override name="cwfCopy" type="eldel"/>
              <override name="cwfDelete" type="eldel"/>
              <override name="cwfDetail" type="eldel"/>
              <override name="cwfUpdate" type="eldel"/>
              <override name="finderMenuLayout" type="elext">
                <elementList>
                  <element name="showDetailsMenuItem" type="elmnu">
                    <clickMethod type="varPath">
                      <variablePathList>
                        <variablePath>findSql_tfsReport.kpiAnalysisReportFinder/ui_UserInterface/usrActionMethod_showDetails</variablePath>
                      </variablePathList>
                    </clickMethod>
                    <label>Show Details</label>
                  </element>
                </elementList>
              </override>
            </overrideList>
          </overlay>
        </overlayList>
      </vform>
    </vformList>
    <methodList>
      <method name="showDetails" type="usrActionMethod">
        <dialogHeight>768</dialogHeight>
        <dialogWidth>1600</dialogWidth>
        <parameterList>
          <parameter name="dialog" type="rifp">
            <type>ui_com.conceptwave.system.Dialog</type>
          </parameter>
        </parameterList>
        <showInPopup>true</showInPopup>
        <script><![CDATA[
          if(this.model.selected == null || this.model.selected.length == 0) {
              Global.throwException("UU0115");
          }

          if(this.model.selected.length > 1) {
              Global.throwException("UU0116");
          }

          var type = this.model.selected[0].type;
          var inputDocument = this.model.searchDocument;

          var finderName = null;

          if(type == "PRN_ONLY") {
              finderName = "tfsReport.kpiAnalysis_PRN_ONLY_Finder";
          }
          else if(type == "TFR_ONLY") {
              finderName = "tfsReport.kpiAnalysis_TFR_ONLY_Finder";
          }
          else if(type == "PRN_TFR_NO_BI") {
              finderName = "tfsReport.kpiAnalysis_PRN_TFR_NO_BI_Finder";
          }
          else {
              finderName = "tfsReport.kpiAnalysis_PRN_TFR_WITH_BI_Finder";
          }

          var detailFinder = new Finder(finderName);
          detailFinder.searchDocument.creationDateFrom = inputDocument.creationDateFrom;
          detailFinder.searchDocument.creationDateTo = inputDocument.creationDateTo;
          detailFinder.searchDocument.completionDateFrom = inputDocument.completionDateFrom;
          detailFinder.searchDocument.completionDateTo = inputDocument.completionDateTo;
          detailFinder.searchDocument.numDaysProvisioningPRN = inputDocument.numDaysProvisioningPRN;
          detailFinder.searchDocument.numDaysProvisioningTFR = inputDocument.numDaysProvisioningTFR;
          detailFinder.searchDocument.numDaysProvisioningPRN_TFR_noBI = inputDocument.numDaysProvisioningPRN_TFR_noBI;
          detailFinder.searchDocument.numDaysProvisioningPRN_TFR_BI = inputDocument.numDaysProvisioningPRN_TFR_BI;

          detailFinder.search();

          return detailFinder;
        ]]></script>
      </method>
    </methodList>
  </UserInterface>
  <viewList>
    <view name="kpiAnalysisReportFinderView" type="findVView">
      <inputOverlay>doc_tfsReport.kpiAnalysisReportSearchDoc/ui_UserInterface/frmui_Forms/exov_Default</inputOverlay>
      <label>KPI Analysis Report</label>
      <outputOverlay>findSql_tfsReport.kpiAnalysisReportFinder/ui_UserInterface/frmui_Forms/exov_Result</outputOverlay>
      <showSearchForm>true</showSearchForm>
      <sortListList>
        <sortList type="stitem">
          <path>doc_tfsReport.kpiAnalysisReportResultDoc/leaf_position</path>
        </sortList>
      </sortListList>
    </view>
  </viewList>
</findSql>