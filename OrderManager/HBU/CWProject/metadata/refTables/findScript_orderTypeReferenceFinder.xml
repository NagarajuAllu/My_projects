  <methodList>
    <method name="cwOnFinderSel" type="action">
      <guid>{0f5aedf1-f1b0-2b6f-8aad-20ddbe1d7027}</guid>
      <system>true</system>
      <script><![CDATA[
        var orderTypeDocs = new Array();

        var stdOrderTypes = ["I", "C", "T", "D", "E", "O"];
        for(var i=0; i<stdOrderTypes.length; i++) {
              var orderTypeDocument = new Document("refTables:orderTypeReferenceDoc");
              orderTypeDocument.orderType = stdOrderTypes[i];
              orderTypeDocs[orderTypeDocs.length] = orderTypeDocument;
        }

        var multipleOpsOrderTypes = refTables.getMultipleOpsOrderTypes();
        if(multipleOpsOrderTypes != null) {
              for(var i=0; i<multipleOpsOrderTypes.length; i++) {
                    var orderTypeDocument = new Document("refTables:orderTypeReferenceDoc");
                    orderTypeDocument.orderType = multipleOpsOrderTypes[i].orderType;
                    orderTypeDocs[orderTypeDocs.length] = orderTypeDocument;
              }
        }

        return orderTypeDocs;
      ]]></script>
    </method>
    <method name="editablePerm" type="permMethod">
