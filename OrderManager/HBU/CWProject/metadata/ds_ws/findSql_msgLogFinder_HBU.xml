  <methodList>
    <method name="cwOnFilter" type="action">
      <guid>{02249d69-c2de-47fc-0a79-637345e299ff}</guid>
      <system>true</system>
      <script><![CDATA[
        var result = true;

        // Modified on 16.11.2009:
        // - check if IF's name is equal to 'ifExpedtier' [returns 'true' to accept the record]
        if (input.operation != null) {
              // - search the position of ':' in the operation field
              var p_colon = input.operation.indexOf(":");
              // - search the position of '/' in the operation field
              var p_slash = input.operation.indexOf("/");
              var if_name = input.operation.substring(p_colon + 1, p_slash);
              var operation_name = input.operation.substr(p_slash + 1);
              result = ((if_name == "ifExpedtier_HOME") &&
                            (operation_name == "operation_SubmitOrder" ||
                             operation_name == "operation_UpdateOrder" ||
                             operation_name == "operation_QuoteValidation"));
        }

        return result;
      ]]></script>
    </method>
    <method name="cwOnFinderSQLSel" type="action">
      <guid>{0bba9a26-87ed-854d-7586-03204b646869}</guid>
      <system>true</system>
      <script><![CDATA[
        SELECT {[T1]/*+INDEX (cwmessagelog MSGLOG_CREATION)*/} {+*+}
        FROM cwmessagelog
        WHERE 1=1
        {[0] and USER_ID = ?}
        {[1,S1] and CREATION_TIME >= ?}
        {[2,S1] and CREATION_TIME <= ?}
        {[3] and USER_DATA1 = ?}
      ]]></script>
    </method>
