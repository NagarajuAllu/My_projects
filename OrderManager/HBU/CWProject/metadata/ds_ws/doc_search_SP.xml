    <methodList>
      <method name="runTrigger" type="usrActionMethod">
        <script><![CDATA[
          this.cw$super_runTrigger();
          Global.runContentDisplayScript(this, "displayScript");
        ]]></script>
      </method>
      <method name="ok" type="usrActionMethod">
        <allowInvalidObject>true</allowInvalidObject>
        <autosave>true</autosave>
        <closeUi>true</closeUi>
        <script>Global.confirmOK();</script>
      </method>
      <method name="ok_menu" type="usrActionMethod">
        <allowInvalidObject>true</allowInvalidObject>
        <confirmation>doc_ds_ws.search_SP/ui_UserInterface/action_cwAutosaveConfirmation</confirmation>
        <validate>true</validate>
        <script><![CDATA[
          var result = this.ok();
          Global.runContentDisplayScript(this, "displayScript");
          return result;
        ]]></script>
      </method>
      <method name="cwAutosaveConfirmation" type="action">
        <valueType>dtype_com.conceptwave.system.String</valueType>
        <script><![CDATA[
          if (!this.isValid && Global.isAutosaveEnabled() && !this.model.readonly && this.model.isSavable()) {
            return "UU0256:YN";
          }
          return null;
        ]]></script>
      </method>
      <method name="cancel" type="usrActionMethod">
        <allowInvalidObject>true</allowInvalidObject>
        <autosave>true</autosave>
        <closeUi>true</closeUi>
        <script>return Global.STOP_UI;</script>
      </method>
      <method name="cancel_menu" type="usrActionMethod">
        <allowInvalidObject>true</allowInvalidObject>
        <confirmation>doc_ds_ws.search_SP/ui_UserInterface/action_cwAutosaveConfirmation</confirmation>
        <validate>true</validate>
        <script><![CDATA[
          var result = this.cancel();
          if (result == Global.STOP_UI){
          if(this.model.owner instanceof Finder && this.model.owner.controllerOwner != null &&
             this.model.owner.controllerOwner.result != null) {
            this.model.owner.controllerOwner.result.updateList;
          }
            var parentWindow = this.getParentWindow();
            if (parentWindow instanceof com.conceptwave.system.Application) {
              parentWindow.content = null;
              result = null;
            }
          }
          Global.runContentDisplayScript(this, "displayScript");
          return result;
        ]]></script>
      </method>
    </methodList>
