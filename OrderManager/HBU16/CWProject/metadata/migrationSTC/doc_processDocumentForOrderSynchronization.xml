    <methodList>
      <method name="runTrigger" type="usrActionMethod">
        <script><![CDATA[
          this.cw$super_runTrigger();
          Global.runContentDisplayScript(this, "displayScript");
        ]]></script>
      </method>
      <method name="startProcessSynchronization" type="usrActionMethod">
        <autosave>true</autosave>
        <confirmation>doc_migrationSTC.processDocumentForOrderSynchronization/ui_UserInterface/action_startProcessSynchronization_confirm</confirmation>
        <script><![CDATA[
          var document = this.model;
          debugPrintln("[" + ds_ws.common_currentDateAsStringDDMMYYYYHH24MISS() + "] startProcessSynchronizationMenu - Starting process for synchronization");

          // checking if the process is already running
          var countProcessesFinder = new Finder("migrationSTC:countProcessesByStatusAndType");
          countProcessesFinder.searchDocument.TYPENAME = "migrationSTC:orderSynchronizationGraniteExpediter";
          countProcessesFinder.searchDocument.PROCESS_STATUS = 1; // process status = Running
          countProcessesFinder.search();

          if(countProcessesFinder.list.size > 0) {
                if(countProcessesFinder.list.start.result > 0) {
                      Global.throwException("AE0016");
                }
          }

          var processId = Process.startProcess("migrationSTC:orderSynchronizationGraniteExpediter",
                                                                 document,
                                                                 null);

          debugPrintln("[" + ds_ws.common_currentDateAsStringDDMMYYYYHH24MISS() + "] startProcessSynchronizationMenu - Started synchronization Process with id " + processId);
          Global.showUserMessage("Started synchronization Process with id " + processId);
        ]]></script>
      </method>
      <method name="startProcessSynchronization_confirm" type="action">
        <script><![CDATA[return "AW0002";]]></script>
      </method>
      <method name="startProcessSynchronization_menu" type="usrActionMethod">
        <allowInvalidObject>true</allowInvalidObject>
        <validate>true</validate>
        <script><![CDATA[
          if (!this.isValid) {
            var messages = this.validationErrors == null ? null : this.validationErrors.getErrorsAsString(10);
            if (messages == null)
              Global.throwException("UU0129", this);
            else
              Global.throwException("UU0147", "<br>" + messages);
          }
          var result = this.startProcessSynchronization();
          Global.runContentDisplayScript(this, "displayScript");
          return result;
        ]]></script>
      </method>
    </methodList>
  </UserInterface>
</document>