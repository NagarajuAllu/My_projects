            <label>Menu</label>
            <overrideList>
              <override name="hl1" type="elext">
                <elementList>
                  <element name="startProcessSynchronization" type="elmnu">
                    <clickMethod type="varPath">
                      <variablePathList>
                        <variablePath>doc_migrationSTC.processDocumentForOrderSynchronization/ui_UserInterface/usrActionMethod_startProcessSynchronization_menu</variablePath>
                      </variablePathList>
                    </clickMethod>
                    <label>Start Process Synchronization</label>
                    <menuType>5</menuType>
                    <showDown>true</showDown>
                    <showRollOver>true</showRollOver>
