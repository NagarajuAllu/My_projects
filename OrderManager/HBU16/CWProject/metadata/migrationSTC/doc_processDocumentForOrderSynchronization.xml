  <methodList>
    <method name="cwOnDocInit" type="action">
      <system>true</system>
      <script><![CDATA[
        this.MAX_NUMBER_OF_ORDERS = Global.getConfigVariable("MAX_NO_OF_ORDERS_FOR_MISSING_ORDER_CREATION", 1000);
        this.ORDER_TYPE           = Global.getConfigVariable("ORDER_TYPE_FOR_MISSING_ORDER_CREATION", "I");
        this.SUBMITTED_DATE_YEAR  = Global.getConfigVariable("SUBMITTED_YEAR_FOR_MISSING_ORDER_CREATION", "2012");
      ]]></script>
    </method>
    <method name="MAX_NUMBER_OF_ORDERS1" type="crule">
      <code>AE0019</code>
      <path>doc_migrationSTC.processDocumentForOrderSynchronization/leaf_MAX_NUMBER_OF_ORDERS</path>
      <severity>error</severity>
      <type>validator</type>
      <script><![CDATA[
        // 9999 is the max value returned by the finder
        return (document.MAX_NUMBER_OF_ORDERS > 9999);
      ]]></script>
    </method>
    <method name="SUBMITTED_DATE_YEAR1" type="crule">
      <code>AE0020</code>
      <path>doc_migrationSTC.processDocumentForOrderSynchronization/leaf_SUBMITTED_DATE_YEAR</path>
      <severity>error</severity>
      <type>validator</type>
      <script><![CDATA[return (document.SUBMITTED_DATE_YEAR < 2000 || document.SUBMITTED_DATE_YEAR > 2050);]]></script>
    </method>
  </methodList>
