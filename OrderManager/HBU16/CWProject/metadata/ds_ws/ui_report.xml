    <method name="action_createOrderSTC_Home" type="usrActionMethod">
      <allowInvalidObject>true</allowInvalidObject>
      <autosave>true</autosave>
      <validate>true</validate>
      <script><![CDATA[
        var order = new Order("ds_ws:default_orderSTC_HOME");
        order.orderMessage.actionOnOrder = "Create";
        order.orderMessage.orderStatus = "New";
        order.orderMessage.businessUnit = "Home";
        order.orderMessage.createdBy = "Expediter";
        order.orderMessage.cctType = "FTTH_LINK";
        order.orderMessage.creationDate = new Date();
        // Setting the Service Date as today + 7 days
        var today = new Date();
        var todayMS = today.getTime();
        var weekMS = 604800000;
        order.orderMessage.serviceDate = new Date(todayMS + weekMS);

        return order;
      ]]></script>
    </method>
    <method name="action_createOrderSTC_Home_menu" type="usrActionMethod">
      <script><![CDATA[
        var result = this.action_createOrderSTC_Home();
        Global.runContentDisplayScript(this, "displayScript");
        return result;
      ]]></script>
    </method>
