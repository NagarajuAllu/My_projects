                    <methodList>
                      <method name="cwOnProcActBefore" type="action">
                        <category>before</category>
                        <guid>{0e2ec29b-e81e-d801-baf6-000041d87ebd}</guid>
                        <system>true</system>
                        <script><![CDATA[
                          var debugHeader = "[" + this.process.id + "] - UpdateOrder - ";
                          debugPrintln(stcc.getSysdateForLog() + debugHeader + "Start UpdateOrder ... removing last version...");

                          stcw.createRejectedOrder(this.process.processOrder.header.orderNumber,
                                                               "Granite",
                                                               this.process.processDocument.errorText);

                          var updateOrder = this.process.processOrder;
                          stcw.deleteLastVersionFolder(updateOrder);
                          updateOrder.save();

                          debugPrintln(stcc.getSysdateForLog() + debugHeader + "UpdateOrder Archived...");
                        ]]></script>
