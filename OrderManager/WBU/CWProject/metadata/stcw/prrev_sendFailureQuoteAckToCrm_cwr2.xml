        <methodList>
          <method name="cwOnProcActBefore" type="action">
            <category>before</category>
            <guid>{048d959e-cf0e-850c-7962-dc4eff45aca4}</guid>
            <system>true</system>
            <script><![CDATA[
              var debugHeader = "[" + this.process.id + "] - SendQuoteAck - ";

              var processDocument = this.process.processDocument;
              processDocument.exceptionType = null;

              try {
                    // extracting Granite response must be forward to EAI
                    var requestEl = new DataStructure("siquoAck:processRequest_el");
                    requestEl.Request.BusinessUnit     = "Wholesale";
                    requestEl.Request.ErrorCode        = processDocument.ErrorCode;
                    requestEl.Request.ErrorDescription = stcc.limitStringLengthToCRMValue(processDocument.ErrorText);
                    requestEl.Request.ErrorStatus      = "ERROR";
                    requestEl.Request.ErrorType        = "FUNCTIONAL";
                    requestEl.Request.QuoteNumber      = processDocument.quoteNumber;
                    //requestEl.Request.orderRowItemId   = processDocument.orderRowItemId;

                    // check payload size
                    stcc.checkPayloadToCRMSize(requestEl, this.process.processDocument.interOperationName);

                    // sending data to EAI
                    var response = stcw.sendQuoteAckToEAI(requestEl, this.process.id);

                    if (response == null) {
                          processDocument.exceptionType = "Interface";
                    }
              }
              catch(exc) {
              debugPrintln(stcc.getSysdateForLog() + debugHeader + "Catched Exception " + exc.message);
                          processDocument.exceptionType = "Application";
                    }
            ]]></script>
          </method>
        </methodList>
      </child>
