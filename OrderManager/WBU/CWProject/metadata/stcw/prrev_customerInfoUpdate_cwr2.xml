        <methodList>
          <method name="cwOnProcActBefore" type="action">
            <category>before</category>
            <guid>{0ac8f215-7d44-fb90-e5a3-32f8b025e5ce}</guid>
            <system>true</system>
            <script><![CDATA[
              rpts.updateHistoryEvent (this, "expediterValidation");

              var crmInput = stcw.findGenericProcessMessage (this.process.id, this.process.processDocument.activityName, "wsws:customerInformationUpdate");

              // Applying Validation Rules
              var error = null;
              if (crmInput.businessUnit != "Wholesale") {
                    error = (util.createError ("InvalidBusinessUnit", crmInput.businessUnit));
              }
              else {
                    var validationResult = crmInput.validate(1, true);
                    if(validationResult != null) {
                          error      = new DataStructure ("util:Error");
                          error.code = validationResult[0];
                          error.text = validationResult[1];
                    }
              }

              this.process.processDocument.activityName = "EXPEDITER_VALIDATION";
              var response = new DataStructure("grws:customerInformationUpdateResponse");
              var result = response.result;
              result.AccountNumber           = crmInput.accountNumber;
              result.BusinessUnit            = crmInput.businessUnit;

              if(error != null) {
                    // error, so setting additional fields Data Structure
                    result.Status.ErrorCode        = error.code;
                    result.Status.ErrorDescription = stcc.limitStringLengthToCRMValue(error.text);
                    result.Status.ErrorTime        = stcc.getSysdateInYYYYMMDD_HH24MMSS();
                    result.Status.ErrorType        = "FUNCTIONAL";
                    result.Status.SystemName       = "Expediter";
                    result.Status.Status           = "ERROR";
              }
              else {
                    result.Status.Status           = "SUCCESS";
              }

              this.process.processDocument.ackResult = result.Status.Status;
              stcw.storeGenericProcessMessage (this.process.id, this.process.processDocument.activityName, response);
            ]]></script>
          </method>
