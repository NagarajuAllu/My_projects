    <methodList>
      <method name="archiveOrders" type="usrActionMethod">
        <allowInvalidObject>true</allowInvalidObject>
        <autosave>true</autosave>
        <confirmation>findOrder_stcw.orderToArchiveFinder/ui_UserInterface/action_archiveOrders_confirm</confirmation>
        <script><![CDATA[
          var selectList = this.result.selected;
                if(selectList == null || selectList.length == 0) {
                      Global.throwException("AE0003 ");
                }

                for(var i=0; i<selectList.length; i++) {
                      var selectedOrder = Order.getOrderById(selectList[i].cwOrderId);
                      stcw.archiveOrder(selectedOrder, false);
                }

                return Global.showUserMessage("AI0001");
        ]]></script>
      </method>
      <method name="archiveOrders_confirm" type="action">
        <script><![CDATA[
          var selectList = this.result.selected;
                if(selectList == null || selectList.length == 0) {
                      Global.throwException("AE0003");
                }

                return "AW0002";
