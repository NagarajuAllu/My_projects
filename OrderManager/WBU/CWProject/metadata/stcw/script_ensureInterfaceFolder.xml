<script filenameHash="5784164ec713bf135ded53af96495be5" versionHash="eccbc87e4b5ce2fe28308fd9f2a7baf3" name="stcw.ensureInterfaceFolder">
  <guid>{0040a8c2-d849-d138-8f62-0e8495e59c3d}</guid>
  <label>Ensure Interface Folder</label>
  <metaVersion>3</metaVersion>
  <parameterList>
    <parameter name="order" type="rifp">
      <mandatory>true</mandatory>
      <type>order_com.conceptwave.system.Order</type>
    </parameter>
    <parameter name="interfaceName" type="rifp">
      <mandatory>true</mandatory>
      <type>dtype_stcw.InterfaceName</type>
    </parameter>
    <parameter name="versionNumber" type="rifp">
      <type>dtype_com.conceptwave.system.Integer</type>
    </parameter>
  </parameterList>
  <valueType>nmeta_com.conceptwave.system.Object</valueType>
  <script><![CDATA[
    var interfaceFolder = stcw.findLastInterfaceFolder (order, interfaceName, versionNumber);
    if (interfaceFolder != null)
          return (interfaceFolder);

    return (stcw.createInterfaceFolder (order, interfaceName, versionNumber));
  ]]></script>
</script>