      <child name="terminateOldProcesses" type="scriptActivity">
        <guid>{026ff7a1-7575-6641-96cf-db9f56ce2fa1}</guid>
        <label>Terminate Old Processes</label>
        <x>324.83063</x>
        <y>389.20703</y>
        <methodList>
          <method name="cwOnProcActBefore" type="action">
            <category>before</category>
            <guid>{0edecd48-1148-3ca0-5265-5447fbd457f8}</guid>
            <system>true</system>
            <script><![CDATA[
              if(this.process.processDocument.ackResult == "SUCCESS") {
                    // check if it's an Add or not ...
                    var quoteOrder = this.process.processOrder;
                    if(quoteOrder.versions.length > 1) {
                          // it's an update install quote
                          var processCount = stcc.terminateAllProcesses (this.process.processOrder.header.cwOrderId, "stcw:validateQuote", this.process.id);
                          debugPrintln(stcc.getSysdateForLog() + "[" + this.process.id + "] - ValidateQuote - terminated " + processCount + " processes");

                          quoteOrder.header.quoteStatus = quoteOrder.header.orderAction;

                          // resetting Completed Date
                          quoteOrder.header.completedDate = null;
                          quoteOrder.save();
                    }
              }
            ]]></script>
          </method>
        </methodList>
      </child>
