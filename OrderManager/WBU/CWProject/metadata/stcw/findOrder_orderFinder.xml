    <methodList>
      <method name="synchronizeSelectedOrdersWithGranite" type="usrActionMethod">
        <allowInvalidObject>true</allowInvalidObject>
        <autosave>true</autosave>
        <confirmation>findOrder_stcw.orderFinder/ui_UserInterface/action_synchronizeSelectedOrdersWithGranite_confirm</confirmation>
        <script><![CDATA[
          var selectList = this.result.selected;
                var debugHeader = " synchronizeSelectedOrders - ";

                if(selectList != null && selectList.length > 0) {
          debugPrintln(stcc.getSysdateForLog() + debugHeader + "Selected " + selectList.length +  " orders");

                      for(var i=0; i<selectList.length; i++) {
          debugPrintln(stcc.getSysdateForLog() + debugHeader + "Processing order number " + (i+1) + "/" + selectList.length);

                            var cwOrderIdSelected = selectList[i].cwOrderId;
                            var selectedOrder = Order.getOrderById(selectList[i].cwOrderId);

          debugPrintln(stcc.getSysdateForLog() + debugHeader + "cwOrderIdSelected " + cwOrderIdSelected + "; orderNumber = " + selectedOrder.header.orderNumber);

                            // check if it's a rejected order
                            var positionRejection = (selectedOrder.header.orderNumber.search("/*_201[0-9]/[0-9][0-9]/[0-9][0-9]_ErrGran"));
                            var isRejected = (positionRejection != -1);

                            if(isRejected) {
                                  var processDocument = new Document("stcw:syncRejectedOrderProcessDocument");
                                  var originalOrderNumber = selectedOrder.header.orderNumber.substring(0, positionRejection);

                                  processDocument.currentRejectedOrderId = selectedOrder.id;
                                  processDocument.originalOrderNumber    = originalOrderNumber;

                                  var processId = Process.startProcess("stcw:syncRejectedOrderBetweenExpdAndGranite",
                                                                                         processDocument,
                                                                                         null);

          debugPrintln(stcc.getSysdateForLog() + debugHeader + "Started RejectedSyncOrder Process with id " + processId + " and process document " + processDocument.toXML());
                            }
                            else {
                                  var processOrderFinder = new Finder ("xcutl:processForOrderFinder");
                                  processOrderFinder.searchDocument.orderId = cwOrderIdSelected;
                                  processOrderFinder.searchDocument.processStatus = 1;
                                  processOrderFinder.search();

                                  var processList = processOrderFinder.list;
                                  if (processList != null && processList.size > 0) {
                                        var processDocument = new Document("stcw:syncOrderQuoteProcessDocument");
                                        processDocument.errorFound = false;

                                        if(processList.size == 1) {
                                              processDocument.currentProcessId = processList[0].PROCESS_ID;
                                        }
                                        else {
                                              // found more than 1 process running for the same order ... it's due to update order management (there can be more than 1 process while
                                              // the new instance of the order is not in granite ...
                                              var tmpProcessId = -1;
                                              for (var i = 0; i < processList.size; i++) {
                                                  processDocument = processList[i];
          debugPrintln(stcc.getSysdateForLog() + debugHeader + "Found process running with id " + processDoc.PROCESS_ID);
                                                    if(processDoc.PROCESS_ID > tmpProcessId) {
                                                          tmpProcessId = processDoc.PROCESS_ID;
          debugPrintln(stcc.getSysdateForLog() + debugHeader + "New Candidate ProcessId " + tmpProcessId);
                                                    }
                                              }
                                              processDocument.currentProcessId = tmpProcessId;
                                        }


                                        var processId = Process.startProcess("stcw:syncOrderBetweenExpdAndGranite",
                                                                                               processDocument,
                                                                                               null);

          debugPrintln(stcc.getSysdateForLog() + debugHeader + "Started syncOrder Process with id " + processId + " and process document " + processDocument.toXML());
                                  }
                                  else {
          debugPrintln(stcc.getSysdateForLog() + debugHeader + "Unable to find process that is managing the order " + selectedOrder.header.orderNumber);
                                  }
                            }
                      }
                }
        ]]></script>
      </method>
      <method name="synchronizeSelectedOrdersWithGranite_confirm" type="action">
        <script><![CDATA[
          var selectList = this.result.selected;
                if(selectList == null || selectList.length == 0) {
                      Global.throwException("AE0003");
                }

                for(var i=0; i<selectList.length; i++) {
                      var cwOrderIdSelected = selectList[i].cwOrderId;
                      var selectedOrder = Order.getOrderById(selectList[i].cwOrderId);

                      if(selectedOrder.header.orderStatus == "COMPLETED" || selectedOrder.header.orderStatus == "CANCELLED") {
                            Global.throwException("AE0005");
                      }
                }


                return "AW0005";
        ]]></script>
