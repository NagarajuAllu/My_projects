                <methodList>
                  <method name="cwOnProcActBefore" type="action">
                    <category>before</category>
                    <guid>{0bbe818f-056e-52b6-73ff-be0505c3ac20}</guid>
                    <system>true</system>
                    <script><![CDATA[
                      debugPrintln(stcc.getSysdateForLog() + "["+ process.id + "] - Sending ValidateQuote To Granite: Retrying ...");
                    ]]></script>
