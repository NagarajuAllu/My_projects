                <guid>{0868c00d-d2cd-8d80-e138-401a4a0f3578}</guid>
                <system>true</system>
                <script><![CDATA[
                  var debugHeader = "[" + this.process.id + "] Handle I/F Error - ";

                  var processDocument = this.process.processDocument;
                  var retryDocument = stcc.getInterfaceRetryDocument (processDocument.interOperationName);
                  if (retryDocument == null) {
                        Global.logDebug (debugHeader + "could not get the retry document");
                  debugPrintln(debugHeader + "could not get the retry document");
                              return (false);
                        }

                  debugPrintln(stcc.getSysdateForLog() + debugHeader + "processDocument.interfaceRetryCount = " + processDocument.interfaceRetryCount +
                                     "; retryDocument.interfaceRetryCount = " + retryDocument.interfaceRetryCount);

                        return (processDocument.interfaceRetryCount < retryDocument.interfaceRetryCount);
                ]]></script>
              </method>
