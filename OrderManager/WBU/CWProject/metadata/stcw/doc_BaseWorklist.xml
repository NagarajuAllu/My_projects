      <method name="customWorklist$cwCalculate_Details" type="action">
        <system>true</system>
        <valueType>dtype_com.conceptwave.system.String</valueType>
        <script><![CDATA[
          if(this.model.OrderId == null || this.model.OrderId == "0") {
                // process and worklist are not related to Orders, so it could be related to
                // - CustomerInformationUpdate
                // - SiteInformationUpdate
                // - Restoration
                // Decode it from process type
                var details = "";
                if(this.model.SenderType != null) {
                      var sender = Global.getMetadataNameByIndex(this.model.SenderType);

                      if(sender.indexOf("customer") != -1) {
                            details = "Customer Information Update";
                      }
                      else if(sender.indexOf("site") != -1) {
                            details = "Site Information Update";
                      }
                      else if(sender.indexOf("restoration") != -1) {
                            details = "Restoration";
                      }
                }
                return (details);
          }

          var order = Order.getOrderById (this.model.OrderId);
          if (order == undefined || order == null)
                return ("");

          var orderItem = order.findItemById (this.model.orderItemId);
          if (orderItem == undefined || orderItem == null)
                return ("unknwon");

          return (Global.getCodeDescription ("stcw:InterfaceName", orderItem.interfaceName, null));
        ]]></script>
      </method>
      <method name="customWorklist$cwCalculate_Process_Type" type="action">
        <system>true</system>
        <valueType>dtype_com.conceptwave.system.String</valueType>
        <script><![CDATA[
          var sender = "";
          if(this.model.SenderType != null)
                sender = Global.getMetadataNameByIndex(this.model.SenderType);

          return sender;
        ]]></script>
      </method>
