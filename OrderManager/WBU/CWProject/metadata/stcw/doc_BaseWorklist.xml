    <method name="cwOnDocStore" type="action">
      <guid>{06cef963-6e61-aa9c-0135-5c8552a58937}</guid>
      <system>true</system>
      <script><![CDATA[
        this.cw$super();

        debugPrintln("Worklist.OrderId = " + this.OrderId);

              if(this.OrderId != null && this.OrderId > 0) {
                    try {
                          var order = Order.getOrderById(this.OrderId);

                          if (order.metadataType == "stcw:wholesaleOrder") {
                                this.orderNumber = order.header.orderNumber;
                          }
                          else if(order.metadataType == "stcw:wholesaleQuote") {
                                this.orderNumber = order.header.quoteNumber;
                          }
                    }
                    catch(exc) {
                          // do nothing
                    }
              }
      ]]></script>
    </method>
