      <method name="cwOnGetFormFieldValue_customWorklist_Details" type="action">
        <parameterList>
          <parameter name="document" type="rifp">
            <mandatory>true</mandatory>
            <type>doc_stcw.BaseWorklist</type>
          </parameter>
        </parameterList>
        <valueType>dtype_com.conceptwave.system.String</valueType>
        <script><![CDATA[
          if(document.OrderId == null || document.OrderId == "0") {
                // process and worklist are not related to Orders, so it could be related to
                // - CustomerInformationUpdate
                // - SiteInformationUpdate
                // - Restoration
                // Decode it from process type
                var details = "";
                if(document.SenderType != null) {
                      var sender = Global.getMetadataNameByIndex(document.SenderType);

                      if(sender.indexOf("customer") != -1) {
                            details = "Customer Information Update";
                      }
                      else if(sender.indexOf("site") != -1) {
                            details = "Site Information Update";
                      }
                      else if(sender.indexOf("restoration") != -1) {
                            details = "Restoration";
                      }
                }
                return (details);
          }

          var order = Order.getOrderById (document.OrderId);
          if (order == undefined || order == null)
                return ("");

          var orderItem = order.findItemById (document.orderItemId);
          if (orderItem == undefined || orderItem == null)
                return ("unknwon");

          return (Global.getCodeDescription ("stcw:InterfaceName", orderItem.interfaceName, null));
        ]]></script>
      </method>
      <method name="cwOnGetFormFieldValue_customWorklist_Process_Type" type="action">
        <parameterList>
          <parameter name="document" type="rifp">
            <mandatory>true</mandatory>
            <type>doc_stcw.BaseWorklist</type>
          </parameter>
        </parameterList>
        <valueType>dtype_com.conceptwave.system.String</valueType>
        <script><![CDATA[
          var sender = "";
          if(document.SenderType != null)
                sender = Global.getMetadataNameByIndex(document.SenderType);

          return sender;
        ]]></script>
      </method>
      <method name="_1" type="action">
        <parameterList>
