        <methodList>
          <method name="cwOnProcActBefore" type="action">
            <category>before</category>
            <guid>{048d959e-cf0e-850c-7962-dc4eff45aca4}</guid>
            <system>true</system>
            <script><![CDATA[
              var debugHeader = "[" + this.process.id + "] - SendCustomerInfoAck - ";

              var processDocument = this.process.processDocument;
              processDocument.exceptionType = null;

              try {
                    // extracting Granite response must be forward to EAI
                    var graniteResponse = stcw.findGenericProcessMessage(this.process.id, processDocument.activityName, "grws:customerInformationUpdateResponse");

                    var request = new DataStructure("sicustack:processRequest_el");

              debugPrintln(stcc.getSysdateForLog() + debugHeader + "graniteResponse = '" + graniteResponse.toXML() + "'");

                          stcw.map_graniteCustomerInfoAck_CRMCustomerInfoAck (request, graniteResponse);

                          // check payload size
                          stcc.checkPayloadToCRMSize(request, this.process.processDocument.interOperationName);

                          // sending data to EAI
                          var response = stcw.sendCustomerInfoAckToEAI(request, this.process.id);

                          // managing response
                          rpts.updateHistoryEvent (this);

                          var activityName = "CRM_ACK";

                          if (response == null) {
                                processDocument.exceptionType = "Interface";
                          }
                          else {
                                processDocument.activityName = activityName;
                                stcw.storeGenericProcessMessage(this.process.id, activityName, response);
                          }

                    }
                    catch(exc) {
              debugPrintln(stcc.getSysdateForLog() + debugHeader + "Catched Exception " + exc.message);
                          processDocument.exceptionType = "Application";
                    }
            ]]></script>
          </method>
