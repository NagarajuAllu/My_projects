            <category>cond</category>
            <guid>{0e0c96bc-b526-951c-0145-9f100bd2a09c}</guid>
            <system>true</system>
            <script><![CDATA[
              rpts.createHistoryEvent (this);
              return (true);
            ]]></script>
          </method>
