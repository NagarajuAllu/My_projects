      <method name="searchResult$cwCalculate_Process_Status" type="action">
        <system>true</system>
        <valueType>dtype_com.conceptwave.system.String</valueType>
        <script><![CDATA[
          var status = null;
          switch(this.model.processStatus) {
                case 1:
                      status = "Executing";
                      break;
                case 2:
                      status = "Suspended";
                      break;
                case 3:
                      status = "Completed";
                      break;
                case 4:
                      status = "Aborted";
                      break;
                case 5:
                      status = "Error";
                      break;
                case 6:
                      status = "Terminated";
                      break;
                default:
                      status = "Not Started";
                      break;
          }

          return status;
        ]]></script>
      </method>
