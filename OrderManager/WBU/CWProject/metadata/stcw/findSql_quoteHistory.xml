  <methodList>
    <method name="cwOnFinderSQLSel" type="action">
      <guid>{07f6dd25-01ac-2f28-a20a-d6811ca26f5b}</guid>
      <system>true</system>
      <script><![CDATA[
        SELECT h.cwdocid, o.cwOrderId, order_number, q.quotenumber, reservationnumber, p.process_id, p.status, p.process_metadatype,
               p.metadatatype_ver, p.revision, h.activity_path, e.activity_description,
               msgid, m.creation_time, m.receive_time, m.send_time
          FROM orb_order_header o, stcw_quote_header q, rpt_history_event h, cwmessagelog m, cwprocess p, ref_history_events e
         WHERE o.cwdocid = q.cwdocid
           AND o.cworderid = h.order_id
           AND m.msgid (+) = h.message_id
           AND p.process_id = h.process_id
           AND h.activity_path like e.activity_path
        {[0] AND o.order_number = ?}
        {[1] AND q.quotenumber = ?}
        ORDER BY NVL(m.creation_time, h.lastupdateddate) , process_id, h.cwdocid
      ]]></script>
    </method>
    <method name="editablePerm" type="permMethod">
      <script>return this.enabled;</script>
    </method>
