    <methodList>
      <method name="cwOnGetFormFieldValue_searchResult_Process_Status" type="action">
        <parameterList>
          <parameter name="document" type="rifp">
            <mandatory>true</mandatory>
            <type>doc_stcw.quoteHistoryResult</type>
          </parameter>
        </parameterList>
        <valueType>dtype_com.conceptwave.system.String</valueType>
        <script><![CDATA[
          var status = null;
          switch(document.processStatus) {
                case 1:
                      status = "Executing";
                      break;
                case 2:
                      status = "Suspended";
                      break;
                case 3:
                      status = "Completed";
                      break;
                case 4:
                      status = "Aborted";
                      break;
                case 5:
                      status = "Error";
                      break;
                case 6:
                      status = "Terminated";
                      break;
                default:
                      status = "Not Started";
                      break;
          }

          return status;
        ]]></script>
      </method>
      <method name="processActivities" type="usrActionMethod">
        <allowInvalidObject>true</allowInvalidObject>
        <autosave>true</autosave>
        <confirmation>findSql_stcw.quoteHistory/ui_UserInterface/action_processActivities_confirm</confirmation>
        <dialogHeight>600</dialogHeight>
        <dialogWidth>800</dialogWidth>
        <parameterList>
          <parameter name="cwDialogParam" type="rifp">
            <type>ui_com.conceptwave.system.Dialog</type>
          </parameter>
        </parameterList>
        <showInPopup>true</showInPopup>
        <script><![CDATA[
          cwDialogParam.title = "Process Activities";

          var selectList = this.result.selected;
          // var processDoc = new cwf_pm.CWPROCESS.UserInterface(selectList[0], this);
          // var ui = new cwf_pm.ActivityFinder.UserInterface(null,processDoc);
          var ui = new cwf_pm.ActivityFinder.UserInterface(null,this);

          if(selectList[0].processStatus == 3 || selectList[0].processStatus == 6) {
              ui = new cwf_pm.ActivityArchiveFinder.UserInterface(null, this);
          }
          ui.search.model.PROCESS_ID = this.result.selected[0].processId;
          ui.searchAction();
          return ui;
        ]]></script>
      </method>
