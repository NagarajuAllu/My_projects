      <method name="cwOnGetFormFieldValue_customSharedSelect_Details" type="action">
        <parameterList>
          <parameter name="document" type="rifp">
            <mandatory>true</mandatory>
            <type>doc_stcw.BaseWorklist</type>
          </parameter>
        </parameterList>
        <valueType>dtype_com.conceptwave.system.String</valueType>
        <script><![CDATA[
          var order = Order.getOrderById (document.OrderId);
          if (order == undefined || order == null)
                return ("unknown");

          var orderItem = order.findItemById (document.orderItemId);
          if (orderItem == undefined || orderItem == null)
                return ("unknwon");

          return (Global.getCodeDescription ("stcw:InterfaceName", orderItem.interfaceName, null));
        ]]></script>
      </method>
      <method name="cwfSelectTask" type="usrActionMethod">
        <script><![CDATA[
          var sel = this.result.selected;
          if(sel==null || sel.length<1){
                Global.throwException('UU0115');
          } else{
                var updateSize = Global.getWorkListFinder().selectTasks(sel);
                if(updateSize==0)
                       Global.throwException('UU0135', sel[0].cwDocId);
                this.parent.owner.cwRefreshWL();
                Global.closeDialog(this.parent);
          }
        ]]></script>
      </method>
      <method name="cwfSelectTask_menu" type="usrActionMethod">
        <script><![CDATA[
          var result = this.cwfSelectTask();
          if(result instanceof Finder) {
            Global.runContentDisplayScript(result.searchDocument, "displayScript");
          } else {
            Global.runContentDisplayScript(this, "displayScript");
          }

          return result;
        ]]></script>
      </method>
