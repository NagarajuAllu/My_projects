    <method name="cwOnFinderSQLSel" type="action">
      <guid>{0e559cc1-70b2-5e98-0274-c26e47401f34}</guid>
      <system>true</system>
      <script><![CDATA[
        SELECT COUNT(*),
               COUNT(CASE WHEN decoded_status = 'COMPLETED' THEN 1 ELSE NULL END) AS "COMPLETED",
               COUNT(CASE WHEN decoded_status = 'OPEN' THEN 1 ELSE NULL END) AS "OPEN",
               COUNT(CASE WHEN decoded_status = 'REJECTED' THEN 1 ELSE NULL END) AS "REJECTED"
          FROM (SELECT DECODE(UPPER(NVL(quote_status, 'X')), 'CANCELLED', 'COMPLETED',
                                                             'ARCHIVED',  'COMPLETED',
                                                             'COMPLETED', 'COMPLETED',
                                                             'OPEN') decoded_status,
                       s.cwdocid
                  FROM stcw_quote_header s, orb_order_header o
                 WHERE s.cwdocid = o.cwdocid
                   AND NVL(quote_status, 'X') <> 'REJECTED'
              {[0] AND o.cwordercreationdate >= ?}
              {[1] AND o.cwordercreationdate <= ?}
                 UNION
                SELECT 'REJECTED',
                       cwdocid
                  FROM stcw_rejected_quote
                 WHERE 1=1
              {[0] AND rejecteddatetime >= ?}
              {[1] AND rejecteddatetime <= ?})
      ]]></script>
    </method>
    <method name="editablePerm" type="permMethod">
      <script>return this.enabled;</script>
    </method>
