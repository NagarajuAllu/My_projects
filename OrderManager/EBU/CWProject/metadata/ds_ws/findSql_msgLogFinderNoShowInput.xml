      <method name="searchAction" type="action">
        <parameterList>
          <parameter name="doValidate" type="rifp">
            <type>dtype_com.conceptwave.system.Boolean</type>
          </parameter>
        </parameterList>
        <script><![CDATA[
          var finder = this.model;
          var originalOperation = null;
          var searchDoc = this.search==null?(this.model.searchDocument==null?null:this.model.searchDocument):this.search.model;
          if(searchDoc.operation!=null) { // For backwards compatibility, operation name changed from ":"->"."
            var index = searchDoc.operation.lastIndexOf(".");
            if(index!=-1){
              originalOperation =   searchDoc.operation;
              searchDoc.operation =  searchDoc.operation.substr(0, index) + ':' +searchDoc.operation.substr(index + 1);
            }
          }

          this.model.cwOnInitSearch(searchDoc);
          if(this.search!=null && this.searchForm!=null && (doValidate==true || doValidate==null)){
                var searchValidation = this.search.validate(5);
                if(searchValidation==null || searchValidation.size() == 0){
                      this.result = finder.invoke(searchDoc, "select");
                }else{
                      Global.throwException("UU0208");
                }
          }else{
                this.result = finder.invoke(searchDoc, "select");
          }

          if(originalOperation != null){ //reset operation name to original to avoid any side-effect, the "."->":" should only be used in this finder.
              searchDoc.operation =originalOperation;
          }
        ]]></script>
