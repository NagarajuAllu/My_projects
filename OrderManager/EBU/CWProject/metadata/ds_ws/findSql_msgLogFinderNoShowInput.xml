    <method name="cwOnFinderSQLSel" type="action">
      <system>true</system>
      <script><![CDATA[
        SELECT {[T1]/*+INDEX (cwmessagelog MSGLOG_CREATION)*/} {+*+}
        FROM cwmessagelog
        WHERE 1=1
        {[0] and MSGID = ?}
        {[1] and VMID = ?}
        {[2] and INTER_TYPE = ?}
        {[3] and OPERATION = ?}
        {[4] and USER_ID = ?}
        {[5,S1] and CREATION_TIME >= ?}
        {[6,S1] and CREATION_TIME <= ?}
        {[8] and USER_DATA1 = ?}
        {[9] and USER_DATA2 = ?}
        order by CREATION_TIME DESC
      ]]></script>
    </method>
