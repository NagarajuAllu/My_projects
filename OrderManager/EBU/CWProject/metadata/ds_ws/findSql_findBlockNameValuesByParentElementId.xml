  <dbSchema>ORDER</dbSchema>
  <extends>find_com.conceptwave.system.Finder</extends>
  <input>doc_cwt_common.docID</input>
  <label>Find BlockNameValues Document By ParentElementId</label>
  <maxRowsLimit>5000</maxRowsLimit>
  <metaVersion>4</metaVersion>
  <output>doc_ds_ws.blockNameWithAllValuesDocument</output>
  <type>SQL</type>
  <volatility>1</volatility>
  <methodList>
    <method name="cwOnFinderSQLSel" type="action">
      <system>true</system>
      <script><![CDATA[
        select bnv.name, listagg(bv.blockvalue, ';') within group (order by bv.cwdocid), bnv.action, bnv.parentelementid, bnv.cwdocid
          from stc_block_name_value bnv, stc_block_value bv
         where bv.parentdocid = bnv.cwdocid
        {[0] and bnv.parentelementid = ?}
        group by bnv.name, bnv.action, bnv.parentelementid, bnv.cwdocid
      ]]></script>
    </method>
  </methodList>
