  <methodList>
    <method name="cwOnDocStore" type="action">
      <system>true</system>
      <script><![CDATA[
        this.cw$super_cwOnDocStore();

        if(ds_ws.isNameInSkipNVPairList(this.name)) {
            Global.throwException("AE0118", [this.name, "Skip"]);
        }

        if(ds_ws.isNameInStoreAndForwardNVPairList(this.name)) {
            Global.throwException("AE0118", [this.name, "Store & Forward"]);
        }

        if(ds_ws.isNameInNewAttrNVPairList(this.name)) {
            Global.throwException("AE0118", [this.name, "New Attribute"]);
        }

        if(this.isStored) {
            Global.showUserMessage(Global.translateText("AI0002"));
        }
        else {
            Global.showUserMessage(Global.translateText("AI0001"));
        }
      ]]></script>
    </method>
    <method name="addPerm" type="permMethod">
      <privilegesList>
        <privileges type="ppriv">
