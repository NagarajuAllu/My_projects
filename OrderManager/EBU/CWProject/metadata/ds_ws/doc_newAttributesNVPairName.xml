  <label>NewAttribute NV Pair Name</label>
  <metaVersion>4</metaVersion>
  <variableList>
    <variable name="name" type="leaf">
      <column>NAME</column>
      <key>1</key>
      <propertyElement type="elfld">
        <label>Name in NV Pair</label>
        <optional type="varPath">
          <variablePathList>
            <variablePath>dtype_com.conceptwave.system.Boolean/leaf_FALSE</variablePath>
          </variablePathList>
        </optional>
      </propertyElement>
      <table>STC_NEWATTR_NVPAIR</table>
