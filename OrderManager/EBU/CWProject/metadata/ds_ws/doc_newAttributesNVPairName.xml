      </privilegesList>
      <privilegesType>SOME</privilegesType>
      <script>this.cw$super_addPerm($psCondition);</script>
    </method>
    <method name="deletePerm" type="permMethod">
      <privilegesList>
        <privileges type="ppriv">
          <privilege>STC_Config</privilege>
        </privileges>
        <privileges type="ppriv">
          <privilege>UPAdmin</privilege>
        </privileges>
      </privilegesList>
      <privilegesType>SOME</privilegesType>
      <script>this.cw$super_deletePerm($psCondition);</script>
    </method>
    <method name="editablePerm" type="permMethod">
      <script><![CDATA[
        var isEditable = this.cw$super_editablePerm($psCondition);

        if(isEditable) {
            isEditable = ((this.name == null) || (this.name.trim().length == 0));
        }

        return isEditable;
      ]]></script>
    </method>
  </methodList>
