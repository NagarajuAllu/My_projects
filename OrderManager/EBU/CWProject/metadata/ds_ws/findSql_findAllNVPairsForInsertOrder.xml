  <methodList>
    <method name="cwOnFinderSQLSel" type="action">
      <system>true</system>
      <script><![CDATA[
        select r.cwdocid,
               d.name,
               r.value,
               (select h.cwdocid
                  from STC_INSERTORDER_HEADER h
                 where 1=1
                {[0] and h.cwdocid = ?}) header_id,
               d.mandatory,
               d.mandatory_on,
               d.mandatory_on_value,
               d.value_type,
               d.values_for_lov,
               d.finder_name
          from STC_NVPAIR_4_GUI_CONFIG d, STC_INSERTORDER_NVPAIR r
         where d.name = r.name (+)
        {[0] and r.header_id (+) = ?}
        order by d.name
      ]]></script>
    </method>
