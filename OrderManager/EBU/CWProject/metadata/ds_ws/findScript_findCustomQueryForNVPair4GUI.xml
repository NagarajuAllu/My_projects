<?xml version="1.0" encoding="UTF-8" ?>
<findScript filenameHash="aa609a8fd61ef9d056a369e4fb611ea1" versionHash="a87ff679a2f3e71d9181a67b7542122c" name="ds_ws.findCustomQueryForNVPair4GUI">
  <extends>find_com.conceptwave.system.Finder</extends>
  <input>doc_common.genericInput_String</input>
  <label>Find With Custom Query For NV Pair For Insert Order From GUI</label>
  <maxRowsLimit>9999</maxRowsLimit>
  <metaVersion>4</metaVersion>
  <output>doc_common.genericOutput_String</output>
  <type>script</type>
  <vDetailForm>ui_com.conceptwave.system.DocumentUserInterface/frmui_Forms/exov_Default</vDetailForm>
  <volatility>10</volatility>
  <methodList>
    <method name="cwOnFinderSel" type="action">
      <system>true</system>
      <script><![CDATA[
        var resultSet = null;

        var configFinderDetails = ds_ws.getFinderForNVPairForGUI(this.searchDocument.inputData);
        if(configFinderDetails != null) {
            var dbName = configFinderDetails.dbLogicalConnection;
            var querySQL = configFinderDetails.querySQL;
            resultSet = Global.runSQLQuery(dbName, "common.genericOutput_String", 9999, null, querySQL);
        }

        return resultSet;
      ]]></script>
    </method>
  </methodList>
  <UserInterface name="UserInterface" type="ui">
    <extends>ui_com.conceptwave.system.FinderUserInterface</extends>
    <variableList>
      <variable name="result" type="uivar">
        <flags enum-type="metadataFlag">ARRAY</flags>
        <valueType>doc_common.genericOutput_String</valueType>
      </variable>
      <variable name="detail" type="uivar">
        <flags enum-type="metadataFlag">VALIDATE</flags>
        <valueType>doc_common.genericOutput_String/ui_UserInterface</valueType>
      </variable>
      <variable name="search" type="uivar">
        <valueType>doc_common.genericInput_String/ui_UserInterface</valueType>
      </variable>
    </variableList>
    <vformList>
      <vform name="Forms" type="frmui">
        <extends>ui_com.conceptwave.system.FinderUserInterface/frmui_Forms</extends>
        <overlayList>
          <overlay name="Result" type="exov">
            <base>ui_com.conceptwave.system.FinderUserInterface/frmui_Forms/vform_Result</base>
            <extends>ui_com.conceptwave.system.FinderUserInterface/frmui_Forms/vform_Result</extends>
            <label>Result</label>
            <overrideList>
              <override name="table" type="elvlt">
                <height>100%</height>
                <width>100%</width>
                <elementList>
                  <element name="findCustomQueryForNVPair4GUIResultTable" type="eltabl">
                    <onSelectChange type="varPath">
                      <variablePathList>
                        <variablePath>ui_com.conceptwave.system.MasterDetailUserInterface/action_OnSelChanged</variablePath>
                      </variablePathList>
                    </onSelectChange>
                    <variable type="varPath">
                      <variablePathList>
                        <variablePath>findScript_ds_ws.findCustomQueryForNVPair4GUI/ui_UserInterface/uivar_result</variablePath>
                      </variablePathList>
                    </variable>
                    <elementList>
                      <element name="result" type="elfld">
                        <label>Result Found</label>
                        <variable type="varPath">
                          <variablePathList>
                            <variablePath>findScript_ds_ws.findCustomQueryForNVPair4GUI/ui_UserInterface/uivar_result</variablePath>
                            <variablePath>doc_common.genericOutput_String/leaf_result</variablePath>
                          </variablePathList>
                        </variable>
                        <width>300px</width>
                      </element>
                    </elementList>
                  </element>
                </elementList>
              </override>
            </overrideList>
          </overlay>
          <overlay name="Default" type="exov">
            <base>ui_com.conceptwave.system.FinderUserInterface/frmui_Forms/exov_Default</base>
            <extends>ui_com.conceptwave.system.FinderUserInterface/frmui_Forms/exov_Default</extends>
            <label>Default</label>
            <overrideList>
              <override name="detailSection" type="elsect">
                <formReference type="varPath">
                  <variablePathList>
                    <variablePath>ui_com.conceptwave.system.MasterDetailUserInterface/uivar_detailForm</variablePath>
