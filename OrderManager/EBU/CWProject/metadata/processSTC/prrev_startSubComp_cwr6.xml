            <y>230.92578</y>
            <methodList>
              <method name="cwOnProcActCond" type="action">
                <category>cond</category>
                <guid>{03cbf811-e246-7175-6653-4774bb75b47b}</guid>
                <system>true</system>
                <script><![CDATA[
                  if (this.process.processDocument.firstPassSub == "N") {
                      return true;
                  }
                  return false;
