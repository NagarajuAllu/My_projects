            <childList>
              <child name="alert" type="scriptActivity">
                <guid>{07f7a2f8-7ba4-e336-369f-9dcf22b65ce1}</guid>
                <label>Alert</label>
                <x>114.0</x>
                <y>229.92578</y>
                <methodList>
                  <method name="cwOnProcActBefore" type="action">
                    <category>before</category>
                    <guid>{0a25f0f0-4fa7-c0cc-d8d6-75b93a14a3ab}</guid>
                    <system>true</system>
                    <script><![CDATA[
                      if(this.process.processDocument.isSubmit == "Y") {
                          processSTC.sendMailForError("Caught error in submit process " + this.process.id, Global.getConfigVariable("MAIL_RECEIVER_FOR_ALERTS"));
                      }
                      else {
                          processSTC.sendMailForError("Caught error in update process " + this.process.id, Global.getConfigVariable("MAIL_RECEIVER_FOR_ALERTS"));
                      }
                    ]]></script>
                  </method>
                </methodList>
              </child>
            </childList>
          </child>
          <child name="no" type="caseActivity">
