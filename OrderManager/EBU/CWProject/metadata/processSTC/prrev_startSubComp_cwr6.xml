      <child name="waitTimeforSubmit" type="scriptActivity">
        <guid>{00e05ae7-626c-8968-ce15-15f3a5d64d9f}</guid>
        <label>Wait Time for Submit</label>
        <schedule type="sched">
          <duration type="dur">
            <methodList>
              <method name="cwOnDuration" type="action">
                <system>true</system>
                <script><![CDATA[
                  // configured time for sleeping
                  var submitSleep = Global.getConfigVariable("SUBMITSLEEP", "10");

                  // if downtime
                  var ifDelay = Global.getInterfaceDelay("ifGranite_jms:XngServicesWR");
                  ifDelay = ((ifDelay != null) ? ifDelay : "10") ;

                  // sleepTime of the process = max(submitSleep, ifDelay);
                  var sleepTime = ((submitSleep > ifDelay) ? submitSleep : ifDelay);

                  debugPrintln("[" + common.common_currentDateAsStringDDMMYYYYHH24MISS() + "][" + this.id + "] - sleep for : " + sleepTime + " secs -- {" + submitSleep + ";" + ifDelay + "}");

                  return sleepTime;
                ]]></script>
              </method>
            </methodList>
          </duration>
        </schedule>
        <x>68.166504</x>
        <y>327.88867</y>
        <methodList>
          <method name="cwOnProcActBefore" type="action">
