  <methodList>
    <method name="cwOnFinderSQLSel" type="action">
      <system>true</system>
      <script><![CDATA[
        select {+*+}
          from val_customer
         where 1=1
        {[0] and customer_id like ?}
        {[1] and customer_name like ?}
        {[2] and customer_type like ?}
           and cust_inst_id in (select cdm.cust_inst_id
                                  from cust_domain_map cdm, domain_inst di
                                 where cdm.domain_inst_id = di.domain_inst_id
                                   and di.domain_name = 'ebu__domain')
      ]]></script>
    </method>
  </methodList>
