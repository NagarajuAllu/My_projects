<?xml version="1.0" encoding="UTF-8" ?>
<findSql name="processSTC.findRunningProcessesForUpdatedOrders">
  <dbSchema>ORDER</dbSchema>
  <extends>find_com.conceptwave.system.Finder</extends>
  <label>Find Running Processes For Updated Orders</label>
  <maxRowsLimit>9999</maxRowsLimit>
  <metaVersion>21</metaVersion>
  <output>doc_processSTC.processIdDocument</output>
  <type>SQL</type>
  <volatility>5</volatility>
  <methodList>
    <method name="cwOnFinderSQLSel" type="action">
      <system>true</system>
      <script><![CDATA[
        select p.process_id
          from stc_bundleorder_header o, cwprocess_not_completed p
         where p.order_id = o.cworderid
           and p.status not in (3, 6)
           and (o.ordernumber like '%_REVI_201%' or o.ordernumber like '%_CANC_201%')
      ]]></script>
    </method>
  </methodList>
