      </variable>
    </variableList>
    <vformList>
      <vform name="Forms" type="frmui">
        <extends>ui_com.conceptwave.system.FinderUserInterface/frmui_Forms</extends>
        <overlayList>
          <overlay name="Result" type="exov">
            <base>ui_com.conceptwave.system.FinderUserInterface/frmui_Forms/vform_Result</base>
            <extends>ui_com.conceptwave.system.FinderUserInterface/frmui_Forms/vform_Result</extends>
            <label>Result</label>
            <overrideList>
              <override name="table" type="elvlt">
                <height>100%</height>
                <width>100%</width>
                <elementList>
                  <element name="findOrdersRejectedForLockResultTable" type="eltabl">
                    <doubleClickMethod type="varPath">
                      <variablePathList>
                        <variablePath>findDoc_processSTC.findOrdersRejectedForLock/ui_UserInterface/usrActionMethod_showOrder</variablePath>
                      </variablePathList>
                    </doubleClickMethod>
                    <onSelectChange type="varPath">
                      <variablePathList>
                        <variablePath>ui_com.conceptwave.system.MasterDetailUserInterface/action_OnSelChanged</variablePath>
                      </variablePathList>
                    </onSelectChange>
                    <variable type="varPath">
                      <variablePathList>
                        <variablePath>findDoc_processSTC.findOrdersRejectedForLock/ui_UserInterface/uivar_result</variablePath>
                      </variablePathList>
                    </variable>
                    <elementList>
                      <element name="orderNumber" type="elfld">
                        <label>Order #</label>
                        <variable type="varPath">
                          <variablePathList>
                            <variablePath>findDoc_processSTC.findOrdersRejectedForLock/ui_UserInterface/uivar_result</variablePath>
                            <variablePath>doc_processSTC.STC_ORDER_REJECTED_FOR_LOCK/leaf_orderNumber</variablePath>
                          </variablePathList>
                        </variable>
                        <width>200px</width>
                      </element>
                    </elementList>
                  </element>
                </elementList>
              </override>
