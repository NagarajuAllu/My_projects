        </overlayList>
      </vform>
    </vformList>
    <methodList>
      <method name="showOrder" type="usrActionMethod">
        <script><![CDATA[
          if(this.model.selected != null) {
              var orderNumber = this.model.selected[0].orderNumber;

              var orderHeader = ds_ws.getBundleOrderByOrderNumber(orderNumber);
              if(orderHeader == null) {
                  Global.throwException("Unable to find orderHeader for orderNumber '" + orderNumber + "'");
              }

              var orderUnderMgmt = Order.getOrderById(orderHeader.orderId);
              // checking if provisioningFlag is different from PROVISIONING
              if(orderUnderMgmt == null) {
                  Global.throwException("Unable to order with id '" + orderHeader.orderId + "' linked to the selected orderNumber '" + orderNumber + "'");
              }

              return (orderUnderMgmt);
          }
        ]]></script>
      </method>
    </methodList>
  </UserInterface>
  <viewList>
    <view name="findOrdersRejectedForLockView" type="findVView">
      <label>Find Orders Rejected Due to Lock Issue</label>
      <outputOverlay>findDoc_processSTC.findOrdersRejectedForLock/ui_UserInterface/frmui_Forms/exov_Result</outputOverlay>
      <searchImmediately>true</searchImmediately>
    </view>
  </viewList>
