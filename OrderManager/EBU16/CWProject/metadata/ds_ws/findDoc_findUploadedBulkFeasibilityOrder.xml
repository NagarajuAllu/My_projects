    <methodList>
      <method name="enableSubmitAll" type="permMethod">
        <privilegesList>
          <privileges type="ppriv">
            <privilege>STC_InsertOrder</privilege>
          </privileges>
        </privilegesList>
        <privilegesType>SOME</privilegesType>
        <script><![CDATA[
          var selectedItem = this.model.selected;
          if(selectedItem == null || selectedItem.length == 0 || selectedItem.length > 1) {
              return false;
          }

          if(selectedItem[0].jobId == null) {
              return false;
          }

          // check if jobId already processed
          return (! ds_ws.checkIfJobIdIsProcessed(selectedItem[0].jobId));
        ]]></script>
      </method>
      <method name="submitAll" type="usrActionMethod">
        <confirmation>findDoc_ds_ws.findUploadedBulkFeasibilityOrder/ui_UserInterface/uimthd_submitAll_confirm</confirmation>
        <script><![CDATA[
          var selectedItem = this.model.selected;
          if(selectedItem == null || selectedItem.length == 0) {
              Global.throwException("UU0115");
          }
          else if(selectedItem.length > 1) {
              Global.throwException("UU0116");
          }

          var selectedJobId = selectedItem[0].jobId;

          // start Process with  procDoc = jobId
          var processDocument = new Document("processSTC.bulkFeasibilityOrderProcessDoc");
          processDocument.jobId = selectedJobId;
          var processId = Process.startProcess("processSTC.processBulkUploadedFeasibility", processDocument);

          // add jobId to list of processed jobs
          ds_ws.addJobIdToProcessedList(selectedJobId, processId);
        ]]></script>
      </method>
      <method name="submitAll_confirm" type="uimthd">
        <script><![CDATA[return "AW0013";]]></script>
      </method>
    </methodList>
