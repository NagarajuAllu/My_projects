    </method>
    <method name="editablePerm" type="permMethod">
      <privilegesList>
        <privileges type="ppriv">
          <privilege>STC_Config</privilege>
        </privileges>
        <privileges type="ppriv">
          <privilege>UPAdmin</privilege>
        </privileges>
      </privilegesList>
      <privilegesType>SOME</privilegesType>
      <script>return $psCondition;</script>
    </method>
    <method name="cwOnInitSearch" type="uimthd">
      <script><![CDATA[
        this.cw$super_cwOnInitSearch();

        var inputBandwidthName = this.searchDocument.bw_name;

        if(inputBandwidthName != null) {
            while(inputBandwidthName.charAt(0) == "%") {
                inputBandwidthName = inputBandwidthName.substring(1);
            }

            while(inputBandwidthName.endsWith("%")) {
                inputBandwidthName = inputBandwidthName.substring(0, inputBandwidthName.length -1);
            }
        }


        if(inputBandwidthName != null && inputBandwidthName.trim().length > 0 && inputBandwidthName != "%") {
            this.searchDocument.bw_name = "%" + inputBandwidthName + "%";
        }
        else {
            this.searchDocument.bw_name = "%";
        }
      ]]></script>
    </method>
  </methodList>
