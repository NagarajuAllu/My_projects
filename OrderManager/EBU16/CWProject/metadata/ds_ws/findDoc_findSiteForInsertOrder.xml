    <method name="cwOnInitSearch" type="uimthd">
      <script><![CDATA[
        this.searchDocument.SITE_HUM_ID = this.addWildCards(this.searchDocument.SITE_HUM_ID);

        if(this.searchDocument.NUM != null && this.searchDocument.NUM.trim().length > 0) {
            this.searchDocument.NUM = this.addWildCards(this.searchDocument.NUM);
        }
        else {
            this.searchDocument.NUM = null;
        }

        if(this.searchDocument.CLLI != null && this.searchDocument.CLLI.trim().length > 0) {
            this.searchDocument.CLLI = this.addWildCards(this.searchDocument.CLLI);
        }
        else {
            this.searchDocument.CLLI = null;
        }
      ]]></script>
    </method>
    <method name="addWildCards" type="uimthd">
      <parameterList>
        <parameter name="fieldValue" type="rifp"/>
      </parameterList>
      <script><![CDATA[
        var result = null;

        if(fieldValue != null) {
            while(fieldValue.charAt(0) == "%") {
                fieldValue = fieldValue.substring(1);
            }

            while(fieldValue.endsWith("%")) {
                fieldValue = fieldValue.substring(0, fieldValue.length -1);
            }
        }

        if(fieldValue != null && fieldValue.trim().length > 0 && fieldValue != "%") {
            result = "%" + fieldValue + "%";
        }
        else {
            result = "%";
        }

        return result;
      ]]></script>
    </method>
  </methodList>
