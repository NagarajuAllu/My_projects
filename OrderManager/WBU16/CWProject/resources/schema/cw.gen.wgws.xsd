	<element name="updateOrderStatusResponse" type="wgws:status"/>

	<complexType name="nameValuePair">
		<annotation>
			<documentation>nameValuePair</documentation>
		</annotation>
			<sequence>
				<element name="name" minOccurs="1" maxOccurs="1" type="xsd:string"/>
				<element name="value" minOccurs="0" maxOccurs="1" type="xsd:string"/>
			</sequence>
		</complexType>

