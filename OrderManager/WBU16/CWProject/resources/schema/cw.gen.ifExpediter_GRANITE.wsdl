						<element name="newAssetStatus" minOccurs="1" maxOccurs="1" type="xsd:string"/>
						<element name="oldAssetNumber" minOccurs="0" maxOccurs="1" type="xsd:string"/>
						<element name="newAssetNumber" minOccurs="1" maxOccurs="1" type="xsd:string"/>
						<element name="businessUnit" minOccurs="1" maxOccurs="1" type="xsd:string"/>
