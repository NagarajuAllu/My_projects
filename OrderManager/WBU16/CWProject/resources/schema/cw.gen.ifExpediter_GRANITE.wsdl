					</sequence>
				</complexType>

			<complexType name="quoteStatus">
				<annotation>
					<documentation>quoteStatus</documentation>
				</annotation>
					<sequence>
						<element name="quoteNumber" minOccurs="1" maxOccurs="1" type="xsd:string"/>
						<element name="orderRowItemId" minOccurs="1" maxOccurs="1" type="xsd:integer"/>
						<element name="feasibilityStatus" minOccurs="0" maxOccurs="1" type="xsd:string"/>
						<element name="reservationDays" minOccurs="0" maxOccurs="1" type="xsd:string"/>
