		<port name="ifExpediter_GRANITE_SOAP_Port" binding="tns:ifExpediter_GRANITE_SOAP">
			<soap:address location="http://localhost:8282/cwf/services/ifExpediter_GRANITE"/>
		</port>
	</service>

</definitions>