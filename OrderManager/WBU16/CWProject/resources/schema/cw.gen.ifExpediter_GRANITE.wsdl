		<operation name="operation_OrderStatusUpdate">
			<input message="tns:updateOrderStatus"/>
			<output message="tns:updateOrderStatusResponse"/>
		</operation>
		<operation name="operation_QuoteStatusUpdate">
			<input message="tns:updateQuoteStatus"/>
			<output message="tns:updateQuoteStatusResponse"/>
		</operation>
		<operation name="operation_Restoration">
			<input message="tns:restoration"/>
			<output message="tns:restorationResponse"/>
		</operation>
		<operation name="operation_SiteInformationUpdate">
			<input message="tns:siteInformationUpdate"/>
			<output message="tns:siteInformationUpdateResponse"/>
		</operation>
		<operation name="operation_LoadSubOrders">
			<input message="tns:loadSubOrders"/>
			<output message="tns:loadSubOrdersResponse"/>
		</operation>
	</portType>

	<binding name="ifExpediter_GRANITE_SOAP" type="tns:ifExpediter_GRANITE">
		<soap:binding transport="http://schemas.xmlsoap.org/soap/http" style="document"/>
		<operation name="operation_OrderStatusUpdate">
			<soap:operation soapAction="operation_OrderStatusUpdate"/>
			<input>
				<soap:body use="literal"/>
			</input>
			<output>
				<soap:body use="literal"/>
			</output>
		</operation>
		<operation name="operation_QuoteStatusUpdate">
