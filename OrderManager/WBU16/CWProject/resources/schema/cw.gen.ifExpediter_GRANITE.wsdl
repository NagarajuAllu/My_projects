						<element name="businessUnit" minOccurs="0" maxOccurs="1" type="xsd:string"/>
						<element name="workOrderStatus" minOccurs="0" maxOccurs="1" type="xsd:string"/>
						<element name="comments" minOccurs="0" maxOccurs="1" type="xsd:string"/>
					</sequence>
				</complexType>

			<element name="updateQuoteStatus" type="wgws:quoteStatus"/>

			<element name="loadSubOrdersResponse" type="wgws:status"/>

			<!-- Documents -->

		</schema>
	</types>

	<message name="updateOrderStatus">
		<part name="updateOrderStatus" element="wgws:updateOrderStatus"/>
	</message>

	<message name="updateOrderStatusResponse">
		<part name="updateOrderStatusResponse" element="wgws:updateOrderStatusResponse"/>
	</message>

	<message name="updateQuoteStatus">
