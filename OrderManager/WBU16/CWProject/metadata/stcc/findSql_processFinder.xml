  <methodList>
    <method name="cwOnFinderSQLSel" type="action">
      <system>true</system>
      <script><![CDATA[
        SELECT p.process_id, p.parent_id, p.root_id, p.status, p.process_metadatype, p.metadatatype_ver,
               p.order_id, p.starttime, p.changedstatustime, p.priority, p.order_type, p.order_item_id,
               p.bucket_no, p.revision
          FROM cwmdtypes t, cwprocess p
         WHERE t.typeid = p.process_metadatype
        {[0] AND p.order_id = ?}
        {[1] AND t.typename = ?}
        {[2] AND p.process_id <> ?}
      ]]></script>
    </method>
    <method name="editablePerm" type="permMethod">
      <script>return this.enabled;</script>
    </method>
