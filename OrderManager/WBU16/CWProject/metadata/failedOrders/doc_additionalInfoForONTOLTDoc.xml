    <methodList>
      <method name="onInit" type="action">
        <valueType>nmeta_com.conceptwave.system.Object</valueType>
        <script><![CDATA[
          this.menuLabel = "Update ONT/OLT Info and " + (this.model.isForResending ? "Resend" : "Complete") + " \"" + this.model.taskOperation + "\" Task";

          // search ont details
          var ontEquipmentDetails = granite.getONTEquipmentDetailsByPathInstId(this.model.pathInstId);
          if(ontEquipmentDetails != null) {
              this.ontEquipmentDetailsUI = new granite.ontEquipmentDetails.UserInterface(ontEquipmentDetails);

              this.model.ONT_EQUIP_INST_ID = ontEquipmentDetails.EQUIP_INST_ID;

              this.model.SERIAL_NO_NEW = ontEquipmentDetails.SERIAL_NO;
              this.model.ONT_ID_NEW = ontEquipmentDetails.ONT_ID;
          }
          else {
              this.ontEquipmentDetailsUI = null;
              this.model.ONT_EQUIP_INST_ID = null;

              this.model.SERIAL_NO_NEW = null;
              this.model.ONT_ID_NEW = null;
          }

          // search olt details
          var oltEquipmentDetails = granite.getOLTEquipmentDetailsByPathInstId(this.model.pathInstId);
          if(oltEquipmentDetails != null) {
              this.oltEquipmentDetailsUI = new granite.oltEquipmentDetails.UserInterface(oltEquipmentDetails);

              this.model.OLT_EQUIP_INST_ID = oltEquipmentDetails.EQUIP_INST_ID;
              this.model.OLT_PORT_INST_ID = oltEquipmentDetails.PORT_INST_ID;

              this.model.OLT_UDA_NAME_NEW = oltEquipmentDetails.OLT_UDA_NAME;
              this.model.OLT_UDA_SLOT_NEW = oltEquipmentDetails.OLT_UDA_SLOT;
              this.model.OLT_UDA_PORT_NEW = oltEquipmentDetails.OLT_UDA_PORT;
          }
          else {
              this.oltEquipmentDetailsUI = null;
              this.model.OLT_EQUIP_INST_ID = null;
              this.model.OLT_PORT_INST_ID = null;

              this.model.OLT_UDA_NAME_NEW = null;
              this.model.OLT_UDA_SLOT_NEW = null;
              this.model.OLT_UDA_PORT_NEW = null;
          }
        ]]></script>
      </method>
      <method name="executeActionForTask" type="usrActionMethod">
        <confirmation>doc_failedOrders.additionalInfoForONTOLTDoc/ui_UserInterface/uimthd_executeActionForTask_confirm</confirmation>
        <validate>true</validate>
        <script><![CDATA[
          // updating OLT/ONT
          var wo = granite.getWorkOrderByWOInstId(this.model.woInstId);
          var woName = (wo != null ? wo.WO_NAME : "UNKNOWN WO_NAME");
          var task = granite.getTaskById(this.model.taskInstId);
          var taskName = (task != null ? task.TASK_NAME : "UNKNOWN TASK_NAME");

          var userId = Global.getUserId();

          var oltUDAs = this.createUDAsForOLT();
          var ontUDAs = this.createUDAsForONT();

          var resultForUpdateOLT_ONT = granitews4eoc.invokeUpdateOLT_ONTInfoAPI(this.model.OLT_EQUIP_INST_ID,
                                                                                this.model.OLT_PORT_INST_ID,
                                                                                oltUDAs,
                                                                                this.model.ONT_EQUIP_INST_ID,
                                                                                this.model.SERIAL_NO_NEW,
                                                                                this.model.taskInstId,
                                                                                userId,
                                                                                ontUDAs,
                                                                                woName,
                                                                                taskName);

          if(resultForUpdateOLT_ONT != null) {
              var resultMessage = resultForUpdateOLT_ONT.ResultMessage;
              if(resultMessage.result != "SUCCESS") {
                  // there was an error in updating OLT and ONT, so throwing the error;
                  var errorMsg = "Error in updating OLT and ONT; received Error Message: '" + resultMessage.errorDescription + "'";
                  Global.throwException(errorMsg);
              }
          }
          else {
              // no message received
              var errorMsg = "No response received while updating OLT and ONT! Pls refresh the WorkOrder and retry";
              Global.throwException(errorMsg);
          }

          var actionResult = null;

          // perform the action
          if(this.model.isForResending) {
              // it's for resending
              if(task.STATUS_CODE == 9) {
                  // the selected task is in status FAILED; completing it
                  actionResult = granitews4eoc.invokeChangeTaskStatusAPI(this.model.taskInstId, 4, userId, woName, taskName);
              }
              else {
                  if(this.model.blockingTaskInstId == null) {
                      Global.throwException("Unexpected case! The task is not FAILED but there is no Blocking Task");
                  }
                  var blockingTask = granite.getTaskById(this.model.blockingTaskInstId);

                  if(blockingTask == null) {
                      Global.throwException("Unable to find the Blocking Task for id " + this.model.blockingTaskInstId);
                  }

                  // the selected task is in status FAILED; completing it
                  actionResult = granitews4eoc.invokeCompleteTaskAPI(blockingTask.TASK_INST_ID, userId, woName, taskName);
              }

              // log user action
              failedOrders.insertNewUserActionForFO(userId, "Resend With Addtional Information");
          }
          else {
              // it's for complete
              if(task.STATUS_CODE == 9) {
                  // the selected task is in status FAILED; completing it
                  actionResult = granitews4eoc.invokeCompleteTaskAPI(this.model.taskInstId, userId, woName, taskName);
              }
              else {
                  if(this.model.blockingTaskInstId == null) {
                      Global.throwException("Unexpected case! The task is not FAILED but there is no Blocking Task");
                  }
                  var blockingTask = granite.getTaskById(this.model.blockingTaskInstId);

                  if(blockingTask == null) {
                      Global.throwException("Unable to find the Blocking Task for id " + this.model.blockingTaskInstId);
                  }

                  var queueName = granite.getQueueNameById(blockingTask.QUEUE_INST_ID);
                  if(queueName == null) {
                      queueName = "FTTH REJECTIONS";
                  }
                  // the selected task is in status FAILED; completing it
                  actionResult = granitews4eoc.invokeCompleteBlockedTaskAPI(this.model.taskInstId, queueName, blockingTask.TASK_INST_ID, userId, woName, taskName);
              }

              // log user action
              failedOrders.insertNewUserActionForFO(userId, "Complete");
          }

          if(actionResult != null) {
              var resultMessage = actionResult.ResultMessage;
              if(resultMessage.result != "SUCCESS") {
                  // there was an error in updating OLT and ONT, so throwing the error;
                  var errorMsg = "Error in updating OLT and ONT; received Error Message: '" + resultMessage.errorDescription + "'";
                  Global.throwException(errorMsg);
              }
          }
          else {
              // no message received
              var errorMsg = "No response received while updating OLT and ONT! Pls refresh the WorkOrder and retry";
              Global.throwException(errorMsg);
          }


          // refresh parent
          this.parent.owner.refreshUI();

          Global.showUserMessage("Action completed successfully");

          // close dialog
          return Global.STOP_UI;
        ]]></script>
      </method>
      <method name="executeActionForTask_confirm" type="uimthd">
