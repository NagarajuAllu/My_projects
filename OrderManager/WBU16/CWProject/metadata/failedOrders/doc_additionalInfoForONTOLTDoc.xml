      <method name="createUDAsForOLT" type="uimthd">
        <script><![CDATA[
          var oltUDAs = new Array();
          var oltUDA1 = new DataStructure("granitews4eoc.udaDetailsPojo");
          oltUDA1.udaName = "OLT Name";
          oltUDA1.udaGroup = "OLT Attributes Information";
          oltUDA1.udaValue = this.model.OLT_UDA_NAME_NEW;
          oltUDAs.push(oltUDA1);

          var oltUDA2 = new DataStructure("granitews4eoc.udaDetailsPojo");
          oltUDA2.udaName = "OLT Physical Slot";
          oltUDA2.udaGroup = "OLT Attributes Information";
          oltUDA2.udaValue = this.model.OLT_UDA_SLOT_NEW;
          oltUDAs.push(oltUDA2);

          var oltUDA3 = new DataStructure("granitews4eoc.udaDetailsPojo");
          oltUDA3.udaName = "OLT Physical Port";
          oltUDA3.udaGroup = "OLT Attributes Information";
          oltUDA3.udaValue = this.model.OLT_UDA_PORT_NEW;
          oltUDAs.push(oltUDA3);

          return oltUDAs;
        ]]></script>
      </method>
      <method name="createUDAsForONT" type="uimthd">
        <script><![CDATA[
          var ontUDAs = new Array();
          var ontUDA1 = new DataStructure("granitews4eoc.udaDetailsPojo");
          ontUDA1.udaName = "ONT ID";
          ontUDA1.udaGroup = "ONT/Customer Details";
          ontUDA1.udaValue = this.model.ONT_ID_NEW;
          ontUDAs.push(ontUDA1);

          return ontUDAs
        ]]></script>
