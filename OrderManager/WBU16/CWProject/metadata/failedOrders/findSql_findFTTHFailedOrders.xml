  <methodList>
    <method name="cwOnFinderSQLSel" type="action">
      <system>true</system>
      <script><![CDATA[
        select distinct {+fo+}
          from stcw_ftth_failed_orders fo, stcw_group_conf_4_users g
         where fo.task_operation = g.taskoperation
           and fo.tsk_status_code = g.taskstatuscode
           and fo.state_prov like nvl(g.region, '%')
           and fo.wo_order_type like nvl(g.ordertype, '%')
           and fo.tsk_reason_code like nvl(g.reasoncode, '%')
           and fo.tsk_reason_code not like  nvl(g.reasoncodenot, 'X@X@Y@Y@Z@Z')
           and fo.tsk_reason_descr like nvl(g.reasondescr, '%')
           and fo.tsk_reason_descr not like  nvl(g.reasondescrnot, 'X@X@Y@Y@Z@Z')
        {[0] and g.userid = ?}
      ]]></script>
    </method>
    <method name="cwOnFinderSQLGet" type="action">
      <system>true</system>
      <script><![CDATA[
        select {+*+}
          from stcw_ftth_failed_orders f
         where f.wo_inst_id = ?
           and f.task_inst_id = ?
           and f.circ_path_inst_id = ?
           and f.resource_inst_id = ?
      ]]></script>
    </method>
