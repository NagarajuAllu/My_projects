<?xml version="1.0" encoding="UTF-8" ?>
<findSql name="failedOrders.findFOGroupConfigurationForUser">
  <dbSchema>ORDER</dbSchema>
  <extends>find_com.conceptwave.system.Finder</extends>
  <input>doc_cwf_up.UserIDSearch</input>
  <label>Find FO Configuration For User</label>
  <metaVersion>25</metaVersion>
  <output>doc_failedOrders.STCW_GROUP_CONFIGURATION</output>
  <type>SQL</type>
  <volatility>5</volatility>
  <methodList>
    <method name="cwOnFinderSQLSel" type="action">
      <system>true</system>
      <script><![CDATA[
        select {+g+}
          from stcw_group_configuration g, cwuserrole ur
         where g.groupname = ur.roleid
        {[0] and ur.userid = ?}
      ]]></script>
    </method>
  </methodList>
</findSql>