    <methodList>
      <method name="showInboundMsg" type="usrActionMethod">
        <dialogHeight>600</dialogHeight>
        <dialogWidth>500</dialogWidth>
        <validate>true</validate>
        <script><![CDATA[
          var m = this.result.selected;
          if (m != null) {
              if (m.length > 1) {
                  Global.throwException("UU0116");
              }
          }
          else {
              Global.throwException("UU0111");
          }

          m = this.result.selected[0];

          var msg = this.result.selected[0].INBOUND_XML;
          Global.showModelessDialog(msg,500,400,'text/xml');
        ]]></script>
      </method>
      <method name="showOutboundMsg" type="usrActionMethod">
        <dialogHeight>600</dialogHeight>
        <dialogWidth>500</dialogWidth>
        <validate>true</validate>
        <script><![CDATA[
