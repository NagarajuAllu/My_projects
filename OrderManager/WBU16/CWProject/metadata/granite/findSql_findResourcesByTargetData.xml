                      <element name="DUE" type="elfld">
                        <label>Due</label>
                        <variable type="varPath">
                          <variablePathList>
                            <variablePath>findSql_granite.findResourcesByTargetData/ui_UserInterface/uivar_result</variablePath>
                            <variablePath>doc_granite.RESOURCE_INST/leaf_DUE</variablePath>
                          </variablePathList>
                        </variable>
                      </element>
                      <element name="IN_SERVICE" type="elfld">
                        <label>In Service</label>
                        <variable type="varPath">
                          <variablePathList>
                            <variablePath>findSql_granite.findResourcesByTargetData/ui_UserInterface/uivar_result</variablePath>
                            <variablePath>doc_granite.RESOURCE_INST/leaf_IN_SERVICE</variablePath>
                          </variablePathList>
                        </variable>
                      </element>
                      <element name="DECOMMISSION" type="elfld">
                        <label>Decommission</label>
                        <variable type="varPath">
                          <variablePathList>
                            <variablePath>findSql_granite.findResourcesByTargetData/ui_UserInterface/uivar_result</variablePath>
                            <variablePath>doc_granite.RESOURCE_INST/leaf_DECOMMISSION</variablePath>
                          </variablePathList>
                        </variable>
                      </element>
                    </elementList>
                  </element>
                </elementList>
              </override>
            </overrideList>
          </overlay>
          <overlay name="Menu" type="exov">
            <base>ui_com.conceptwave.system.BaseFinderUserInterface/frmui_Forms/exov_Menu</base>
            <extends>ui_com.conceptwave.system.BaseFinderUserInterface/frmui_Forms/exov_Menu</extends>
            <label>Menu</label>
            <overrideList>
              <override name="cwfAdd" type="eldel"/>
              <override name="cwfCopy" type="eldel"/>
              <override name="cwfDelete" type="eldel"/>
              <override name="cwfUpdate" type="eldel"/>
            </overrideList>
          </overlay>
        </overlayList>
      </vform>
    </vformList>
  </UserInterface>
  <viewList>
    <view name="findResourcesLinkedToPathView" type="findVView">
      <label>Find Resources Linked To Path</label>
      <outputOverlay>findSql_granite.findResourcesByTargetData/ui_UserInterface/frmui_Forms/exov_Result</outputOverlay>
    </view>
  </viewList>
