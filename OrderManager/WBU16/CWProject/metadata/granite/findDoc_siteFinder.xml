  </UserInterface>
  <viewList>
    <view name="siteFinderView" type="findVView">
      <outputOverlay>findDoc_granite.siteFinder/ui_UserInterface/frmui_Forms/exov_Result</outputOverlay>
    </view>
  </viewList>
</findDoc>