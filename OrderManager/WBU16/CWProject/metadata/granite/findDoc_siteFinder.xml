                      </element>
                      <element name="RESTRICTIONS" type="elfld">
                        <variable type="varPath">
                          <variablePathList>
                            <variablePath>findDoc_granite.siteFinder/ui_UserInterface/uivar_result</variablePath>
                            <variablePath>doc_granite.SITE_INST/leaf_RESTRICTIONS</variablePath>
                          </variablePathList>
                        </variable>
                      </element>
                      <element name="CONTACTS" type="elfld">
                        <variable type="varPath">
                          <variablePathList>
                            <variablePath>findDoc_granite.siteFinder/ui_UserInterface/uivar_result</variablePath>
                            <variablePath>doc_granite.SITE_INST/leaf_CONTACTS</variablePath>
                          </variablePathList>
                        </variable>
                      </element>
                      <element name="COMMENTS" type="elfld">
                        <variable type="varPath">
