                            <variablePath>doc_granite.SITE_INST/leaf_COMMENTS</variablePath>
                          </variablePathList>
                        </variable>
                      </element>
                      <element name="ADDRESS" type="elfld">
                        <variable type="varPath">
                          <variablePathList>
                            <variablePath>findDoc_granite.siteFinder/ui_UserInterface/uivar_result</variablePath>
                            <variablePath>doc_granite.SITE_INST/leaf_ADDRESS</variablePath>
                          </variablePathList>
                        </variable>
                      </element>
                      <element name="POST_CODE_1" type="elfld">
                        <variable type="varPath">
                          <variablePathList>
                            <variablePath>findDoc_granite.siteFinder/ui_UserInterface/uivar_result</variablePath>
                            <variablePath>doc_granite.SITE_INST/leaf_POST_CODE_1</variablePath>
                          </variablePathList>
