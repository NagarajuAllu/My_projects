                      </element>
                      <element name="POST_CODE_2" type="elfld">
                        <variable type="varPath">
                          <variablePathList>
                            <variablePath>findDoc_granite.siteFinder/ui_UserInterface/uivar_result</variablePath>
                            <variablePath>doc_granite.SITE_INST/leaf_POST_CODE_2</variablePath>
