  <volatility>0</volatility>
  <methodList>
    <method name="cwOnFinderSQLSel" type="action">
      <system>true</system>
      <script><![CDATA[
        select ri.resource_inst_id, ri.parent_inst_id, ri.template_inst_id, ri.definition_inst_id, ri.name,
               ri.category, ri.revision, ri.flags, ri.start_date, ri.end_date, ri.status, ri.ordered,
               ri.sched_date, ri.due, ri.installed, ri.in_service, ri.decommission,
               (select name
                  from resource_definition_inst rdi
                 where rdi.definition_inst_id = ri.definition_inst_id) definitionName,
               (select attr_value
                  from resource_attr_settings ras, val_attr_name van
                 where van.attr_name = 'UPSTREAM_BW'
                   and van.group_name = 'FTTH_HSI_ATTRIBUTES'
                   and ras.val_attr_inst_id = van.val_attr_inst_id
                   and ras.resource_inst_id = ri.resource_inst_id) upstreamBW,
               (select attr_value
                  from resource_attr_settings ras, val_attr_name van
                 where van.attr_name = 'DOWNSTREAM_BW'
                   and van.group_name = 'FTTH_HSI_ATTRIBUTES'
                   and ras.val_attr_inst_id = van.val_attr_inst_id
                   and ras.resource_inst_id = ri.resource_inst_id) downstreamBW,
               (select attr_value
                  from resource_attr_settings ras, val_attr_name van
                 where van.attr_name = 'Change Request Type'
                   and van.group_name = decode(ri.category, 'VOIP', 'FTTH_VOIP_ATTRIBUTES',
                                                            'IPTV', 'FTTH_IPTV_ATTRIBUTES',
                                                                    'FTTH_HSI_ATTRIBUTES')
                   and ras.val_attr_inst_id = van.val_attr_inst_id
                   and ras.resource_inst_id = ri.resource_inst_id) changeRequestType
          from resource_inst ri
         where ri.resource_inst_id in (select ra.resource_inst_id
                                         from resource_associations ra
                                        where 1 = 1
                                    {[0]  and ra.target_inst_id = ?}
                                    {[1]  and ra.target_type_id = ?})
      ]]></script>
    </method>
