<findSql name="granite.findTaskDetailsForFOPages">
  <dbSchema>GRANITE</dbSchema>
  <extends>find_com.conceptwave.system.Finder</extends>
  <input>doc_granite.graniteInstId_10_SearchDoc</input>
  <label>Find Task Details For FO Pages</label>
  <maxRowsLimit>500</maxRowsLimit>
  <metaVersion>25</metaVersion>
  <output>doc_granite.taskDetailsForFO</output>
  <type>SQL</type>
  <volatility>0</volatility>
  <methodList>
    <method name="cwOnFinderSQLSel" type="action">
      <system>true</system>
      <script><![CDATA[
        select tsk.TASK_INST_ID, tsk.WO_INST_ID, tsk.TASK_NUMBER, tsk.TASK_NAME, tsk.TASK_OPERATION, tsk.STATUS_CODE,
               (select STATUS_NAME from val_task_status where STAT_CODE = tsk.STATUS_CODE) TASK_STATUS,
               tsk.START_AFTER, tsk.COMPLETE_BY, tsk.ACTUAL_START, tsk.ACTUAL_COMPL,
               (select b_tsk.TASK_INST_ID
                  from task_dependency td, wo_task_inst b_tsk
                 where td.TASK_INST_ID = tsk.TASK_INST_ID
                   and td.WO_INST_ID = tsk.WO_INST_ID
                   and td.DEPENDENCY_TYPE = 'T'
                   and b_tsk.TASK_INST_ID = td.DEPENDENCY_TVALUE
                   and b_tsk.STATUS_CODE not in (7, 8)
                   and rownum = 1) BLOCKING_TASK_INST_ID,
               (select b_tsk.TASK_NUMBER
                  from task_dependency td, wo_task_inst b_tsk
                 where td.TASK_INST_ID = tsk.TASK_INST_ID
                   and td.WO_INST_ID = tsk.WO_INST_ID
                   and td.DEPENDENCY_TYPE = 'T'
                   and b_tsk.TASK_INST_ID = td.DEPENDENCY_TVALUE
                   and b_tsk.STATUS_CODE not in (7, 8)
                   and rownum = 1) BLOCKING_TASK_NUMBER,
               (select b_tsk.TASK_NAME
                  from task_dependency td, wo_task_inst b_tsk
                 where td.TASK_INST_ID = tsk.TASK_INST_ID
                   and td.WO_INST_ID = tsk.WO_INST_ID
                   and td.DEPENDENCY_TYPE = 'T'
                   and b_tsk.TASK_INST_ID = td.DEPENDENCY_TVALUE
                   and b_tsk.STATUS_CODE not in (7, 8)
                   and rownum = 1)  BLOCKING_TASK_NAME,
               (select b_tsk.TASK_OPERATION
                  from task_dependency td, wo_task_inst b_tsk
                 where td.TASK_INST_ID = tsk.TASK_INST_ID
                   and td.WO_INST_ID = tsk.WO_INST_ID
                   and td.DEPENDENCY_TYPE = 'T'
                   and b_tsk.TASK_INST_ID = td.DEPENDENCY_TVALUE
                   and b_tsk.STATUS_CODE not in (7, 8)
                   and rownum = 1)  BLOCKING_TASK_OPERATION,
               (select ATTR_VALUE
                 from task_attr_settings tas, val_attr_name van
                 where tas.TASK_INST_ID = tsk.TASK_INST_ID
                   and tas.VAL_ATTR_INST_ID = van.VAL_ATTR_INST_ID
                   and van.ATTR_NAME = 'Reason Code'
                   and van.GROUP_NAME = 'Rejected Task Details') REASON_CODE,
               (select ATTR_VALUE
                 from task_attr_settings tas, val_attr_name van
                 where tas.TASK_INST_ID = tsk.TASK_INST_ID
                   and tas.VAL_ATTR_INST_ID = van.VAL_ATTR_INST_ID
                   and van.ATTR_NAME = 'Reason Description'
                   and van.GROUP_NAME = 'Rejected Task Details') REASON_DESCR
          from wo_task_inst tsk
         where 1=1
        {[0]  and tsk.WO_INST_ID = ?}
        order by tsk.TASK_NUMBER
      ]]></script>
    </method>
  </methodList>
