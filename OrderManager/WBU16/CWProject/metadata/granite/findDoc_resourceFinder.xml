<?xml version="1.0" encoding="UTF-8" ?>
<findDoc name="granite.resourceFinder">
  <extends>find_com.conceptwave.system.Finder</extends>
  <input>doc_granite.resourceSearchDoc</input>
  <label>Resource Finder</label>
  <maxRowsLimit>100</maxRowsLimit>
  <metaVersion>21</metaVersion>
  <output>doc_granite.RESOURCE_INST</output>
  <type>document</type>
  <volatility>10</volatility>
  <searchList>
    <search type="sitem">
      <document>doc_granite.resourceSearchDoc</document>
      <mappedDocument>doc_granite.RESOURCE_INST</mappedDocument>
      <mappedPath>doc_granite.RESOURCE_INST/leaf_NAME</mappedPath>
      <operation>equal</operation>
      <path>doc_granite.resourceSearchDoc/leaf_NAME</path>
    </search>
    <search type="sitem">
      <document>doc_granite.resourceSearchDoc</document>
      <mappedDocument>doc_granite.RESOURCE_INST</mappedDocument>
      <mappedPath>doc_granite.RESOURCE_INST/leaf_RESOURCE_INST_ID</mappedPath>
      <operation>equal</operation>
      <path>doc_granite.resourceSearchDoc/leaf_RESOURCE_INST_ID</path>
    </search>
  </searchList>
  <methodList>
    <method name="addPerm" type="permMethod">
      <privilegesList>
        <privileges type="ppriv">
          <privilege>stcwConfigMgmt</privilege>
        </privileges>
        <privileges type="ppriv">
          <privilege>UPAdmin</privilege>
        </privileges>
      </privilegesList>
      <privilegesType>SOME</privilegesType>
      <script>return false;</script>
    </method>
    <method name="deletePerm" type="permMethod">
      <privilegesList>
        <privileges type="ppriv">
          <privilege>stcwConfigMgmt</privilege>
        </privileges>
        <privileges type="ppriv">
          <privilege>UPAdmin</privilege>
        </privileges>
      </privilegesList>
      <privilegesType>SOME</privilegesType>
      <script>return false;</script>
    </method>
    <method name="editablePerm" type="permMethod">
      <privilegesList>
        <privileges type="ppriv">
          <privilege>stcwConfigMgmt</privilege>
        </privileges>
        <privileges type="ppriv">
          <privilege>UPAdmin</privilege>
        </privileges>
      </privilegesList>
      <privilegesType>SOME</privilegesType>
      <script>return false;</script>
    </method>
  </methodList>
  <UserInterface name="UserInterface" type="ui">
    <extends>ui_com.conceptwave.system.FinderUserInterface</extends>
    <variableList>
      <variable name="result" type="uivar">
        <flags enum-type="metadataFlag">ARRAY</flags>
        <valueType>doc_granite.RESOURCE_INST</valueType>
      </variable>
      <variable name="search" type="uivar">
        <valueType>ui_com.conceptwave.system.UserInterface</valueType>
      </variable>
      <variable name="detail" type="uivar">
        <flags enum-type="metadataFlag">VALIDATE</flags>
        <valueType>doc_granite.RESOURCE_INST/ui_UserInterface</valueType>
      </variable>
    </variableList>
    <vformList>
      <vform name="Forms" type="frmui">
        <extends>ui_com.conceptwave.system.FinderUserInterface/frmui_Forms</extends>
        <overlayList>
          <overlay name="Result" type="exov">
            <base>ui_com.conceptwave.system.FinderUserInterface/frmui_Forms/vform_Result</base>
            <extends>ui_com.conceptwave.system.FinderUserInterface/frmui_Forms/vform_Result</extends>
            <label>Result</label>
            <overrideList>
              <override name="table" type="elvlt">
                <height>100%</height>
                <width>100%</width>
                <elementList>
                  <element name="resourceFinderResultTable" type="eltabl">
                    <onSelectChange type="varPath">
                      <variablePathList>
                        <variablePath>ui_com.conceptwave.system.MasterDetailUserInterface/action_OnSelChanged</variablePath>
                      </variablePathList>
                    </onSelectChange>
                    <variable type="varPath">
                      <variablePathList>
                        <variablePath>findDoc_granite.resourceFinder/ui_UserInterface/uivar_result</variablePath>
                      </variablePathList>
                    </variable>
                    <elementList>
                      <element name="RESOURCE_INST_ID" type="elfld">
                        <variable type="varPath">
                          <variablePathList>
                            <variablePath>findDoc_granite.resourceFinder/ui_UserInterface/uivar_result</variablePath>
                            <variablePath>doc_granite.RESOURCE_INST/leaf_RESOURCE_INST_ID</variablePath>
                          </variablePathList>
                        </variable>
                      </element>
                      <element name="PARENT_INST_ID" type="elfld">
                        <variable type="varPath">
                          <variablePathList>
                            <variablePath>findDoc_granite.resourceFinder/ui_UserInterface/uivar_result</variablePath>
                            <variablePath>doc_granite.RESOURCE_INST/leaf_PARENT_INST_ID</variablePath>
                          </variablePathList>
                        </variable>
                      </element>
                      <element name="TEMPLATE_INST_ID" type="elfld">
                        <variable type="varPath">
                          <variablePathList>
                            <variablePath>findDoc_granite.resourceFinder/ui_UserInterface/uivar_result</variablePath>
                            <variablePath>doc_granite.RESOURCE_INST/leaf_TEMPLATE_INST_ID</variablePath>
                          </variablePathList>
                        </variable>
                      </element>
                      <element name="DEFINITION_INST_ID" type="elfld">
                        <variable type="varPath">
                          <variablePathList>
                            <variablePath>findDoc_granite.resourceFinder/ui_UserInterface/uivar_result</variablePath>
                            <variablePath>doc_granite.RESOURCE_INST/leaf_DEFINITION_INST_ID</variablePath>
                          </variablePathList>
                        </variable>
                      </element>
                      <element name="NAME" type="elfld">
                        <variable type="varPath">
                          <variablePathList>
                            <variablePath>findDoc_granite.resourceFinder/ui_UserInterface/uivar_result</variablePath>
                            <variablePath>doc_granite.RESOURCE_INST/leaf_NAME</variablePath>
                          </variablePathList>
                        </variable>
                      </element>
                      <element name="CATEGORY" type="elfld">
                        <variable type="varPath">
                          <variablePathList>
                            <variablePath>findDoc_granite.resourceFinder/ui_UserInterface/uivar_result</variablePath>
                            <variablePath>doc_granite.RESOURCE_INST/leaf_CATEGORY</variablePath>
                          </variablePathList>
                        </variable>
                      </element>
                      <element name="REVISION" type="elfld">
                        <variable type="varPath">
                          <variablePathList>
                            <variablePath>findDoc_granite.resourceFinder/ui_UserInterface/uivar_result</variablePath>
                            <variablePath>doc_granite.RESOURCE_INST/leaf_REVISION</variablePath>
                          </variablePathList>
                        </variable>
                      </element>
                      <element name="FLAGS" type="elfld">
                        <variable type="varPath">
                          <variablePathList>
                            <variablePath>findDoc_granite.resourceFinder/ui_UserInterface/uivar_result</variablePath>
                            <variablePath>doc_granite.RESOURCE_INST/leaf_FLAGS</variablePath>
                          </variablePathList>
                        </variable>
                      </element>
                      <element name="START_DATE" type="elfld">
                        <variable type="varPath">
                          <variablePathList>
                            <variablePath>findDoc_granite.resourceFinder/ui_UserInterface/uivar_result</variablePath>
                            <variablePath>doc_granite.RESOURCE_INST/leaf_START_DATE</variablePath>
                          </variablePathList>
                        </variable>
                      </element>
                      <element name="END_DATE" type="elfld">
                        <variable type="varPath">
                          <variablePathList>
                            <variablePath>findDoc_granite.resourceFinder/ui_UserInterface/uivar_result</variablePath>
                            <variablePath>doc_granite.RESOURCE_INST/leaf_END_DATE</variablePath>
                          </variablePathList>
                        </variable>
                      </element>
                      <element name="STATUS" type="elfld">
                        <variable type="varPath">
                          <variablePathList>
                            <variablePath>findDoc_granite.resourceFinder/ui_UserInterface/uivar_result</variablePath>
                            <variablePath>doc_granite.RESOURCE_INST/leaf_STATUS</variablePath>
                          </variablePathList>
                        </variable>
                      </element>
                      <element name="ORDERED" type="elfld">
                        <variable type="varPath">
                          <variablePathList>
                            <variablePath>findDoc_granite.resourceFinder/ui_UserInterface/uivar_result</variablePath>
                            <variablePath>doc_granite.RESOURCE_INST/leaf_ORDERED</variablePath>
                          </variablePathList>
                        </variable>
                      </element>
                      <element name="SCHED_DATE" type="elfld">
                        <variable type="varPath">
                          <variablePathList>
                            <variablePath>findDoc_granite.resourceFinder/ui_UserInterface/uivar_result</variablePath>
                            <variablePath>doc_granite.RESOURCE_INST/leaf_SCHED_DATE</variablePath>
                          </variablePathList>
                        </variable>
                      </element>
                      <element name="DUE" type="elfld">
                        <variable type="varPath">
                          <variablePathList>
                            <variablePath>findDoc_granite.resourceFinder/ui_UserInterface/uivar_result</variablePath>
                            <variablePath>doc_granite.RESOURCE_INST/leaf_DUE</variablePath>
                          </variablePathList>
                        </variable>
                      </element>
                      <element name="INSTALLED" type="elfld">
                        <variable type="varPath">
                          <variablePathList>
                            <variablePath>findDoc_granite.resourceFinder/ui_UserInterface/uivar_result</variablePath>
                            <variablePath>doc_granite.RESOURCE_INST/leaf_INSTALLED</variablePath>
                          </variablePathList>
                        </variable>
                      </element>
                      <element name="IN_SERVICE" type="elfld">
                        <variable type="varPath">
                          <variablePathList>
                            <variablePath>findDoc_granite.resourceFinder/ui_UserInterface/uivar_result</variablePath>
                            <variablePath>doc_granite.RESOURCE_INST/leaf_IN_SERVICE</variablePath>
                          </variablePathList>
                        </variable>
                      </element>
                      <element name="DECOMMISSION" type="elfld">
                        <variable type="varPath">
                          <variablePathList>
                            <variablePath>findDoc_granite.resourceFinder/ui_UserInterface/uivar_result</variablePath>
                            <variablePath>doc_granite.RESOURCE_INST/leaf_DECOMMISSION</variablePath>
                          </variablePathList>
                        </variable>
                      </element>
                    </elementList>
                  </element>
                </elementList>
              </override>
            </overrideList>
          </overlay>
          <overlay name="Default" type="exov">
            <base>ui_com.conceptwave.system.FinderUserInterface/frmui_Forms/exov_Default</base>
            <extends>ui_com.conceptwave.system.FinderUserInterface/frmui_Forms/exov_Default</extends>
            <label>Default</label>
            <overrideList>
              <override name="detailSection" type="elsect">
                <formReference type="varPath">
                  <variablePathList>
                    <variablePath>ui_com.conceptwave.system.MasterDetailUserInterface/uivar_detailForm</variablePath>
                  </variablePathList>
                </formReference>
                <variable type="varPath">
                  <variablePathList>
                    <variablePath>findDoc_granite.resourceFinder/ui_UserInterface/uivar_detail</variablePath>
                  </variablePathList>
                </variable>
                <visible type="varPath">
                  <variablePathList>
                    <variablePath>ui_com.conceptwave.system.MasterDetailUserInterface/permMethod_detailVisible</variablePath>
                  </variablePathList>
                </visible>
              </override>
              <override name="searchFormSection" type="elsect">
                <formReference type="varPath">
                  <variablePathList>
                    <variablePath>ui_com.conceptwave.system.BaseFinderUserInterface/uivar_searchForm</variablePath>
                  </variablePathList>
                </formReference>
                <variable type="varPath">
                  <variablePathList>
                    <variablePath>findDoc_granite.resourceFinder/ui_UserInterface/uivar_search</variablePath>
                  </variablePathList>
                </variable>
                <visible type="varPath">
                  <variablePathList>
                    <variablePath>ui_com.conceptwave.system.FinderUserInterface/permMethod_showSearchForm</variablePath>
                  </variablePathList>
                </visible>
              </override>
            </overrideList>
          </overlay>
        </overlayList>
      </vform>
    </vformList>
  </UserInterface>
  <viewList>
    <view name="resourceFinderView" type="findVView">
      <outputOverlay>findDoc_granite.resourceFinder/ui_UserInterface/frmui_Forms/exov_Result</outputOverlay>
    </view>
  </viewList>
</findDoc>