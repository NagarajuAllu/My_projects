                      </element>
                      <element name="averageMinutes" type="elfld">
                        <label>Minutes</label>
                        <variable type="varPath">
                          <variablePathList>
                            <variablePath>findSql_wDashboards.averageOrderCompletionTime/ui_UserInterface/uivar_result</variablePath>
                            <variablePath>doc_wDashboards.averageCompletion/leaf_averageMinutes</variablePath>
                          </variablePathList>
                        </variable>
                        <width>100px</width>
                      </element>
                      <element name="averageSeconds" type="elfld">
                        <label>Seconds</label>
                        <variable type="varPath">
                          <variablePathList>
                            <variablePath>findSql_wDashboards.averageOrderCompletionTime/ui_UserInterface/uivar_result</variablePath>
                            <variablePath>doc_wDashboards.averageCompletion/leaf_averageSeconds</variablePath>
                          </variablePathList>
                        </variable>
                        <width>100px</width>
                      </element>
                    </elementList>
                  </element>
                </elementList>
              </override>
            </overrideList>
