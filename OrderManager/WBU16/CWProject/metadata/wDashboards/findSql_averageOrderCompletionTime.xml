<?xml version="1.0" encoding="UTF-8" ?>
<findSql name="wDashboards.averageOrderCompletionTime">
  <dbSchema>ORDER</dbSchema>
  <extends>find_com.conceptwave.system.Finder</extends>
  <input>doc_wDashboards.inputForDashboards</input>
  <label>Average Order Completion Time</label>
  <maxRowsLimit>100</maxRowsLimit>
  <metaVersion>25</metaVersion>
  <output>doc_wDashboards.averageCompletion</output>
  <type>SQL</type>
  <volatility>3</volatility>
  <methodList>
    <method name="cwOnFinderSQLSel" type="action">
      <system>true</system>
      <script><![CDATA[
        SELECT NULL,
               timeInSeconds,
               extractDaysFromDataInSeconds(timeInSeconds) days,
               extractHoursFromDataInSeconds(timeInSeconds) hours,
               extractMinutesFromDataInSecs(timeInSeconds) minutes,
               extractSecsFromDataInSecs(timeInSeconds) seconds
          FROM (SELECT TRUNC(AVG(h.completiondate - h.cwordercreationdate)*86400) timeInSeconds
                  FROM stcw_bundleorder_header h
                 WHERE h.completiondate IS NOT NULL
                   AND h.orderType <> 'F'
                   AND h.ismigrated = 0
              {[0] AND h.cwordercreationdate >= ?}
              {[1] AND h.cwordercreationdate <= ?}
                )
      ]]></script>
    </method>
    <method name="cwOnInitSearch" type="action">
      <system>true</system>
      <script>wDashboards.setInputParametersInDashboard(this);</script>
    </method>
  </methodList>
  <UserInterface name="UserInterface" type="ui">
    <extends>ui_com.conceptwave.system.FinderUserInterface</extends>
    <variableList>
      <variable name="finder" type="uivar">
        <valueType>ui_com.conceptwave.system.FinderUserInterface</valueType>
      </variable>
      <variable name="result" type="uivar">
        <flags enum-type="metadataFlag">ARRAY</flags>
        <valueType>doc_wDashboards.averageCompletion</valueType>
      </variable>
      <variable name="search" type="uivar">
        <valueType>doc_wDashboards.inputForDashboards/ui_UserInterface</valueType>
      </variable>
      <variable name="detail" type="uivar">
        <flags enum-type="metadataFlag">VALIDATE</flags>
        <valueType>doc_wDashboards.averageCompletion/ui_UserInterface</valueType>
      </variable>
    </variableList>
    <vformList>
      <vform name="Forms" type="frmui">
        <extends>ui_com.conceptwave.system.FinderUserInterface/frmui_Forms</extends>
        <overlayList>
          <overlay name="Result" type="exov">
            <base>ui_com.conceptwave.system.FinderUserInterface/frmui_Forms/vform_Result</base>
            <extends>ui_com.conceptwave.system.FinderUserInterface/frmui_Forms/vform_Result</extends>
            <label>Result</label>
            <overrideList>
              <override name="table" type="elvlt">
                <height>100%</height>
                <width>100%</width>
                <elementList>
                  <element name="averageOrderCompletionTimeResultTable" type="eltabl">
                    <onSelectChange type="varPath">
                      <variablePathList>
                        <variablePath>ui_com.conceptwave.system.MasterDetailUserInterface/action_OnSelChanged</variablePath>
                      </variablePathList>
                    </onSelectChange>
                    <variable type="varPath">
                      <variablePathList>
                        <variablePath>findSql_wDashboards.averageOrderCompletionTime/ui_UserInterface/uivar_result</variablePath>
                      </variablePathList>
                    </variable>
                    <elementList>
                      <element name="averageTimeInSecs" type="elfld">
                        <label>Total Time [secs]</label>
                        <variable type="varPath">
                          <variablePathList>
                            <variablePath>findSql_wDashboards.averageOrderCompletionTime/ui_UserInterface/uivar_result</variablePath>
                            <variablePath>doc_wDashboards.averageCompletion/leaf_averageTimeInSecs</variablePath>
                          </variablePathList>
                        </variable>
                        <width>150px</width>
                      </element>
                      <element name="averageDays" type="elfld">
                        <label>Days</label>
                        <variable type="varPath">
                          <variablePathList>
                            <variablePath>findSql_wDashboards.averageOrderCompletionTime/ui_UserInterface/uivar_result</variablePath>
                            <variablePath>doc_wDashboards.averageCompletion/leaf_averageDays</variablePath>
                          </variablePathList>
