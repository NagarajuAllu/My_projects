<findSql name="wDashboards.countOrderGroupByServiceType">
  <dbSchema>ORDER</dbSchema>
  <extends>find_com.conceptwave.system.Finder</extends>
  <input>doc_wDashboards.inputForDashboards</input>
  <label>Count Order Group By ServiceType</label>
  <maxRowsLimit>100</maxRowsLimit>
  <metaVersion>25</metaVersion>
  <output>doc_wDashboards.countForServiceType</output>
  <type>SQL</type>
  <volatility>3</volatility>
  <methodList>
    <method name="cwOnFinderSQLSel" type="action">
      <system>true</system>
      <script><![CDATA[
        SELECT servicetype,
               count(distinct l.cworderid)
          FROM stcw_bundleorder_header h, stcw_lineitem l
         WHERE h.ismigrated = 0
           AND h.orderType <> 'F'
           AND ordernumber not like '%_REVI_%'
           AND ordernumber not like '%_CANC_%'
           AND l.cworderid = h.cworderid
          {[0] AND h.cwordercreationdate >= ?}
          {[1] AND h.cwordercreationdate <= ?}
        GROUP BY serviceType
        ORDER BY serviceType
      ]]></script>
    </method>
    <method name="cwOnInitSearch" type="action">
