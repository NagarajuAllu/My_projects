<?xml version="1.0" encoding="UTF-8" ?>
<findSql name="wDashboards.processSummary">
  <dbSchema>ORDER</dbSchema>
  <extends>find_com.conceptwave.system.Finder</extends>
  <input>doc_wDashboards.inputForDashboards</input>
  <label>Process Summary</label>
  <maxRowsLimit>500</maxRowsLimit>
  <metaVersion>25</metaVersion>
  <output>doc_wDashboards.processSummaryReport</output>
  <type>SQL</type>
  <volatility>3</volatility>
  <methodList>
    <method name="cwOnFinderSQLSel" type="action">
      <system>true</system>
      <script><![CDATA[
        select PROCESS_METADATYPE,
               count(case when STATUS = 1 then 1 else null end) as "Running",
               count(case when STATUS = 2 then 1 else null end) as "Suspended",
               count(case when STATUS = 5 then 1 else null end) as "In Error",
               count(case when STATUS = 3 then 1 else null end) as "Complete",
               count(case when STATUS not in (3, 6) then 1 else null end) as "Total Active"
          from CWPROCESS
         where 1 = 1
        {[0] AND creation_date >= ?}
        {[1] AND creation_date <= ?}
         group by PROCESS_METADATYPE
         order by PROCESS_METADATYPE
      ]]></script>
    </method>
    <method name="cwOnInitSearch" type="action">
      <system>true</system>
      <script>wDashboards.setInputParametersInDashboard(this);</script>
    </method>
  </methodList>
  <UserInterface name="UserInterface" type="ui">
    <extends>ui_com.conceptwave.system.FinderUserInterface</extends>
    <variableList>
      <variable name="finder" type="uivar">
        <valueType>ui_com.conceptwave.system.FinderUserInterface</valueType>
      </variable>
      <variable name="result" type="uivar">
        <flags enum-type="metadataFlag">ARRAY</flags>
        <valueType>doc_wDashboards.processSummaryReport</valueType>
      </variable>
      <variable name="detail" type="uivar">
        <flags enum-type="metadataFlag">VALIDATE</flags>
        <valueType>doc_wDashboards.processSummaryReport/ui_UserInterface</valueType>
      </variable>
      <variable name="search" type="uivar">
        <valueType>doc_wDashboards.inputForDashboards/ui_UserInterface</valueType>
      </variable>
    </variableList>
    <vformList>
      <vform name="Forms" type="frmui">
        <extends>ui_com.conceptwave.system.FinderUserInterface/frmui_Forms</extends>
        <overlayList>
          <overlay name="Result" type="exov">
            <base>ui_com.conceptwave.system.FinderUserInterface/frmui_Forms/vform_Result</base>
            <extends>ui_com.conceptwave.system.FinderUserInterface/frmui_Forms/vform_Result</extends>
            <label>Result</label>
            <overrideList>
              <override name="table" type="elvlt">
                <height>100%</height>
                <width>100%</width>
                <elementList>
                  <element name="processSummaryResultTable1" type="eltabl">
                    <onSelectChange type="varPath">
                      <variablePathList>
                        <variablePath>ui_com.conceptwave.system.MasterDetailUserInterface/action_OnSelChanged</variablePath>
                      </variablePathList>
                    </onSelectChange>
                    <variable type="varPath">
                      <variablePathList>
                        <variablePath>findSql_wDashboards.processSummary/ui_UserInterface/uivar_result</variablePath>
                      </variablePathList>
                    </variable>
                    <elementList>
                      <element name="process" type="elfld">
                        <label>Process Name</label>
                        <variable type="varPath">
                          <variablePathList>
                            <variablePath>findSql_wDashboards.processSummary/ui_UserInterface/uivar_result</variablePath>
                            <variablePath>doc_wDashboards.processSummaryReport/leaf_process</variablePath>
                          </variablePathList>
                        </variable>
                        <width>300px</width>
                      </element>
                      <element name="executing" type="elfld">
                        <variable type="varPath">
                          <variablePathList>
                            <variablePath>findSql_wDashboards.processSummary/ui_UserInterface/uivar_result</variablePath>
                            <variablePath>doc_wDashboards.processSummaryReport/leaf_executing</variablePath>
                          </variablePathList>
                        </variable>
                        <width>120px</width>
                      </element>
                      <element name="suspended" type="elfld">
                        <variable type="varPath">
                          <variablePathList>
                            <variablePath>findSql_wDashboards.processSummary/ui_UserInterface/uivar_result</variablePath>
                            <variablePath>doc_wDashboards.processSummaryReport/leaf_suspended</variablePath>
                          </variablePathList>
                        </variable>
                        <width>120px</width>
                      </element>
                      <element name="error" type="elfld">
                        <variable type="varPath">
                          <variablePathList>
                            <variablePath>findSql_wDashboards.processSummary/ui_UserInterface/uivar_result</variablePath>
                            <variablePath>doc_wDashboards.processSummaryReport/leaf_error</variablePath>
                          </variablePathList>
                        </variable>
                        <width>120px</width>
                      </element>
                      <element name="complete" type="elfld">
                        <variable type="varPath">
                          <variablePathList>
                            <variablePath>findSql_wDashboards.processSummary/ui_UserInterface/uivar_result</variablePath>
                            <variablePath>doc_wDashboards.processSummaryReport/leaf_complete</variablePath>
                          </variablePathList>
                        </variable>
                        <width>120px</width>
                      </element>
                      <element name="totalNonCompleted" type="elfld">
                        <variable type="varPath">
                          <variablePathList>
                            <variablePath>findSql_wDashboards.processSummary/ui_UserInterface/uivar_result</variablePath>
                            <variablePath>doc_wDashboards.processSummaryReport/leaf_totalNonCompleted</variablePath>
                          </variablePathList>
                        </variable>
                        <width>120px</width>
                      </element>
                    </elementList>
                  </element>
                </elementList>
              </override>
            </overrideList>
          </overlay>
          <overlay name="Default" type="exov">
            <base>ui_com.conceptwave.system.FinderUserInterface/frmui_Forms/exov_Default</base>
            <extends>ui_com.conceptwave.system.FinderUserInterface/frmui_Forms/exov_Default</extends>
            <label>Default</label>
            <overrideList>
              <override name="detailSection" type="elsect">
                <formReference type="varPath">
                  <variablePathList>
                    <variablePath>ui_com.conceptwave.system.MasterDetailUserInterface/uivar_detailForm</variablePath>
                  </variablePathList>
                </formReference>
                <variable type="varPath">
                  <variablePathList>
                    <variablePath>findSql_wDashboards.processSummary/ui_UserInterface/uivar_detail</variablePath>
                  </variablePathList>
                </variable>
                <visible type="varPath">
                  <variablePathList>
                    <variablePath>ui_com.conceptwave.system.MasterDetailUserInterface/permMethod_detailVisible</variablePath>
                  </variablePathList>
                </visible>
              </override>
              <override name="searchFormSection" type="elsect">
                <formReference type="varPath">
                  <variablePathList>
                    <variablePath>ui_com.conceptwave.system.BaseFinderUserInterface/uivar_searchForm</variablePath>
                  </variablePathList>
                </formReference>
                <variable type="varPath">
                  <variablePathList>
                    <variablePath>findSql_wDashboards.processSummary/ui_UserInterface/uivar_search</variablePath>
                  </variablePathList>
                </variable>
                <visible type="varPath">
                  <variablePathList>
                    <variablePath>ui_com.conceptwave.system.FinderUserInterface/permMethod_showSearchForm</variablePath>
                  </variablePathList>
                </visible>
              </override>
            </overrideList>
          </overlay>
          <overlay name="Chart" type="exov">
            <base>ui_com.conceptwave.system.FinderUserInterface/frmui_Forms/exov_Chart</base>
            <extends>ui_com.conceptwave.system.FinderUserInterface/frmui_Forms/exov_Chart</extends>
            <label>Chart</label>
            <overrideList>
              <override name="Chart" type="elchart">
                <caption>Process Summary</caption>
                <height>400px</height>
                <is3D>true</is3D>
                <type>Column</type>
                <variable type="varPath">
                  <variablePathList>
                    <variablePath>findSql_wDashboards.processSummary/ui_UserInterface/uivar_result</variablePath>
                  </variablePathList>
                </variable>
                <width>1500px</width>
                <elementList>
                  <element name="processName" type="elchartf">
                    <label>Process Name</label>
                    <variable type="varPath">
                      <variablePathList>
                        <variablePath>findSql_wDashboards.processSummary/ui_UserInterface/uivar_result</variablePath>
                        <variablePath>doc_wDashboards.processSummaryReport/leaf_processName</variablePath>
                      </variablePathList>
                    </variable>
                  </element>
                  <element name="running" type="elchartf">
                    <label>Running</label>
                    <variable type="varPath">
                      <variablePathList>
                        <variablePath>findSql_wDashboards.processSummary/ui_UserInterface/uivar_result</variablePath>
                        <variablePath>doc_rpts.processSummary/leaf_executing</variablePath>
                      </variablePathList>
                    </variable>
                  </element>
                  <element name="suspended" type="elchartf">
                    <label>Suspended</label>
                    <variable type="varPath">
                      <variablePathList>
                        <variablePath>findSql_wDashboards.processSummary/ui_UserInterface/uivar_result</variablePath>
                        <variablePath>doc_rpts.processSummary/leaf_suspended</variablePath>
                      </variablePathList>
                    </variable>
                  </element>
                  <element name="error" type="elchartf">
                    <label>Error</label>
                    <variable type="varPath">
                      <variablePathList>
                        <variablePath>findSql_wDashboards.processSummary/ui_UserInterface/uivar_result</variablePath>
                        <variablePath>doc_rpts.processSummary/leaf_error</variablePath>
                      </variablePathList>
                    </variable>
                  </element>
                  <element name="completed" type="elchartf">
                    <label>Completed</label>
                    <variable type="varPath">
                      <variablePathList>
                        <variablePath>findSql_wDashboards.processSummary/ui_UserInterface/uivar_result</variablePath>
                        <variablePath>doc_rpts.processSummary/leaf_complete</variablePath>
                      </variablePathList>
                    </variable>
                  </element>
                  <element name="totalActive" type="elchartf">
                    <label>Total Active</label>
                    <variable type="varPath">
                      <variablePathList>
                        <variablePath>findSql_wDashboards.processSummary/ui_UserInterface/uivar_result</variablePath>
                        <variablePath>doc_rpts.processSummary/leaf_totalNonCompleted</variablePath>
                      </variablePathList>
                    </variable>
                  </element>
                </elementList>
              </override>
            </overrideList>
          </overlay>
        </overlayList>
      </vform>
    </vformList>
  </UserInterface>
  <viewList>
    <view name="processSummaryView" type="findVView">
      <label>Process Summary</label>
      <outputOverlay>findSql_wDashboards.processSummary/ui_UserInterface/frmui_Forms/exov_Result</outputOverlay>
      <searchImmediately>true</searchImmediately>
    </view>
  </viewList>
</findSql>