  <methodList>
    <method name="cwOnFinderSQLSel" type="action">
      <system>true</system>
      <script><![CDATA[
        SELECT 'COMPLETED', COUNT(*)
          FROM stcw_bundleorder_header h
         WHERE h.ismigrated = 0
           AND h.orderType = 'F'
           AND ordernumber not like '%_REVI_%'
           AND ordernumber not like '%_CANC_%'
           AND orderStatus = 'COMPLETED'
          {[0] AND h.cwordercreationdate >= ?}
          {[1] AND h.cwordercreationdate <= ?}
        UNION
        SELECT 'CANCELLED', COUNT(*)
          FROM stcw_bundleorder_header h
         WHERE h.ismigrated = 0
           AND h.orderType = 'F'
           AND ordernumber not like '%_REVI_%'
           AND ordernumber not like '%_CANC_%'
           AND orderStatus = 'CANCELLED'
          {[0] AND h.cwordercreationdate >= ?}
          {[1] AND h.cwordercreationdate <= ?}
        UNION
        SELECT 'FAILED_SUBMIT', COUNT(*)
          FROM stcw_bundleorder_header h
         WHERE h.ismigrated = 0
           AND h.orderType = 'F'
           AND ordernumber not like '%_REVI_%'
           AND ordernumber not like '%_CANC_%'
           AND orderStatus = 'FAILED_SUBMIT'
          {[0] AND h.cwordercreationdate >= ?}
          {[1] AND h.cwordercreationdate <= ?}
        UNION
        SELECT 'OPEN', COUNT(*)
          FROM stcw_bundleorder_header h
         WHERE h.ismigrated = 0
           AND h.orderType = 'F'
           AND ordernumber not like '%_REVI_%'
           AND ordernumber not like '%_CANC_%'
           AND orderStatus NOT IN ('COMPLETED', 'CANCELLED', 'FAILED_SUBMIT')
          {[0] AND h.cwordercreationdate >= ?}
          {[1] AND h.cwordercreationdate <= ?}
      ]]></script>
    </method>
