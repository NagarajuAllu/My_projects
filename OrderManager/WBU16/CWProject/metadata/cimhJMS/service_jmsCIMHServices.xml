<?xml version="1.0" encoding="UTF-8" ?>
<externalService name="cimhJMS.jmsCIMHServices">
  <label>JMS CIMH Services</label>
  <metaVersion>25</metaVersion>
  <portList>
    <port name="jmsCIMHWOSUPort" type="port">
      <binding>bind_cimhJMS.XngServiceRBinding_WOSU</binding>
      <label>JMS CIMH WorkOrderStatusUpdate Port</label>
      <listener>true</listener>
      <recover>5</recover>
      <script>script_cimhJMS.manageGraniteMsg_WorkOrderStatusUpdate</script>
    </port>
    <port name="jmsCIMHSubmitOrderRPort" type="port">
      <binding>bind_cimhJMS.XngServiceSubmitOrderRBinding</binding>
      <label>JMS CIMH SubmitOrderResult Port</label>
      <listener>true</listener>
      <recover>5</recover>
      <script>script_cimhJMS.manageGraniteMsg_SubmitUpdateOrderResponse</script>
    </port>
    <port name="jmsCIMHUpdateOrderRPort" type="port">
      <binding>bind_cimhJMS.XngServiceUpdateOrderRBinding</binding>
      <label>JMS CIMH UpdateOrderResult Port</label>
      <listener>true</listener>
      <recover>5</recover>
      <script>script_cimhJMS.manageGraniteMsg_SubmitUpdateOrderResponse</script>
    </port>
    <port name="jmsCIMHXngServiceWRPort" type="port">
      <binding>bind_cimhJMS.XngServiceWRBinding</binding>
      <label>JMS CIMH GraniteServices Port</label>
      <recover>5</recover>
    </port>
  </portList>
</externalService>