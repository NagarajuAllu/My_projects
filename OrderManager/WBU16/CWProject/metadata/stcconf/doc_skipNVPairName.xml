  <methodList>
    <method name="cwOnDocStore" type="action">
      <system>true</system>
      <script><![CDATA[
        this.cw$super_cwOnDocStore();

        if(stcw.isNameInSkipNVPairList(this.name, this.serviceType)) {
            Global.throwException("AE0118", [this.name, "Skip"]);
        }

        if(stcw.isNameInStoreAndForwardNVPairList(this.name, this.serviceType)) {
            Global.throwException("AE0118", [this.name, "Store & Forward"]);
        }

        if(stcw.isNameInNewAttrNVPairList(this.name, this.serviceType)) {
            Global.throwException("AE0118", [this.name, "New Attribute"]);
        }


        if(this.isStored) {
            Global.showUserMessage(Global.translateText("AI0002"));
        }
        else {
            Global.showUserMessage(Global.translateText("AI0001"));
        }
      ]]></script>
    </method>
    <method name="addPerm" type="permMethod">
      <privilegesList>
        <privileges type="ppriv">
          <privilege>stcwConfigMgmt</privilege>
        </privileges>
        <privileges type="ppriv">
          <privilege>UPAdmin</privilege>
        </privileges>
      </privilegesList>
      <privilegesType>SOME</privilegesType>
      <script>return $psCondition;</script>
    </method>
    <method name="deletePerm" type="permMethod">
      <privilegesList>
        <privileges type="ppriv">
          <privilege>stcwConfigMgmt</privilege>
        </privileges>
        <privileges type="ppriv">
