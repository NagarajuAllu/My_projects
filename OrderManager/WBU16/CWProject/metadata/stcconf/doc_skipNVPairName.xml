      <propertyElement type="elfld">
        <label>Name in NV Pair</label>
        <optional type="varPath">
          <variablePathList>
            <variablePath>dtype_com.conceptwave.system.Boolean/leaf_FALSE</variablePath>
          </variablePathList>
        </optional>
      </propertyElement>
      <table>STCW_SKIP_NVPAIR</table>
