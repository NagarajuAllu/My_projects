            <script><![CDATA[
              var debugHeader = "[" + this.process.id + "] - UpdateOrderStatus For Voice - ";

              var processDocument = this.process.processDocument;
              processDocument.exceptionType = null;

              try {
                    var processRequest_el = new DataStructure("siosu:processRequest_el");

              debugPrintln(stcc.getSysdateForLog() + debugHeader + "processDocument = '" + processDocument.toXML() + "'");

                          updateEAIRequestWithGraniteEvent_ (processRequest_el.Request, processDocument);

                          // check payload size
                          stcc.checkPayloadToCRMSize(processRequest_el, this.process.processDocument.interOperationName);

                          // sending data to EAI
                          var response = stcw.sendOrderUpdateToEAI(processRequest_el, this.process.id);

                          if (response == null)
                                processDocument.exceptionType = "Interface";
                    }
                    catch(exc) {
              debugPrintln(stcc.getSysdateForLog() + debugHeader + "Catched Exception " + exc.message);
                          processDocument.exceptionType = "Application";
                    }



                    //
                    //  updateEAIRequestWithGraniteEvent_
                    //
                    function updateEAIRequestWithGraniteEvent_ (request, processDocument) {

                          request.AssetNumber        = processDocument.assetNumber;
                          request.AssetStatus        = processDocument.assetStatus;
                          request.BusinessUnit       = processDocument.businessUnit;
                          request.Comments           = processDocument.comments;
                          request.NINumber           = processDocument.NINumber;
                          request.OrderNumber        = processDocument.OrderNumber;
                          request.SystemDesignation  = processDocument.systemDesignation;
                          request.workOrderStatus    = processDocument.workOrderStatus;

                    }            // u p d a t e E A I R e q  u e s t W i t h G r a n i t e E v e n t _
            ]]></script>
          </method>
