  <methodList>
    <method name="cwOnInitSearch" type="action">
      <system>true</system>
      <script><![CDATA[
        if(searchData.orderNumber == null)  {
              searchData.orderNumber = "%";
        }
        else {
              if(! searchData.orderNumber.startsWith("%") )  {
                    searchData.orderNumber = ("%" + searchData.orderNumber);
              }
              if(! searchData.orderNumber.endsWith("%") )  {
                    searchData.orderNumber = (searchData.orderNumber + "%");
              }
        }
      ]]></script>
    </method>
