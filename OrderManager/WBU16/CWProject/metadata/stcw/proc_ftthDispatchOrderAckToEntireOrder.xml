                        <system>true</system>
                        <script>this.process.processDocument.counter++;</script>
                      </method>
                    </methodList>
                  </child>
                  <child name="repeatWithNextService" type="repeatActivity">
                    <element>proc_stcw.ftthDispatchOrderAckToEntireOrder/seqActivity_start/switchActivity_processedAllServices</element>
