                        <childList>
                          <child name="sendOrderAckForService" type="seqActivity">
                            <label>Send OrderAck For Service</label>
                            <x>608.0</x>
                            <y>278.0</y>
                            <childList>
                              <child name="sendOrderAckForFTTHToCOM" type="spawnActivity">
                                <element>proc_stcw.sendOrderAckForFTTHToCOM</element>
                                <label>Send OrderAck For FTTH To COM</label>
                                <x>743.0</x>
                                <y>257.0</y>
                                <methodList>
                                  <method name="cwOnProcActBefore" type="action">
                                    <category>before</category>
                                    <system>true</system>
                                    <script><![CDATA[
                                      var childProcessDocument = this.activityData;
                                      childProcessDocument.orderAckProcessId = this.process.processDocument.orderAckProcessId;
                                      childProcessDocument.lineItemId = this.process.processOrder.parentLineItems[0].services[this.process.processDocument.counter].serviceLineItem.id;
                                    ]]></script>
                                  </method>
                                </methodList>
                              </child>
                              <child name="waitForCompletion" type="joinActivity">
                                <element>proc_stcw.sendOrderAckForFTTHToCOM</element>
                                <label>Wait For Completion</label>
                                <x>738.0</x>
                                <y>32.0</y>
                              </child>
                            </childList>
                          </child>
                        </childList>
                        <methodList>
                          <method name="cwOnProcActCond" type="action">
                            <category>cond</category>
                            <system>true</system>
                            <script><![CDATA[
                              var order = this.process.processOrder;
                              var serviceUnderManagement = order.parentLineItems[0].services[this.process.processDocument.counter].serviceLineItem;

                              var requireOrderAck = (stcw.isServiceProvisionable(serviceUnderManagement.id) || (serviceUnderManagement.action != "N"));

                              if(requireOrderAck) {
                                  if(serviceUnderManagement.receivedAction == "N") {
                                      // EOC changed the action to something else but COM is not expecting orderAck for this element
                                      requireOrderAck = false;
                                  }
                              }

                              return requireOrderAck;
                            ]]></script>
                          </method>
                        </methodList>
