                        <methodList>
                          <method name="cwOnProcActBefore" type="action">
                            <category>before</category>
                            <system>true</system>
                            <script><![CDATA[
                              var orderItem = this.process.processOrderItem;
                              var workOrderType = null;

                              if(orderItem.workOrderNumber == null) {
                                  var quoteOrderType = "I";
                                  if(this.process.processOrder.orderHeader.feasibilityFor.toUpperCase() == "CHANGE") {
                                      quoteOrderType = "C";
                                  }
                                  workOrderType = stcw.getWorkOrderType(quoteOrderType, orderItem.action);
                                  if(workOrderType == null) {
                                      Global.throwException("Error in generating workOrderType");
                                  }
                                  orderItem.workOrderType = workOrderType;

                                  orderItem.reservationNumber = stcw.createReservationNumber(this.process.processOrder.orderHeader.orderNumber, orderItem.orderRowItemId);

                                  var workOrderNumber = stcw.createChildQuoteNumber(this.process.processOrder.orderHeader.orderNumber, orderItem.orderRowItemId, this.process.processOrder.parentLineItems[0].parentLineItem.versionNumber);
                                  if(workOrderNumber == null) {
                                      Global.throwException("Error in generating childQuoteNumber");
                                  }
                                  orderItem.workOrderNumber = workOrderNumber;
                                  orderItem.save();
                              }
                            ]]></script>
                          </method>
                        </methodList>
