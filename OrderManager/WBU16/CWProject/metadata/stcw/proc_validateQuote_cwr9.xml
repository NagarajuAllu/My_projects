                        <script><![CDATA[
                          var debugHeader = "[" + this.process.id + "] - ValidateQuote - ";
                          debugPrintln(stcc.getSysdateForLog() + debugHeader + "Start Quote Archiving...");

                          var quoteOrder = this.process.processOrder;

                          stcw.createRejectedQuote(quoteOrder.header.orderNumber,
                                                               quoteOrder.header.orderRowItemId,
                                                               quoteOrder.header.quoteNumber,
                                                               "Granite",
                                                               this.process.processDocument.errorText);

                          if(quoteOrder.versions.length > 1) {
                                debugPrintln(stcc.getSysdateForLog() + debugHeader + "UpdateQuote ... so Removing last version");
                                stcw.deleteLastVersionFolder(quoteOrder);
                          }
                          else {
                                debugPrintln(stcc.getSysdateForLog() + debugHeader + "Install/Change Quote ... so archiving");
                                stcw.archiveQuote(quoteOrder, true);
                          }
                          quoteOrder.save();
                          debugPrintln(stcc.getSysdateForLog() + debugHeader + "Quote Archived...");
