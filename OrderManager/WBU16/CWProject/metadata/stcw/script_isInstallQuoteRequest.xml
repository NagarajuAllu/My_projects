<?xml version="1.0" encoding="UTF-8" ?>
<script name="stcw.isInstallQuoteRequest">
  <label>Is Install Quote Request</label>
  <metaVersion>3</metaVersion>
  <parameterList>
    <parameter name="input" type="rifp">
      <type>nmeta_com.conceptwave.system.Object</type>
    </parameter>
  </parameterList>
  <script><![CDATA[
    if (input == null)
          return (false);

    return (input.quoteType == "I");
  ]]></script>
</script>