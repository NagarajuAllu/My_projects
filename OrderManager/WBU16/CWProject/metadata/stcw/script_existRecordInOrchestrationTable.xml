<?xml version="1.0" encoding="UTF-8" ?>
<script name="stcw.existRecordInOrchestrationTable">
  <label>Exist Record In Orchestration Table</label>
  <metaVersion>25</metaVersion>
  <parameterList>
    <parameter name="docId" type="rifp">
      <type>dtype_cwf.docId</type>
    </parameter>
  </parameterList>
  <script><![CDATA[
    var record = stcw.getRecordInOrchestrationTableByLineItemId(docId);

    return (record != null);
  ]]></script>
</script>